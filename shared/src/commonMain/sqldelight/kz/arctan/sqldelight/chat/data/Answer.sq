INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кто написал картину «Тайная вечеря»?", "«Тайная вечеря» написана Леонардо да Винчи.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кто автор «Войны и мира»?", "«Войну и мир» написал Лев Толстой.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Каков процесс тектоники плит?", "Тектоника плит – это научная теория, объясняющая, как литосфера Земли делится на несколько крупных плит, которые движутся и взаимодействуют друг с другом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какая химическая формула серной кислоты?", "Химическая формула серной кислоты - H2SO4.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Каков процесс клеточного дыхания?", "Клеточное дыхание - это процесс, посредством которого клетки превращают глюкозу и кислород в углекислый газ, воду и АТФ (энергию).");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кто автор «Властелина колец»?", "«Властелин колец» написал Дж. Р. Р. Толкин.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кто является нынешним президентом России?", "На момент моего последнего обновления знаний в сентябре 2021 года президентом России является Владимир Путин.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой самый большой вулкан в мире?", "Самый большой вулкан в мире – Мауна-Лоа, расположенный на Гавайях.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас домашние животные?", "Да, у меня есть игривый лабрадор-ретривер по кличке Макс.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какую еду вы предпочитаете?", "Я люблю побаловать себя теплой тарелкой макарон с сыром.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый стиль музыки?", "Мне нравится слушать самые разные жанры, но мой любимый – альтернативный рок.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Ваше любимое хобби?", "Я люблю читать книги и играть на гитаре.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Смотрели ли вы какие-нибудь хорошие фильмы в последнее время?", "Да, я недавно посмотрел «Начало» и нашел его умопомрачительным и захватывающим.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ расслабиться после долгого дня?", "Я люблю расслабляться, принимая горячую ванну и читая хорошую книгу.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы недавно путешествовали куда-нибудь интересное?", "Да, я был в Японии и был очарован ее богатой культурой и красивыми пейзажами.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какое ваше любимое занятие на свежем воздухе?", "Мне нравится ходить в походы и исследовать природные тропы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы жаворонок или сова?", "Утром я более продуктивна и энергична, поэтому считаю себя жаворонком.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какое ваше любимое время года?", "Я люблю яркие цвета и прохладную осеннюю погоду.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У тебя есть братья и сестры?", "Да, у меня есть старшая сестра.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится заниматься спортом?", "Мне нравится заниматься йогой, так как она помогает мне оставаться в хорошей физической и умственной форме.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какая ваша самая любимая книга всех времен?", "Трудно выбрать какую-то одну, но «Алхимик» Пауло Коэльо оказал на меня глубокое влияние.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какая ваша любимая кухня?", "Я люблю итальянскую кухню, особенно пиццу и пасту.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата или девиз?", "«Единственный способ сделать большую работу — любить то, что ты делаешь» Стива Джобса — это цитата, которая меня вдохновляет.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вам нравится готовить?", "Да, я считаю приготовление пищи терапевтическим и творческим занятием.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какая ваша любимая настольная или карточная игра?", "Мне нравится играть в Эрудит с друзьями и семьей.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы интроверт или экстраверт?", "Я считаю себя интровертом, но мне также нравится общаться в небольших группах.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы предпочитаете кофе или чай?", "Я больше чайный человек. Я наслаждаюсь успокаивающими ароматами и разнообразием вариантов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой лучший совет вы когда-либо получали?", "«Следуй своей страсти и никогда не сдавайся» — это совет, который всегда оставался со мной.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид десерта?", "Я питаю слабость к шоколадному торту лава.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимое телешоу?", "В настоящее время я подсел на сериал 'Очень странные дела'.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас скрытые таланты?", "Умею играть на фортепиано и жонглировать одновременно.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какое ваше любимое приложение на вашем телефоне?", "Я считаю приложение для медитации очень полезным для расслабления и осознанности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится проводить ленивое воскресенье?", "Свернувшись калачиком на диване с хорошей книгой или просмотром фильмов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы планируете или предпочитаете плыть по течению?", "Я склонен больше планировать, но я также ценю спонтанные приключения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вам нравится садоводство?", "Да, я считаю садоводство терапевтическим и полезным хобби.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы кошатник или собачник?", "Я больше кошатник. Я ценю их независимый характер.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид упражнений?", "Мне нравится бегать и исследовать различные беговые маршруты.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какая ваша любимая цитата из фильма?", "«Жизнь похожа на коробку конфет: никогда не знаешь, что получишь» из Форреста Гампа.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый праздник?", "Рождество мой любимый праздник. Люблю праздничную атмосферу и проводить время с близкими.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимый подкаст?", "Да, мне нравится слушать 'The Joe Rogan Experience' из-за разнообразия тем и интересных гостей.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Ваша любимая погода?", "Я наслаждаюсь ясностью и свежестью солнечного осеннего дня.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата из книги?", "«Это наш выбор, Гарри, который показывает, кто мы на самом деле, гораздо больше, чем наши способности» из «Гарри Поттер и Тайная комната» Дж. К. Роулинг.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться организованным?", "Я полагаюсь на комбинацию цифровых календарей и рукописных списков дел.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый сорт кофе?", "Мне нравится хороший капучино с щепоткой корицы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата известного человека?", "«В конце концов, важны не годы вашей жизни. Это жизнь в ваши годы» Авраама Линкольна.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вам нравится рисовать или рисовать?", "Да, я нахожу это терапевтическим и получаю удовольствие от выражения своего творчества через искусство.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая спортивная команда?", "Я болею за баскетбольную команду «Лос-Анджелес Лейкерс».");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ снятия стресса?", "Я считаю, что длительные прогулки на природе — отличный способ снять стресс и очистить разум.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ узнавать что-то новое?", "Я предпочитаю совмещать чтение книг и просмотр обучающих видео.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вам нравится ходить в походы?", "Да, я люблю исследовать природные тропы и бросать вызов себе в походах.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый завтрак?", "Я люблю классические яйца Бенедикт с хрустящим беконом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимый музыкальный инструмент?", "Фортепиано занимает особое место в моем сердце.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый тип тренировки?", "Высокоинтенсивный интервальный тренинг (HIIT) — мой любимый, поскольку он эффективен и поддерживает мою мотивацию.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый сорт сыра?", "Мне нравится сливочный и ароматный камамбер.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая вдохновляющая цитата?", "«Единственный способ сделать хорошую работу — любить то, что ты делаешь» Стива Джобса.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая вдохновляющая песня?", "'Imagine' Джона Леннона всегда вдохновляет меня своим посланием мира и единства.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ побаловать себя?", "Расслабляющий спа-день с массажем и уходом за лицом — это мой лучший способ побаловать себя.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимое выступление на TED Talk?", "Я настоятельно рекомендую посмотреть «Сила уязвимости» Брене Браун из-за его проницательного сообщения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый тип супа?", "Я люблю сытную тарелку томатного супа с ломтиком хрустящего хлеба.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вам нравится фотографировать?", "Да, запечатлевать моменты и исследовать разные точки зрения с помощью фотографии — это хобби, которое мне нравится.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какое ваше любимое мороженое?", "Шоколадная стружка с мятой — мой самый любимый вкус.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимый комедийный фильм?", "'Мальчишник' всегда вызывает у меня улыбку.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вы больше всего любите проводить время с друзьями?", "Неторопливый поздний завтрак или домашняя вечерняя игра — оба приятные способы провести время с друзьями.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата из исторической личности?", "«Мы можем жаловаться, потому что у розовых кустов есть шипы, или радоваться, потому что у шипов есть розы» Авраама Линкольна.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый тип сэндвича?", "Классический сэндвич BLT (бекон, салат и помидор) попадает в точку.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вам нравятся проекты «сделай сам»?", "Да, я получаю удовольствие от выполнения проектов «сделай сам» и создания чего-то своими руками.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимый мотивационный спикер?", "Тони Роббинс — мотивационный спикер, которого я нахожу вдохновляющим.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какое ваше любимое блюдо из пасты?", "Я люблю сливочную и ароматную феттучини Альфредо.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Ваш любимый сорт чая?", "Мой любимый чай Эрл Грей с ноткой бергамота.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата о любви?", "«Любовь не в том, сколько дней, недель или месяцев вы вместе, а в том, как сильно вы любите друг друга каждый день» от Неизвестного.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какая ваша любимая начинка для пиццы?", "Мне нравится классическая пицца Маргарита со свежим базиликом и моцареллой.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вам нравится кемпинг?", "Да, кемпинг позволяет мне отключиться от технологий и воссоединиться с природой.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид салата?", "Мне нравится освежающий салат Цезарь с курицей-гриль.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата из мотивационной книги?", "В книге Джен «Ты задира» есть много вдохновляющих цитат, одна из моих любимых: «Тебя любят. Сильно. Яростно. Безоговорочно».");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Каков ваш любимый способ отблагодарить сообщество?", "Волонтерство в местных организациях и участие в общественных мероприятиях — это для меня значимый способ отблагодарить.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вам нравится разгадывать головоломки или загадки?", "Да, я нахожу это умственно стимулирующим и отличным способом бросить себе вызов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вам нравится танцевать?", "Да, танцы – это интересный способ выразить себя и расслабиться.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид тренировок?", "Я считаю, что кикбоксинг — отличный способ оставаться в форме и снимать стресс.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие ваши любимые фрукты?", "Я люблю сочные арбузы в летние месяцы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вам нравится ходить на концерты?", "Да, живая музыка и энергия концерта – это то, что мне очень нравится.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы жаворонок или сова?", "Я более продуктивна и бдительна по утрам, поэтому считаю себя жаворонком.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата, которая вас вдохновляет?", "«Будущее принадлежит тем, кто верит в красоту своей мечты» Элеоноры Рузвельт.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится начинать день?", "Спокойная утренняя рутина, состоящая из медитации и здорового завтрака, задает позитивный тон моему дню.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Нравится ли вам ходить в художественные музеи?", "Да, знакомство с различными произведениями искусства и оценивание их красоты – это прекрасный опыт для меня.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид цветов?", "Я люблю элегантность и аромат роз.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие ваши любимые хлопья для завтрака?", "Мне нравится тарелка мюсли со свежими фруктами и йогуртом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите острую пищу?", "Да, мне нравится привкус специй в блюдах.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид печенья?", "Печенье с шоколадной крошкой — моя слабость.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата известного человека?", "«Единственный способ сделать большую работу — любить то, что ты делаешь» Стива Джобса.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться активным?", "Долгие прогулки на природе или занятия йогой помогают мне быть активным и заземленным.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ избежать обезвоживания?", "Мне нравится пить настоянную воду с лимоном и мятой.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вам нравится ходить на пляж?", "Да, мне нравится ощущение песка между пальцами ног и звук разбивающихся волн.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый тип сэндвича?", "Я люблю классический сэндвич с индейкой и авокадо на свежеиспеченном хлебе.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите острую пищу?", "Да, я получаю удовольствие от острого блюда.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вам нравится готовить?", "Да, мне доставляет удовольствие экспериментировать с новыми рецептами и вкусами на кухне.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ расслабиться после долгого дня?", "Принятие теплой ванны с пеной и чтение книги помогают мне расслабиться и восстановить силы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата из книги?", "«Счастье можно найти даже в самые темные времена, если только не забыть включить свет» из книги «Гарри Поттер и узник Азкабана» Дж. К. Роулинг.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид начинки для пиццы?", "Мне нравится вегетарианская пицца со свежими овощами и сыром моцарелла.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая настольная игра?", "Я люблю играть в стратегические настольные игры, такие как Settlers of Catan.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый чай?", "Мне нравится успокаивающая чашка ромашкового чая перед сном.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите документальные фильмы?", "Да, я считаю, что документальные фильмы — отличный способ узнать о различных предметах и получить новые перспективы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какая ваша любимая заправка для салатов?", "Бальзамический винегрет — моя любимая заправка для салатов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вам нравится ходить на фермерские рынки?", "Да, мне нравится поддерживать местных фермеров и знакомиться со свежими продуктами на фермерских рынках.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться мотивированным?", "Постановка целей и разбиение их на более мелкие достижимые шаги поддерживают мою мотивацию и сосредоточенность.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата из фильма?", "«Жизнь движется довольно быстро. Если вы не остановитесь и не оглянетесь время от времени, вы можете ее пропустить» из «Выходного дня» Ферриса Бьюллера.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник мюзиклов?", "Да, мне нравится артистизм и повествование в музыкальных театральных постановках.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какая ваша любимая еда на завтрак?", "Блинчики с кленовым сиропом и ломтиком хрустящего бекона - восхитительное лакомство.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник комедийных шоу?", "Да, мне нравится смотреть ситкомы, такие как «Друзья», и стендап-комедии.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид суши-ролла?", "Мне нравится классический калифорнийский ролл со свежим крабовым мясом и авокадо.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится проводить дождливый день?", "Свернуться калачиком с хорошей книгой и чашкой горячего шоколада — это идеальный способ насладиться дождливым днем.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вы предпочитаете поддерживать местный бизнес?", "Мне нравится делать покупки в местных бутиках и есть в местных ресторанах.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник научно-фантастических фильмов?", "Да, мне нравится исследовать фантастические миры и наводящие на размышления концепции в научно-фантастических фильмах.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид орехов?", "Миндаль - мой любимый, особенно когда он жареный и слегка подсоленный.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый сорт сыра?", "Мне нравится сливочный и острый вкус козьего сыра.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вам нравится ходить на комедийные шоу?", "Да, смех – это отличный способ расслабиться и хорошо провести время.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата из исторической личности?", "«Лучший способ предсказать свое будущее — создать его» Авраама Линкольна.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ наслаждаться природой?", "Походы и изучение национальных парков позволяют мне общаться с природой и ценить ее красоту.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите острую пищу?", "Да, мне нравится вкус и острота острой кухни.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый тип супа?", "Я люблю успокаивающую тарелку куриного супа с лапшой.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится отдыхать на выходных?", "Просмотр фильмов или сериалов, устроившись на диване, — мой любимый метод расслабления.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите музеи?", "Да, мне нравится посещать художественные и исторические музеи, чтобы узнавать и ценить разные культуры и периоды истории.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата об успехе?", "'Успех не является ключом к счастью. Счастье - это ключ к успеху. Если вы любите то, что делаете, вы добьетесь успеха' Альберта Швейцера.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид кексов?", "Кексы с черникой — мой классический фаворит.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о настойчивости?", "«Успех не окончателен, неудача не фатальна: значение имеет мужество продолжать» Уинстона Черчилля.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Ваш любимый сорт хлеба?", "Люблю свежеиспеченный багет с хрустящей корочкой и мягкой начинкой.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата о жизни?", "«Жизнь на 10% состоит из того, что с нами происходит, и на 90% из того, как мы на это реагируем» Чарльза Р. Суиндолла.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите документальные фильмы?", "Да, я нахожу документальные фильмы увлекательным способом узнать о различных предметах и расширить свои знания.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид мяса на завтрак?", "Я люблю хрустящий бекон на завтрак.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вы любите пить кофе?", "Я люблю простой черный кофе со сливками.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый сорт сыра?", "Мне нравится сливочный и острый козий сыр.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид смузи?", "Мне нравится освежающий тропический смузи с ананасом, манго и кокосом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам нравится начинать день?", "Я люблю начинать свой день с утренней медитации и питательного завтрака.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата из известного спортсмена?", "«Упорный труд побеждает талант, когда талант не работает усердно» Тима Нотке.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о дружбе?", "'Дружба рождается в тот момент, когда один человек говорит другому: 'Что! Ты тоже? Я думал, что я один такой'' К.С. Льюиса.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы жаворонок или сова?", "Я больше сова и, как правило, более продуктивна в поздние вечера.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый суп?", "Томатный суп с бутербродами с сыром на гриле — это классическое сочетание, которое я люблю.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какое ваше любимое занятие на свежем воздухе?", "Я люблю совершать длительные велосипедные прогулки и исследовать новые маршруты.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какое ваше любимое блюдо из пасты?", "Я никогда не смогу устоять перед классическими спагетти карбонара с хрустящим беконом и сливочным соусом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться организованным?", "Я полагаюсь на цифровые календари и списки дел, чтобы оставаться организованным и отслеживать свои задачи.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид десерта?", "Я питаю слабость к сливочным чизкейкам со свежими фруктами.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится проводить летний день?", "Пойти на пляж и устроить пикник на берегу для меня идеальный летний день.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите комедийные шоу?", "Да, я люблю смотреть стендап-комедии и ситкомы, чтобы посмеяться.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите исторические драмы?", "Да, мне нравится смотреть исторические драмы, которые оживляют важные моменты из прошлого.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый музыкальный жанр?", "У меня эклектичный музыкальный вкус, но особенно мне нравится слушать джаз и блюз.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какая ваша любимая кухня?", "Я большой поклонник тайской кухни с ее смелыми вкусами и ароматными специями.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид десерта?", "Я сладкоежка, и я никогда не могу устоять перед богатым и декадентским шоколадным тортом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимое место для путешествий?", "Изучение исторических улиц Рима было одним из моих самых запоминающихся путешествий.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вы предпочитаете отмечать особые события?", "Я предпочитаю собираться с близкими друзьями и семьей за уютным ужином дома.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид корочки для пиццы?", "Мне нравится тонкая и хрустящая корочка для пиццы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о решимости?", "«Успех не окончателен, неудача не фатальна: значение имеет мужество продолжать» Уинстона Черчилля.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о творчестве?", "«Творчество — это развлечение разума» Альберта Эйнштейна.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата о любви?", "«Любовь состоит из одной души, населяющей два тела» Аристотеля.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вам нравится пробовать новые рецепты?", "Да, экспериментировать с разными вкусами и методами приготовления – это то, что я нахожу захватывающим.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой вкус мороженого вам больше всего нравится?", "Я люблю побаловать себя шариком богатого сливочного шоколадного мороженого с шоколадной помадкой.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ расслабиться после напряженного дня?", "Я нахожу утешение в занятиях йогой и медитации, чтобы расслабить ум и тело.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый кофейный напиток?", "Мне нравится бархатистый капучино с щепоткой какао сверху.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник экстремальных видов спорта?", "Да, мне нравятся такие занятия, как скалолазание и зиплайн для получения прилива адреналина.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться активным в помещении?", "Танцы — это мое любимое занятие в помещении, чтобы оставаться активным и получать удовольствие.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о счастье?", "«Счастье — это не что-то готовое. Оно приходит от ваших собственных действий» Далай-ламы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о дружбе?", "«Настоящий друг — это тот, кто всегда рядом во время взлетов и падений жизни» от Неизвестного.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата о благодарности?", "«Благодарность превращает то, что у нас есть, в достаточно» Мелоди Битти.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник анимационных фильмов?", "Да, анимационные фильмы могут быть невероятно душевными и интересными для всех возрастов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата об обучении?", "«Чем больше вы читаете, тем больше вещей вы узнаете. Чем больше вы узнаете, тем больше мест вы посетите» доктора Сьюза.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата о настойчивости?", "'Упорство - это не долгий забег; это множество коротких забегов один за другим' Уолтера Эллиота.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ избежать обезвоживания?", "Настоянная вода с ломтиками огурца и листьями мяты освежает и увлажняет.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите садоводство?", "Да, я получаю удовольствие от ухода за растениями и наблюдения за тем, как они растут в моем саду.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любитель пеших прогулок?", "Абсолютно! Мне нравится вызов и потрясающие виды, которые предлагает пеший туризм.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите настольные игры?", "Да, мне нравится собираться с друзьями и играть в стратегические настольные игры, чтобы весело и соревновательно провести время.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вам нравится ходить в картинные галереи?", "Абсолютно! Я ценю красоту и творчество, представленные в художественных галереях.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник научно-фантастических книг?", "Да, погружение в воображаемые миры и футуристические концепции — это для меня захватывающий опыт.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие ваши любимые фрукты?", "Я обожаю сочные и сладкие манго, особенно летом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится отдыхать воскресным днем?", "Свернувшись калачиком с хорошей книгой и чашкой травяного чая, мой идеальный воскресный ритуал релаксации.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какое ваше любимое блюдо из пасты?", "Я люблю сливочную и ароматную феттучини Альфредо.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник боевиков?", "Да, я наслаждаюсь выбросом адреналина и захватывающими приключениями, изображаемыми в боевиках.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата об обучении?", "«Способность учиться — это дар; способность учиться — это навык; готовность учиться — это выбор» Брайана Герберта.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться организованным?", "Я полагаюсь на цифровой календарь и приложения для управления задачами, чтобы следить за своим расписанием и списками дел.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый тип сэндвича?", "Классический клубный сэндвич с индейкой, хрустящим беконом, листьями салата и помидорами пришелся мне по вкусу.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник стендап-комедии?", "Да, я люблю смотреть стендап-комедии, чтобы посмеяться и развлечься.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вы больше всего любите наслаждаться солнечным днем?", "Пикник в парке с друзьями, вкусная еда и игры на свежем воздухе.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник фотографии?", "Да, запечатлевать моменты и красоту мира с помощью фотографии — моя страсть.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о доброте?", "«Ни одно доброе дело, каким бы малым оно ни было, никогда не бывает напрасным» Эзопа.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Ваш любимый сорт чая?", "Я нахожу утешение в теплой чашке ромашкового чая, особенно перед сном.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам нравится начинать день?", "Освежающая утренняя прогулка, за которой следует питательный завтрак, задает позитивный тон на весь день.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о мудрости?", "«Единственная истинная мудрость заключается в том, чтобы знать, что вы ничего не знаете» Сократа.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Ваш любимый сорт шоколада?", "Я любитель темного шоколада с высоким содержанием какао, особенно в сочетании с миндалем или морской солью.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ расслабиться перед сном?", "Я нахожу расслабление в чтении книги или прослушивании успокаивающей музыки перед сном.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник живых музыкальных выступлений?", "Абсолютно! Энергия и атмосфера живых музыкальных выступлений бесподобны.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид цветов?", "Подсолнухи всегда вызывают у меня улыбку своими яркими цветами и жизнерадостным внешним видом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата об успехе?", "'Успех не является ключом к счастью. Счастье - это ключ к успеху. Если вы любите то, что делаете, вы добьетесь успеха' Альберта Швейцера.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите документальные фильмы?", "Да, я нахожу документальные фильмы информативными и поучительными, позволяя мне узнать о различных темах.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться в форме?", "Мне нравится заниматься йогой и регулярно ходить на пробежки, чтобы поддерживать свою физическую форму.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата о мечтах?", "«Будущее принадлежит тем, кто верит в красоту своей мечты» Элеоноры Рузвельт.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид овощей?", "Жареная брюссельская капуста с оттенком бальзамической глазури для меня вкусный и полезный выбор.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник кулинарных шоу?", "Да, я нахожу кулинарные шоу интересными и познавательными, они вдохновляют меня пробовать новые рецепты.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится отдыхать на выходных?", "Проводить время с близкими на прогулках или просто наслаждаться обществом друг друга дома.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата о жизни?", "«Жизнь не в том, чтобы ждать, пока пройдет буря, а в том, чтобы научиться танцевать под дождем» Вивиан Грин.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид хлеба?", "Хлеб на закваске с его острым вкусом и жевательной текстурой — мой самый любимый.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник документальных фильмов?", "Да, документальные фильмы предлагают увлекательный и информативный способ исследовать реальные истории и события.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник экстремальных видов спорта?", "Да, острые ощущения и прилив адреналина от таких занятий, как скалолазание и банджи-джампинг, волнуют.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый тип супа?", "Успокаивающая тарелка куриного супа с лапшой никогда не перестает согревать мое тело и душу.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о счастье?", "'Счастье - это не пункт назначения, а образ жизни' от Неизвестного.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любитель мультфильмов?", "Да, мультфильмы обладают особым очарованием и пробуждают во мне внутреннего ребенка.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться организованным?", "Я полагаюсь на комбинацию цифровых инструментов и физического планировщика, чтобы отслеживать свои задачи и встречи.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата из известного спортсмена?", "«Единственный способ делать большую работу — любить то, что ты делаешь» Стива Джобса.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид смузи?", "Освежающая тропическая смесь ананаса, манго и кокоса — мой любимый смузи.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится проводить летний день?", "Освежиться в бассейне или приготовить барбекю с друзьями и семьей.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата об обучении?", "«Образование — самое мощное оружие, которое вы можете использовать, чтобы изменить мир» Нельсона Манделы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник исторических драм?", "Да, я нахожу исторические драмы увлекательными, поскольку они оживляют важные события и эпохи.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о решимости?", "«Успех не окончателен, неудача не фатальна: значение имеет мужество продолжать» Уинстона Черчилля.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о творчестве?", "«Творчество — это развлечение разума» Альберта Эйнштейна.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый тип блюда из макарон?", "Сливочные и сырные макароны с сыром — это классическое блюдо из макарон, которое я просто обожаю.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любитель комедийных шоу?", "Абсолютно! Мне нравится смотреть комедийные шоу и стендап-шоу, чтобы посмеяться и развлечься.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вы предпочитаете наслаждаться чашкой кофе?", "Я предпочитаю чашку простого черного кофе с оттенком корицы для усиления вкуса.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый сорт сыра?", "Мне нравится сливочный и острый вкус козьего сыра, особенно в сочетании с медом и грецкими орехами.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится расслабляться после долгого дня?", "Я нахожу утешение в теплой ванне и отдыхе с хорошей книгой.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам нравится начинать день?", "Мне нравится начинать день с сеанса успокаивающей медитации и питательного завтрака.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый тип бутерброда?", "Я не могу устоять перед классическим бутербродом BLT (бекон, салат и помидор) на поджаренном хлебе.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Нравится ли вам посещать живые концерты?", "Да, энергия и атмосфера живых концертов зажигательны, и мне нравится их испытывать.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам нравится начинать утро?", "Быстрая прогулка, за которой следует здоровый завтрак, задает позитивный тон на весь день.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид начинки для пиццы?", "Мне нравится классическое сочетание пепперони, грибов и черных оливок в моей пицце.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любитель активного отдыха?", "Абсолютно! Мне нравится ходить в походы, ходить в походы и исследовать природу, когда у меня есть такая возможность.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится проводить дождливый день?", "Свернувшись калачиком на диване с уютным пледом, чашкой горячего чая и просмотром фильмов или чтением.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Ваш любимый сорт шоколада?", "Я питаю слабость к сливочному молочному шоколаду с оттенком лесного ореха.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата об успехе?", "'Успех не является ключом к счастью; счастье - это ключ к успеху' Альберта Швейцера.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Ваш любимый сорт чая?", "Мне нравится успокаивающий и ароматный вкус ромашкового чая, особенно перед сном.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите пикники на природе?", "Абсолютно! Наслаждаться вкусной едой и красотой природы на пикнике — это восхитительный опыт.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник комедий?", "Да, я люблю посмеяться и нахожу комедии отличным способом расслабиться и повеселиться.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид десерта?", "Я питаю слабость к декадентским и насыщенным шоколадным тортам с расплавленным центром.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата о доброте?", "'Доброта - это язык, который слышат глухие и видят слепые' Марка Твена.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите домашние настольные игры?", "Да, мне нравится играть в настольные игры с друзьями и семьей, особенно уютными вечерами.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата об обучении?", "«Чем больше вы читаете, тем больше вещей вы узнаете. Чем больше вы узнаете, тем больше мест вы посетите» доктора Сьюза.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться активным?", "Я нахожу радость в танце; это интересный способ оставаться активным и творчески выражать себя.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится отдыхать в выходные?", "Проводить время с близкими, будь то неторопливая прогулка или вечер кино дома.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о благодарности?", "«Благодарность не только величайшая из добродетелей, но и родительница всех других» Марка Туллия Цицерона.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите романтические фильмы?", "Да, мне нравятся душевные истории и эмоциональные путешествия, изображенные в романтических фильмах.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник фантастических фильмов?", "Абсолютно! Я нахожу футуристические концепции и воображаемые миры в научно-фантастических фильмах захватывающими.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид салата?", "Я люблю освежающий греческий салат с хрустящими листьями салата, помидорами, огурцами, оливками и сыром фета.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится отмечать знаменательную дату?", "Собраться с близкими за особенным ужином или устроить памятную вечеринку — отличный способ отпраздновать это событие.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о дружбе?", "'Настоящий друг - это тот, кто всегда рядом во время взлетов и падений' от Неизвестного.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник художественных галерей?", "Да, изучение художественных галерей и оценка различных форм художественного самовыражения — это прекрасный опыт.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид пасты?", "Я никогда не смогу устоять перед тарелкой сливочного феттучини Альфредо.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый фруктовый сок?", "Мне нравится освежающий стакан свежевыжатого апельсинового сока по утрам.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться мотивированным?", "Постановка целей и визуализация желаемого результата помогают мне оставаться мотивированным и сосредоточенным.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о настойчивости?", "«Успех не окончателен, неудача не фатальна: значение имеет мужество продолжать» Уинстона Черчилля.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид мороженого?", "Я наслаждаюсь сливочным вкусом печенья и сливочного мороженого с кусочками шоколадного печенья.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится начинать разговор?", "Задавать открытые вопросы, чтобы показать неподдельный интерес к другому человеку и его опыту.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник театральных постановок?", "Да, я люблю живые выступления, замысловатые декорации и мощное повествование театральных постановок.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата о любви?", "«Любовь состоит из одной души, населяющей два тела» Аристотеля.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид печенья?", "Я никогда не смогу устоять перед вечной классикой печенья с шоколадной крошкой, особенно когда оно свежеиспеченное.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится проводить снежный день?", "Лепить снеговиков, играть в снежки и уютно проводить время дома с горячим какао и хорошей книгой.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник боевиков?", "Да, мне нравится адреналин и захватывающие трюки в боевиках.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о вдохновении?", "«Единственный способ делать великие дела — это любить то, что вы делаете» Стива Джобса.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый гамбургер?", "Я люблю сочный и ароматный чизбургер с беконом и всеми классическими начинками.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться сосредоточенным?", "Создание спокойного и организованного рабочего пространства, минимизация отвлекающих факторов и разбиение задач на управляемые части.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата об осознанности?", "«Настоящий момент — единственный момент, доступный нам, и это дверь ко всем моментам» Тик Нат Хана.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите мюзиклы?", "Абсолютно! Увлекательное повествование с помощью музыки и танца в мюзиклах — это уникальный опыт.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый суп?", "Я нахожу утешение в тарелке теплого томатного супа с бутербродом с жареным сыром.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите документальные фильмы о путешествиях?", "Да, мне нравится смотреть документальные фильмы о путешествиях, чтобы исследовать разные культуры и красивые места.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ выразить свое творчество?", "Мне нравится рисовать и создавать произведения искусства, которые позволяют мне выражать свои мысли и эмоции.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата о мужестве?", "'Мужество - это не отсутствие страха, а победа над ним' Нельсона Манделы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый сорт сыра?", "Я питаю слабость к сливочному и острому вкусу сыра с плесенью.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник анимационных телешоу?", "Да, анимационные телешоу предлагают широкий спектр творческих историй и забавных персонажей.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о мотивации?", "«Поверь, что можешь, и ты уже на полпути» Теодора Рузвельта.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится проводить летний вечер?", "Приготовить барбекю с друзьями, вкусно поесть и вместе полюбоваться закатом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид кексов?", "Я нахожу черничные кексы неотразимыми, особенно когда они влажные и наполнены черникой.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник документальных сериалов?", "Да, документальные сериалы предлагают углубленное изучение различных тем и держат меня в напряжении.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ зарядиться энергией?", "Проводить время на природе, гулять или заниматься осознанностью и медитацией.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата о мечтах?", "«Будущее принадлежит тем, кто верит в красоту своей мечты» Элеоноры Рузвельт.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какая ваша любимая заправка для салата?", "Мне нравится острая бальзамическая заправка винегрет для моих салатов для взрывного вкуса.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ успокоиться перед сном?", "Чтение книги или занятия легкой йогой помогают мне расслабиться и подготовиться к спокойному ночному сну.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите криминальные драмы?", "Да, я нахожу криминальные драмы интригующими своими напряженными сюжетами и сложными персонажами.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о самосовершенствовании?", "«Единственный способ сделать большую работу — любить то, что ты делаешь» Стива Джобса.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид суши?", "Я люблю свежесть и нежный вкус нигири-суши с лососем.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите документальные фильмы о природе?", "Абсолютно! Документальные фильмы о природе демонстрируют красоту и чудеса природного мира.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о терпении?", "«Терпение — это не умение ждать, а умение сохранять хорошее настроение во время ожидания» Джойс Мейер.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится отмечать день рождения друга?", "Планировать вечеринку-сюрприз или организовать встречу, чтобы они почувствовали себя любимыми и особенными.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый фрукт?", "Не могу устоять перед сладким и сочным вкусом спелого манго.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник психологических триллеров?", "Да, я нахожу психологические триллеры захватывающими своими умопомрачительными сюжетами и напряженными поворотами.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ выразить благодарность?", "Написать сердечные благодарственные письма или найти время, чтобы устно выразить признательность другим.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата об управлении временем?", "«Не смотри на часы, делай то, что они делают. Продолжай» Сэма Левенсона.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой хлеб для бутербродов вы предпочитаете?", "Мне нравится ореховый и сытный вкус цельнозернового хлеба для бутербродов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится проводить весенний день?", "Неторопливая велосипедная прогулка или пикник в цветущем парке.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник реалити-шоу?", "Да, я нахожу реалити-шоу интересными и с удовольствием слежу за путешествиями участников.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие ваши любимые хлопья для завтрака?", "Мне нравится начинать день с тарелки хрустящей мюсли с сухофруктами и орехами.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о мудрости?", "«Единственная истинная мудрость заключается в том, чтобы знать, что вы ничего не знаете» Сократа.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любитель фэнтези?", "Абсолютно! Я люблю теряться в волшебных мирах и эпических приключениях в фантастических романах.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о счастье?", "«Счастье вашей жизни зависит от качества ваших мыслей» Марка Аврелия.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ снятия стресса?", "Занятия медитацией, йогой или прослушивание успокаивающей музыки помогают мне расслабиться и отдохнуть.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите исторические драмы?", "Да, я нахожу исторические драмы увлекательными, поскольку они переносят меня в разные периоды времени и учат меня истории.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид суши-ролла?", "Мне нравится сочетание свежего тунца, авокадо и огурца в остром ролле с тунцом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой соус для пасты вы предпочитаете?", "Я люблю насыщенный и пикантный соус маринара к макаронам, наполненный ароматами помидоров, чеснока и трав.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится проводить летний день на пляже?", "Строить замки из песка, купаться в океане и устраивать пикники на берегу.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата о настойчивости?", "'Упорство - это не долгий забег; это множество коротких забегов один за другим' Уолтера Эллиота.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ преодолеть творческий блок?", "Перерыв, прогулка на природе или поиск новых творческих источников вдохновения помогают мне преодолеть творческий блок.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы являетесь поклонником приключенческих видеоигр?", "Да, мне нравятся острые ощущения и захватывающий опыт приключенческих видеоигр.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о дружбе?", "«Дружба рождается в тот момент, когда один человек говорит другому: «Что! Ты тоже? Я думал, что я один такой». К. С. Льюис.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид фруктового смузи?", "Мне нравится освежающий клубнично-банановый смузи с каплей кокосового молока.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите документальные фильмы?", "Да, документальные фильмы дают ценную информацию о реальных историях, культурах и важных социальных проблемах.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится проводить ленивое воскресенье?", "Отдохнуть дома, почитать хорошую книгу или запоем посмотреть сериал и насладиться неторопливым завтраком.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый тип блинов?", "Я люблю пышные блинчики с черникой, сбрызнутые кленовым сиропом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о прощении?", "'Прощение - это аромат, который изливает фиалка на пятку, которая ее раздавила' Марка Твена.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник фотографии дикой природы?", "Абсолютно! Фотографии дикой природы отражают красоту и необузданность животных в их естественной среде обитания.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой тип мексиканской кухни вам больше всего нравится?", "Мне нравится пикантный вкус уличных тако, наполненных нежным жареным мясом, свежей сальсой и кинзой.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимая цитата о целеустремленности?", "'Разница между успешным человеком и другими не в недостатке силы, не в недостатке знаний, а скорее в недостатке воли' Винса Ломбарди.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник комедийных шоу?", "Да, мне нравится смех и комедийный талант, демонстрируемый в комедийных шоу.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится отмечать начало нового года?", "Размышляя о прошедшем году, формулируя намерения и проводя время с близкими в канун Нового года.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться организованным?", "Использование планировщика или цифровых инструментов, составление списков дел и регулярная уборка моего пространства помогают мне оставаться организованным.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ расслабиться после долгого дня?", "Я нахожу расслабление, принимая теплую ванну и свернувшись калачиком с хорошей книгой.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая цитата об успехе?", "«Успех не является ключом к счастью; счастье — это ключ к успеху. Если вы любите то, что делаете, вы добьетесь успеха» Альберта Швейцера.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какая ваша любимая кухня?", "Я питаю слабость к итальянской кухне, особенно к таким блюдам, как спагетти карбонара и пицца Маргарита.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы жаворонок или сова?", "Я определенно больше ночная сова — я обычно чувствую себя наиболее энергичным и продуктивным в поздние часы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вам нравится активный отдых?", "Да, я люблю проводить время на свежем воздухе, будь то походы, езда на велосипеде или просто прогулки на природе.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться активным?", "Мне нравится заниматься йогой — это помогает мне оставаться физически активным, а также способствует психическому благополучию.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы собачник или кошатник?", "Я определенно собачник. Мне нравится их преданность, игривость и радость, которую они приносят в повседневную жизнь.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какое ваше любимое время года?", "Я обожаю красоту осени — яркие цвета, прохладный ветерок и уютные свитера делают это время года моим любимым.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас хобби или интересы?", "Мне нравится фотографировать, играть на музыкальных инструментах и экспериментировать с новыми рецептами на кухне.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться на связи с друзьями и семьей?", "Я считаю важным регулярно проводить видеозвонки или встречаться лично, чтобы наверстать упущенное и поддерживать крепкие отношения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любитель кофе или чая?", "Я больше любитель чая — я люблю пробовать разные вкусы и нахожу успокаивающим глоток теплой чашки чая.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый вид упражнений?", "Мне нравится бегать или участвовать в высокоинтенсивных интервальных тренировках (ВИИТ), чтобы повысить частоту сердечных сокращений.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимый жанр музыки?", "У меня эклектичный музыкальный вкус, но особенно мне нравятся инди-рок и акустический фолк.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите настольные игры?", "Да, я люблю играть в настольные игры с друзьями и семьей — это отличный способ повеселиться и сблизиться.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится начинать день?", "Мне нравится начинать свой день с практики осознанности или медитации, чтобы задать позитивный тон на весь оставшийся день.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый жанр книг?", "Мне нравится читать книги самых разных жанров, но мистические романы и детективы всегда держат меня в плену.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимая программа тренировок?", "Мне нравится менять тренировки, чтобы они были интересными, но сочетание кардио и силовых тренировок мне подходит.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится отмечать особые события?", "Мне нравится собираться с близкими за вкусным ужином, смеяться и вместе создавать прекрасные воспоминания.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник садоводства?", "Да, я нахожу садоводство лечебным и люблю наблюдать за тем, как растения растут и процветают под моим присмотром.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ помочь сообществу?", "Я работаю волонтером в местных приютах и делаю пожертвования благотворительным организациям, которые связаны с теми делами, которые мне небезразличны.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ черпать вдохновение?", "Я черпаю вдохновение в природе, искусстве, чтении мотивирующих книг или прослушивании вдохновляющих подкастов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимый вид десерта?", "Я сладкоежка, но если бы мне пришлось выбирать, я бы сказал, что шоколадный лаваш — мой самый любимый.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любитель кулинарных шоу?", "Абсолютно! Мне нравится смотреть кулинарные шоу, чтобы узнавать новые рецепты и техники от талантливых поваров.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какая ваша любимая одежда для тренировок?", "Я предпочитаю удобную спортивную одежду, такую как леггинсы и дышащий топ, для максимальной подвижности во время тренировок.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимый жанр кино?", "Я ценю широкий спектр жанров кино, но особенно мне нравятся психологические триллеры и наводящие на размышления драмы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ выражения творчества?", "Мне нравится выражать свое творчество через рисование, письмо и создание проектов «сделай сам».");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник подкастов?", "Да, мне нравится слушать подкасты, особенно те, которые рассказывают о личном развитии, настоящих преступлениях и науке.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какая ваша любимая начинка для пиццы?", "Я поклонник классического сочетания — пицца маргарита со свежим базиликом и сыром моцарелла.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ снять стресс в течение напряженного дня?", "Короткие перерывы, чтобы попрактиковаться в упражнениях на глубокое дыхание, и короткая прогулка помогают мне перезагрузиться и переориентироваться.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимый тип музыки для тренировок?", "Мне нравятся оптимистичные и энергичные музыкальные жанры, такие как поп, рок и электронная музыка, которые поддерживают мою мотивацию во время тренировок.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится поддерживать местный бизнес?", "Я стараюсь делать покупки в местных магазинах, обедать в независимых ресторанах и посещать общественные мероприятия.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите комедии?", "Да, мне нравятся комедии — они всегда меня смешит и поднимает настроение.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ охладиться в жаркий летний день?", "Я люблю освежиться в бассейне или расслабиться в тени с прохладительным напитком.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ заботиться о себе?", "Я отдаю предпочтение заботе о себе, занимаясь такими видами деятельности, как принятие ванн с пеной, занятия йогой и чтение вдохновляющих книг.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оказать положительное влияние на окружающую среду?", "Я стремлюсь сократить количество отходов, практикуя переработку, используя многоразовые продукты и не забывая о потреблении энергии.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник научно-фантастических фильмов?", "Да, я нахожу научно-фантастические фильмы захватывающими своей творческой концепцией и футуристическим сеттингом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимый тип оборудования для тренировок?", "Мне нравится использовать эспандеры для силовых тренировок — они универсальны, портативны и предлагают сложную тренировку.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимый вид искусства?", "Я ценю различные художественные средства, но особенно мне нравится работать с акварелью и акриловыми красками.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник джазовой музыки?", "Да, я ценю успокаивающие мелодии и импровизационный характер джазовой музыки — она создает расслабляющую атмосферу.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться организованным в повседневной жизни?", "Я полагаюсь на комбинацию цифровых инструментов, таких как календари и приложения для управления задачами, а также на использование физического планировщика.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимый сорт шоколада?", "Темный шоколад с морской солью — моя слабость — это идеальное сочетание сладкого и пикантного вкусов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится начинать разговор с кем-то новым?", "Обычно я задаю открытые вопросы об их интересах или опыте, чтобы лучше узнать их.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ сохранять мотивацию при достижении цели?", "Я устанавливаю небольшие вехи, вознаграждаю себя за прогресс и окружаю себя поддерживающими людьми, которые меня вдохновляют.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник музыкального театра?", "Да, мне нравится артистизм и рассказывание историй в постановках музыкального театра — они оживляют истории через песни и танцы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ расслабиться во время долгого перелета?", "Я люблю слушать успокаивающую музыку или подкасты, читать увлекательную книгу или смотреть фильмы, чтобы скоротать время.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимый метод заваривания кофе?", "Я поклонник кофе с пуровер — он обеспечивает точный процесс заваривания и дает чашку богатого ароматного кофе.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любитель танцевальных постановок?", "Да, я ценю грацию и атлетизм в танцевальных постановках, будь то балет, современные или культурные танцы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ поддержать свое психическое благополучие?", "Я отдаю предпочтение самоанализу, ведению дневника и практике благодарности для поддержания своего психического и эмоционального здоровья.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться продуктивным и сосредоточенным?", "Я разбиваю задачи на управляемые этапы, устраняю отвлекающие факторы и использую методы блокировки времени, чтобы оставаться сосредоточенным.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оказать положительное влияние на ваше сообщество?", "Я добровольно работаю в местных организациях, участвую в общественных мероприятиях по уборке и поддерживаю инициативы, которые способствуют позитивным изменениям.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ приправить домашнюю еду?", "Мне нравится экспериментировать с различными травами, специями и соусами, чтобы придать моим блюдам глубину и аромат.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимый тип тренировок?", "Мне нравятся занятия высокоинтенсивными интервальными тренировками (ВИИТ) — они обеспечивают эффективную тренировку всего тела и заставляют мое сердце биться быстрее.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник иностранных фильмов?", "Да, я ценю разнообразие повествования и культурных взглядов, предлагаемых иностранными фильмами.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимое направление для путешествий?", "Я люблю исследовать города с богатой историей и культурой, а также живописные природные ландшафты, такие как горы и береговые линии.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться в курсе текущих событий?", "Я получаю информацию, читая новостные статьи, слушая подкасты и следя за авторитетными источниками новостей в социальных сетях.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник импровизационной комедии?", "Да, импровизационно-комедийные шоу мне интересны — впечатляет непосредственность и сообразительность исполнителей.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вам больше всего нравится отмечать личные достижения?", "Мне нравится побаловать себя чем-то особенным, будь то небольшая снисходительность или значимый опыт в ознаменование достижения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимый вид упражнений в помещении?", "Мне нравится заниматься йогой или смотреть онлайн-видео с тренировками, не выходя из дома.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ выразить признательность другим?", "Я выражаю благодарность словами одобрения, небольшими проявлениями доброты или продуманными жестами, такими как написание благодарственных записок.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Любите ли вы стендап-комедии?", "Абсолютно! Я люблю смотреть стендап-комедии — они привносят смех и комическое облегчение в повседневную жизнь.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Каков ваш любимый способ исследовать новые места во время путешествий?", "Мне нравится ходить пешком или ездить на велосипеде по окрестностям, пробовать местную кухню и посещать исторические достопримечательности, чтобы погрузиться в культуру.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ получать вдохновение во время творческих проектов?", "Я ищу вдохновение в природе, художественных галереях и других творческих работах, чтобы питать свое воображение и поддерживать поток своих идей.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимый тип печенья?", "Печенье с шоколадной крошкой — мой классический фаворит — перед сочетанием теплого липкого шоколада и мягкой сердцевины невозможно устоять.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы любите документальные фильмы?", "Да, я нахожу документальные фильмы информативными и поучительными — они дают ценную информацию о различных темах и реальных историях.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться сосредоточенным во время учебы или работы?", "Я создаю специальное рабочее пространство, минимизирую отвлекающие факторы и разбиваю задачи на более мелкие, управляемые части, чтобы поддерживать концентрацию.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас любимый тип супа?", "Томатный суп с жареным сыром — мой любимый утешительный суп, идеально подходящий для холодных дней или когда мне нужно немного утешения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Вы поклонник реалити-шоу?", "Хотя я не смотрю их часто, я могу оценить развлекательную ценность и уникальные перспективы, которые они предлагают.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ проявить доброту к незнакомцам?", "Я верю в случайные проявления доброты — будь то улыбка, придержание двери или помощь нуждающемуся.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ оставаться творческим и вдохновленным в повседневной жизни?", "Я занимаюсь такими видами деятельности, как рисование, пробую новые рецепты и изучаю различные художественные средства, чтобы поддерживать свое творчество.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас есть любимый тип сэндвича?", "Классический сэндвич с индейкой и авокадо с острой горчичной пастой — мой любимый быстрый и вкусный ужин.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый способ успокоиться перед сном?", "Мне нравится читать книгу, заниматься расслабляющим ночным уходом за кожей или слушать успокаивающую музыку, чтобы подготовиться к хорошему ночному сну.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие новые технологии вас больше всего волнуют?", "Меня очень интересуют достижения в области искусственного интеллекта и машинного обучения — они могут произвести революцию в различных отраслях.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии повлияли на вашу повседневную жизнь?", "Технологии сделали мою жизнь более удобной и взаимосвязанной: я могу легко общаться, мгновенно получать доступ к информации и автоматизировать повторяющиеся задачи.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какая ваша любимая функция смартфона?", "Мне нравится удобство биометрической аутентификации, такой как отпечаток пальца или распознавание лица, — она позволяет быстро и безопасно разблокировать мой телефон.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вы следите за последними техническими новостями и трендами?", "Я слежу за блогами о технологиях, слушаю подкасты и подписываюсь на информационные бюллетени, чтобы быть в курсе последних технических достижений.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какая самая инновационная технология, с которой вы столкнулись в последнее время?", "Недавно я наткнулся на систему «умный дом», которая объединяет различные устройства и позволяет управлять ими с помощью голосовых команд — это впечатляет.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что вы думаете о носимых технологиях?", "Я думаю, что носимые технологии имеют большой потенциал — они могут отслеживать показатели здоровья и физической формы, предоставлять своевременные уведомления и повышать удобство.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии изменили способ нашего общения?", "Технологии сделали общение более быстрым и доступным — мы можем общаться с людьми по всему миру с помощью различных платформ и обмена мгновенными сообщениями.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вы думаете, технологии улучшили личную жизнь или помешали ей?", "Это сложный вопрос. Хотя технологии повысили удобство, они также вызвали опасения по поводу конфиденциальности и безопасности данных, которые необходимо решить.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый технологический гаджет или устройство?", "Я большой поклонник умных динамиков, таких как Amazon Echo или Google Home — они обеспечивают голосовую помощь и могут управлять другими интеллектуальными устройствами.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии изменили то, как мы потребляем медиа?", "Технологии изменили то, как мы потребляем медиа — от потоковых сервисов для музыки и фильмов к цифровым публикациям и электронным книгам.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что вы думаете о технологии виртуальной реальности (VR)?", "Я нахожу технологию виртуальной реальности захватывающей — она может революционизировать игры, образование и даже обеспечить иммерсивные виртуальные путешествия.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии изменили отрасль здравоохранения?", "Технологии улучшили уход за пациентами, улучшили диагностику и сделали возможным удаленный мониторинг, что привело к более персонализированному и эффективному медицинскому обслуживанию.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Каково ваше мнение о влиянии социальных сетей на общество?", "Социальные сети имеют как положительные, так и отрицательные стороны: они связывают людей по всему миру, но также вызывают озабоченность по поводу конфиденциальности в Интернете и психического здоровья.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вы управляете своим цифровым присутствием и безопасностью в Интернете?", "Я отдаю предпочтение надежным и уникальным паролям, включаю двухфакторную аутентификацию и ограничиваю личную информацию, которой я делюсь в Интернете, для повышения моей безопасности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие новые технологии, по вашему мнению, окажут наибольшее влияние на будущее?", "Я считаю, что технология блокчейна может изменить различные отрасли, такие как финансы, цепочки поставок и здравоохранение.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что вы думаете о росте автоматизации и искусственного интеллекта в рабочей силе?", "Автоматизация и искусственный интеллект могут повысить эффективность и производительность, но крайне важно устранить влияние на перемещение рабочих мест и переквалификацию.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии произвели революцию в секторе образования?", "Технологии изменили образование с помощью платформ электронного обучения, онлайн-курсов, интерактивных образовательных инструментов и виртуальных классов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какое ваше любимое приложение или программное обеспечение для повышения производительности?", "Я считаю такие инструменты управления проектами, как Trello или Asana, чрезвычайно полезными для организации задач, совместной работы с командами и сохранения продуктивности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие этические соображения, по вашему мнению, следует учитывать при разработке и использовании технологий?", "Этические соображения включают конфиденциальность данных, предвзятость алгоритмов, ответственную разработку ИИ и обеспечение равного доступа к технологиям для всех.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какую роль, по вашему мнению, будут играть технологии в борьбе с изменением климата?", "Технологии могут сыграть значительную роль в решении проблемы изменения климата с помощью решений в области возобновляемых источников энергии, интеллектуальных сетей и устойчивых методов в различных отраслях.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии изменили то, как мы делаем покупки и ведем бизнес?", "Технологии упростили электронную коммерцию, цифровые платежи и анализ данных, позволяя компаниям охватить более широкую аудиторию и предоставлять персонализированный опыт.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Каково ваше мнение о будущем беспилотных автомобилей?", "Беспилотные автомобили могут повысить безопасность дорожного движения и повысить эффективность перевозок, но ключевыми факторами являются нормативно-правовая база и общественное признание.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии повлияли на индустрию развлечений?", "Технологии произвели революцию в индустрии развлечений — от потоковых сервисов и контента по запросу до иммерсивных впечатлений, таких как виртуальная реальность и дополненная реальность.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой у вас любимый подкаст или канал на YouTube, посвященный технологиям?", "Мне нравится слушать [вставить название подкаста] и смотреть [вставить канал на YouTube] для глубоких дискуссий и глубокого анализа технологических тенденций.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии повлияли на сельское хозяйство?", "Технологии привели к точному земледелию, дистанционному зондированию и методам ведения сельского хозяйства, основанным на данных, которые оптимизируют урожайность и минимизируют воздействие на окружающую среду.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие проблемы вы предвидите при широком внедрении технологий использования возобновляемых источников энергии?", "Проблемы включают в себя развитие инфраструктуры, возможности хранения и преодоление сопротивления со стороны существующих энергетических систем, но потенциальные выгоды значительны.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что вы думаете о влиянии технологий на психическое здоровье?", "Технологии оказывают как положительное, так и отрицательное влияние на психическое здоровье — они могут обеспечить доступ к ресурсам и поддержке, но чрезмерное время, проводимое за экраном, также может способствовать возникновению таких проблем, как цифровая зависимость. или социальная изоляция.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии изменили наши путешествия и навигацию?", "Технологии упростили путешествия благодаря навигационным приложениям, обновлениям рейсов в режиме реального времени, платформам онлайн-бронирования и иммерсивным инструментам планирования путешествий.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Каково ваше мнение о влиянии технологий на личные отношения?", "Технологии упростили общение с другими людьми, но важно найти баланс и убедиться, что технологии не заменяют содержательное личное общение.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие инновации вы надеетесь увидеть в области возобновляемых источников энергии?", "Я надеюсь увидеть достижения в технологиях хранения энергии, повышение эффективности возобновляемых источников и более доступные решения для внедрения экологически чистой энергии.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии изменили сферу общения и совместной работы на рабочем месте?", "Технологии упростили общение в режиме реального времени, удаленную работу и инструменты для совместной работы, позволяя командам эффективно работать вместе независимо от местоположения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие меры кибербезопасности, по вашему мнению, имеют решающее значение в современном цифровом ландшафте?", "Надежные пароли, регулярные обновления программного обеспечения, осведомленность о попытках фишинга и шифрование необходимы для защиты личной и конфиденциальной информации в Интернете.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какую роль, по вашему мнению, будут играть технологии в здравоохранении в будущем?", "Технологии, скорее всего, будут и впредь способствовать развитию телемедицины, удаленного мониторинга состояния пациентов, персонализированной медицины и медицинских решений, основанных на данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой ваш любимый документальный фильм или телесериал о технологиях?", "Я настоятельно рекомендую посмотреть [вставить название документального фильма или телесериала], так как он дает ценную информацию об эволюции и влиянии технологий.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии повлияли на сферу финансов и банковского дела?", "Технологии привели к онлайн-банкингу, решениям для мобильных платежей и финансовым приложениям, которые обеспечивают удобный доступ к услугам и упрощают финансовые операции.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии изменили область фотографии и изобразительного искусства?", "Технологии привели к появлению цифровой фотографии, программного обеспечения для редактирования изображений и онлайн-платформ для обмена и демонстрации художественных работ.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие проблемы вы ожидаете при широком внедрении электромобилей?", "Проблемы включают создание необходимой инфраструктуры зарядки, совершенствование технологии аккумуляторов и решение проблем, связанных с запасом хода и стоимостью.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии повлияли на доступность образования для людей во всем мире?", "Технологии сделали образование более доступным благодаря онлайн-курсам, платформам дистанционного обучения и цифровым ресурсам, которые могут быть доступны людям во всем мире.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Каково ваше мнение об использовании дронов в различных отраслях?", "Дроны применяются в различных отраслях, таких как сельское хозяйство, аэрофотосъемка, службы доставки и проверка инфраструктуры, — они обеспечивают эффективность и экономию средств.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии повлияли на область 3D-печати?", "Технологии произвели революцию в 3D-печати, позволив быстро создавать прототипы, настраивать их и продвигаться в таких областях, как медицина, аэрокосмическая промышленность и производство.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вы думаете, какую роль будут играть технологии в исследовании космоса и колонизации?", "Технологии будут иметь важное значение для дальнейшего исследования космоса, разработки устойчивых систем жизнеобеспечения и обеспечения возможности колонизации людьми других планет.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что вы думаете об этических последствиях автономных дронов и роботов?", "Этические соображения включают обеспечение подотчетности, решение проблемы возможного увольнения и разработку руководящих принципов использования автономных систем.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии улучшили реагирование на бедствия и управление ими?", "Технологии сделали возможными системы раннего предупреждения, сбор данных в режиме реального времени и координацию усилий по оказанию помощи, расширяя возможности реагирования на бедствия и управления ими.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие достижения вы надеетесь увидеть в области искусственного интеллекта?", "Я надеюсь увидеть достижения в области объяснимого ИИ, этических основ ИИ и систем ИИ, которые вносят положительный вклад в общество, сводя к минимуму предубеждения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии изменили музыкальную индустрию?", "Технологии произвели революцию в производстве, распространении и потреблении музыки — от цифровых потоковых платформ до программного обеспечения для производства музыки и виртуальных концертов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии повлияли на спорт и спортивные результаты?", "Технологии привели к достижениям в области спортивной аналитики, носимых фитнес-трекеров и симуляторов тренировок, совершенствуя анализ производительности и предотвращая травмы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что вы думаете об этических последствиях технологий редактирования генов, таких как CRISPR?", "Этические соображения включают обеспечение ответственного использования технологий редактирования генов, устранение потенциальных непредвиденных последствий и участие в открытых дискуссиях об их применении.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вы думаете, какую роль технологии будут играть в решении глобальных проблем, таких как бедность и неравенство?", "Технологии могут сыграть важную роль в решении глобальных проблем, облегчая доступ к образованию, здравоохранению и ресурсам в недостаточно обслуживаемых сообществах.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии улучшили транспорт и логистику?", "Технологии оптимизировали логистические процессы, обеспечили отслеживание в режиме реального времени и улучшили планирование маршрутов, что привело к созданию более эффективных транспортных систем.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какую роль, по вашему мнению, будут играть технологии в формировании будущего работы и занятости?", "Технологии, скорее всего, продолжат автоматизировать повторяющиеся задачи, создавать новые рабочие места и требовать изменений в наборах навыков и непрерывного обучения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии изменили сферу маркетинга и рекламы?", "Технологии привели к целевому цифровому маркетингу, персонализированной рекламе и анализу данных, которые позволяют проводить более эффективные и действенные маркетинговые кампании.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что вы думаете об этических последствиях технологии распознавания лиц?", "Этические проблемы включают вторжение в частную жизнь, потенциальные предубеждения и необходимость прозрачности и регулирования при разработке и использовании технологии распознавания лиц.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии улучшили доступность для людей с ограниченными возможностями?", "Технологии позволили использовать вспомогательные устройства, средства чтения с экрана и средства связи, которые позволяют людям с ограниченными возможностями более независимо ориентироваться в мире.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии изменили сферу финансов и инвестиций?", "Технологии привели к появлению платформ онлайн-трейдинга, алгоритмической торговли, роботов-консультантов и финансовых решений на основе блокчейна, открывающих новые возможности для инвестирования и управления финансами.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какую роль, по вашему мнению, будут играть технологии в будущем виртуальной и дополненной реальности?", "Технологии, скорее всего, расширят возможности виртуальной и дополненной реальности, что приведет к более захватывающему опыту в играх, образовании и различных отраслях.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что вы думаете об этических последствиях технологии дипфейков?", "Этические проблемы связаны с потенциальным неправомерным использованием технологии дипфейков для обмана, дезинформации и нарушения личной конфиденциальности и безопасности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технология повлияла на область хранения возобновляемой энергии?", "Технология привела к достижениям в области хранения аккумуляторов, гидроаккумулирующих насосов и других инновационных решений, которые учитывают прерывистый характер возобновляемых источников энергии.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какую роль, по вашему мнению, будут играть технологии в будущем моды и текстиля?", "Технологии, скорее всего, будут способствовать устойчивым модным практикам, персонализации с помощью 3D-печати и умным тканям, повышающим комфорт и функциональность.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии изменили область кибербезопасности?", "Технологии предусмотрели передовые меры кибербезопасности, такие как шифрование, системы обнаружения вторжений и обнаружение угроз с помощью ИИ, для смягчения киберугроз и защиты цифровых активов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что вы думаете о влиянии технологий на сохранение культурного наследия и культурного наследия?", "Технологии могут помочь в сохранении культурного наследия за счет оцифровки артефактов, виртуальных музеев и инициатив по сохранению языка.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии повысили эффективность и устойчивость производственных процессов?", "Технологии позволили автоматизировать, робототехнику и анализ данных в производстве, что привело к рационализации процессов, сокращению отходов и энергоэффективным операциям.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какую роль, по вашему мнению, будут играть технологии в решении глобальных проблем продовольственной безопасности?", "Технологии могут способствовать внедрению устойчивых методов ведения сельского хозяйства, точного земледелия и вертикального земледелия, решая проблемы производства и распределения продуктов питания для растущего населения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технология повлияла на область производства возобновляемой энергии?", "Технология привела к прогрессу в солнечных панелях, ветряных турбинах и других технологиях возобновляемой энергии, повысив их эффективность и жизнеспособность в качестве альтернативных источников энергии.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что вы думаете о влиянии технологий на демократические процессы и гражданскую активность?", "Технологии облегчили доступ к информации, онлайн-активность и демократическое участие, но важно решать проблемы дезинформации и конфиденциальности в цифровую эпоху.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии изменили область архитектуры и городского планирования?", "Технологии упростили 3D-моделирование, моделирование виртуальной реальности и городское планирование на основе данных, улучшив процессы проектирования и стратегии городского развития.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вы думаете, какую роль технологии будут играть в будущем развлечений и игр?", "Технологии, вероятно, сделают возможным более захватывающий опыт виртуальной реальности, реалистичную графику в играх и интерактивные развлекательные форматы, такие как игры с дополненной реальностью.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии улучшили готовность к стихийным бедствиям и системы раннего предупреждения?", "Технологии привели к усовершенствованному мониторингу погоды, системам раннего обнаружения землетрясений и геопространственному картированию, повышая готовность к стихийным бедствиям и общественную безопасность.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что вы думаете об этических последствиях технологии биометрической идентификации?", "Этические соображения включают вопросы конфиденциальности, потенциальные предубеждения и ответственное использование биометрических данных в системах идентификации и наблюдения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии повлияли на область передачи и распределения возобновляемой энергии?", "Технологии привели к достижениям в области интеллектуальных сетей, систем хранения энергии и программного обеспечения для управления сетями, оптимизируя передачу и распределение возобновляемой энергии.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какую роль, по вашему мнению, будут играть технологии в освоении космоса и научных исследованиях в будущем?", "Технологии, скорее всего, продолжат расширять возможности исследования космоса, обеспечивать удаленные научные исследования и облегчать сбор и анализ данных в космических миссиях.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии изменили сферу розничной торговли и электронной коммерции?", "Технологии привели к появлению онлайн-рынков, персонализированных покупок и инноваций, таких как магазины без касс и дроны для доставки, которые изменили индустрию розничной торговли.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие достижения вы надеетесь увидеть в области квантовых вычислений?", "Я надеюсь увидеть достижения в области квантовых вычислений, методов исправления ошибок и практических приложений в таких областях, как криптография и проблемы оптимизации.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии улучшили доступность транспорта для людей с ограниченными возможностями?", "Технологии привели к появлению доступных функций общественного транспорта, услуг по вызову пассажиров с вариантами специальных возможностей и навигационных приложений, адаптированных для людей с ограниченными возможностями.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какую роль, по вашему мнению, будут играть технологии в защите конфиденциальности и безопасности данных в будущем?", "Технологии, вероятно, будут продолжать развивать методы шифрования, разрабатывать решения для децентрализованного хранения данных и усиливать меры по защите конфиденциальности для отдельных лиц и организаций.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии повлияли на область искусственного интеллекта в здравоохранении?", "Технологии упростили анализ медицинских изображений, прогнозную аналитику и диагностику с помощью ИИ, способствуя улучшению результатов лечения и персонализированным вариантам лечения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что вы думаете об этических последствиях автономных транспортных средств и их влиянии на безопасность дорожного движения?", "Этические соображения включают вопросы ответственности, алгоритмы принятия решений в критических ситуациях и обеспечение плавного перехода от автомобилей, управляемых человеком, к автономным транспортным средствам.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии изменили сферу финансирования и инвестиций в возобновляемые источники энергии?", "Технологии упростили создание краудфандинговых платформ, торговлю энергией на основе блокчейна и аналитику данных для оценки жизнеспособности проектов в области возобновляемых источников энергии, привлекая больше инвестиций в этот сектор.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие достижения вы надеетесь увидеть в области биоинженерии и биотехнологии?", "Я надеюсь увидеть достижения в регенеративной медицине, биофабрикации органов и устойчивых методах биообработки для целого ряда приложений.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии повысили эффективность управления цепочками поставок и логистики?", "Технологии позволили отслеживать в режиме реального времени, прогнозировать спрос на основе данных и автоматизировать управление запасами, оптимизируя операции цепочки поставок и снижая затраты.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какую роль, по вашему мнению, будут играть технологии в журналистике и СМИ в будущем?", "Технологии, скорее всего, продолжат формировать журналистику посредством репортажей на основе данных, сбора новостей с помощью ИИ и цифровых платформ для распространения и потребления контента.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии повлияли на область робототехники и автоматизации производства?", "Технологии привели к разработке промышленных роботов, коллаборативных роботов и систем автоматизации, которые повышают производительность и точность производственных процессов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что вы думаете об этических последствиях секвенирования генов и генной инженерии?", "Этические соображения включают конфиденциальность генетических данных, потенциальное неправомерное использование генетической информации и ответственные методы редактирования генов в соответствии с социальными и этическими нормами.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии изменили сферу образования для людей с особыми потребностями?", "Технологии упростили вспомогательные устройства, образовательные приложения для учащихся с особыми потребностями и платформы персонализированного обучения, предоставляя возможности инклюзивного образования.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какую роль, по вашему мнению, будут играть технологии в будущем устойчивой архитектуры и зеленых зданий?", "Технологии, скорее всего, позволят использовать устойчивые строительные материалы, энергоэффективные конструкции и интеллектуальные системы управления зданиями для более экологичных и устойчивых структур.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии повысили точность и эффективность прогнозирования погоды?", "Технологии привели к расширенному моделированию погоды, спутниковым снимкам и анализу данных, повысив точность и своевременность прогнозов погоды и предупреждений о суровой погоде.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что вы думаете о влиянии технологий на культурное разнообразие и глобализацию?", "Технологии могут способствовать культурному обмену, доступу к разнообразному контенту и цифровым платформам для усиления недостаточно представленных мнений, но крайне важно устранить цифровой разрыв и сохранить культуру .");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии повлияли на политику и регулирование в области возобновляемых источников энергии?", "Технологии повлияли на политические решения, стимулировали внедрение возобновляемых источников энергии и позволили нормативно-правовой базе поддерживать интеграцию возобновляемых источников энергии в существующие энергетические системы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие достижения вы надеетесь увидеть в области интерфейсов мозг-компьютер?", "Я надеюсь увидеть достижения в области неинвазивных интерфейсов, улучшенных средств связи для людей с ограниченными возможностями и потенциальных приложений в нейрореабилитации и психическом здоровье.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии изменили сферу спортивных трансляций и зрительских впечатлений?", "Технологии сделали возможными трансляции в высоком разрешении, мгновенные повторы, интерактивное взаимодействие с болельщиками и виртуальную реальность, улучшая общее впечатление от просмотра спортивных событий и зрительских впечатлений.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какую роль, по вашему мнению, будут играть технологии в кибербезопасности и обороне в будущем?", "Технологии, скорее всего, продолжат разработку передовых систем обнаружения угроз, методов шифрования и безопасных протоколов связи для борьбы с киберугрозами и защиты критической инфраструктуры.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии повысили эффективность и точность медицинских диагнозов?", "Технологии упростили медицинскую визуализацию, диагностику на основе ИИ и носимые устройства для мониторинга здоровья, что привело к более быстрой и точной медицинской диагностике.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что вы думаете об этических последствиях технологий улучшения человека?", "Этические соображения включают равный доступ к улучшениям, потенциальное социальное неравенство и обеспечение того, чтобы технологии улучшения человека уважали индивидуальную автономию и благополучие.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии повлияли на интеграцию возобновляемых источников энергии с электрической сетью?", "Технологии упростили связанные с сетью системы возобновляемых источников энергии, интеллектуальное управление сетью и программы реагирования на спрос, что позволило лучше интегрировать возобновляемые источники энергии в электрическую сеть.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какую роль, по вашему мнению, будут играть технологии в будущем персонализированной медицины и здравоохранения?", "Технологии, вероятно, позволят создавать персонализированную медицину на основе геномики, носимые устройства для мониторинга состояния здоровья и планирование лечения с помощью ИИ, что приведет к более целенаправленному и эффективному здравоохранение.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии изменили сельское хозяйство с точки зрения устойчивых методов?", "Технологии привели к появлению методов точного земледелия, датчиков почвы и удаленного мониторинга, позволяющих оптимизировать управление ресурсами и снизить воздействие сельского хозяйства на окружающую среду.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии улучшили доступность для людей с нарушениями зрения?", "Технологии привели к появлению программ чтения с экрана, программного обеспечения для преобразования текста в речь и вспомогательных устройств, таких как дисплеи Брайля, которые повышают доступность и независимость людей с нарушениями зрения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие достижения вы надеетесь увидеть в области квантовой криптографии и безопасной связи?", "Я надеюсь увидеть достижения в распределении квантовых ключей, квантово-устойчивых алгоритмах шифрования и безопасных сетях квантовой связи для обеспечения надежной конфиденциальности и безопасности данных .");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как технологии повлияли на область архитектуры с точки зрения устойчивого проектирования и экологичных зданий?", "Технологии упростили моделирование энергопотребления, экологичные строительные материалы и передовые инструменты моделирования для оптимизации энергоэффективности и устойчивости в архитектурном дизайне.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как вы думаете, какую роль технологии будут играть в будущем транспорта, работающего на возобновляемых источниках энергии?", "Технологии, вероятно, будут способствовать развитию электромобилей, инфраструктуры зарядки и систем общественного транспорта, работающих на возобновляемых источниках энергии, продвигая устойчивые варианты транспорта .");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что вы думаете о влиянии технологий на неприкосновенность частной жизни и защиту данных?", "Технологии вызвали обеспокоенность по поводу утечки данных, слежки и ответственного использования личных данных, что подчеркивает необходимость надежных мер по обеспечению конфиденциальности и защиты данных .");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое программирование?", "Программирование — это процесс создания инструкций или кода, который компьютер может понять и выполнить для выполнения определенных задач.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое языки программирования?", "Языки программирования — это формальные языки, используемые для написания компьютерных программ. Они предоставляют набор правил и синтаксис для написания кода.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между компилируемыми и интерпретируемыми языками программирования?", "Компилируемые языки перед выполнением преобразуются в машинный код, а интерпретируемые языки выполняются напрямую без предварительной компиляции.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое алгоритм?", "Алгоритм — это пошаговая процедура или набор правил для решения конкретной проблемы или выполнения конкретной задачи.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между функцией и методом в программировании?", "Функция — это автономный блок кода, выполняющий определенную задачу, а метод — это функция, связанная с классом или объектом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между переменной и константой?", "Переменная — это именованное место хранения, которое может содержать значение, а константа — это значение, которое нельзя изменить после присвоения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое объектно-ориентированное программирование (ООП)?", "Объектно-ориентированное программирование — это парадигма программирования, которая организует код вокруг объектов, которые являются экземплярами классов, содержащих данные и поведение.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое отладка?", "Отладка — это процесс выявления и исправления ошибок или ошибок в программе, чтобы убедиться, что она ведет себя должным образом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое контроль версий?", "Контроль версий — это система, которая отслеживает изменения в файлах с течением времени, позволяя нескольким людям совместно работать над проектом и легко управлять изменениями кода.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое API?", "API (интерфейс прикладного программирования) — это набор правил и протоколов, которые позволяют различным программным приложениям общаться и взаимодействовать друг с другом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое рекурсия?", "Рекурсия — это метод программирования, при котором функция вызывает себя для решения проблемы, разбивая ее на более мелкие подзадачи.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель цикла в программировании?", "Цикл позволяет вам повторять набор инструкций несколько раз, либо для определенного количества итераций, либо до тех пор, пока не будет выполнено определенное условие.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между статической и динамической типизацией?", "Статическая типизация требует объявления типов переменных во время компиляции, в то время как динамическая типизация позволяет определять типы переменных во время выполнения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль компилятора в программировании?", "Компилятор переводит исходный код, написанный на языке программирования, в машинный код или представление более низкого уровня, которое может быть выполнено компьютером.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое база данных?", "База данных — это организованный набор данных, к которому можно легко получить доступ, управлять и обновлять. Он хранит информацию в структурированных форматах.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое SQL?", "SQL (язык структурированных запросов) — это язык программирования, используемый для управления реляционными базами данных и управления ими.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между списком и массивом?", "В программировании список — это динамическая структура данных, которая может увеличиваться или уменьшаться, а массив имеет фиксированный размер.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель обработки исключений в программировании?", "Обработка исключений позволяет перехватывать и обрабатывать ошибки или исключительные ситуации, которые могут возникнуть во время выполнения программы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между процедурным и объектно-ориентированным программированием?", "Процедурное программирование фокусируется на написании процедур или функций для выполнения задач, в то время как объектно-ориентированное программирование делает упор на использование объектов и их взаимодействие.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль конструктора в объектно-ориентированном программировании?", "Конструктор — это специальный метод, используемый для инициализации объектов при их создании. Он устанавливает начальное состояние объекта.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между фреймворком и библиотекой?", "Фреймворк предоставляет основу и набор многократно используемых компонентов для создания приложений, а библиотека предлагает предварительно написанный код для определенных функций.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель тестирования при разработке программного обеспечения?", "Тестирование помогает убедиться, что программное обеспечение ведет себя так, как ожидается, выявляет и исправляет ошибки и проверяет, соответствует ли оно требованиям.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль менеджера пакетов в программировании?", "Менеджер пакетов — это инструмент, который автоматизирует процесс установки, обновления и управления зависимостями для программных библиотек и фреймворков.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция модульности в программировании?", "Модульность — это практика разбиения программы на более мелкие, автономные модули или компоненты, что делает код более организованным и удобным в сопровождении.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между текстовым редактором и интегрированной средой разработки (IDE)?", "Текстовый редактор — это базовый инструмент для написания кода, тогда как IDE предоставляет дополнительные функции, такие как отладка, завершение кода и управление проектами.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль документации в программировании?", "Документация предоставляет информацию о том, как использовать и понимать код, облегчая разработчикам работу с программным обеспечением и его сопровождение.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова концепция полиморфизма в объектно-ориентированном программировании?,", "Полиморфизм позволяет рассматривать объекты разных классов как объекты общего суперкласса, позволяя писать код, который работает с объектами, не зная их конкретных типов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между стеком и очередью?", "Стек — это структура данных, которая следует принципу «последним пришел — первым обслужен» (LIFO), в то время как очередь следует принципу «первый пришел — первый обслужен» ( принцип FIFO).");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль комментариев в программировании?", "Комментарии используются для добавления пояснений или аннотаций в код, чтобы улучшить его читабельность и облегчить понимание другими разработчиками.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова концепция наследования в объектно-ориентированном программировании?", "Наследование позволяет классу наследовать свойства и поведение другого класса, способствуя повторному использованию кода и создавая иерархические отношения между классами.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель рефакторинга кода?", "Рефакторинг кода включает в себя реструктуризацию существующего кода для улучшения его удобочитаемости, удобства обслуживания и эффективности без изменения его внешнего поведения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое шаблон проектирования в разработке программного обеспечения?", "Шаблон проектирования — это многократно используемое решение общей проблемы в разработке программного обеспечения. Он обеспечивает проверенный подход к решению конкретных задач проектирования.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между синхронным и асинхронным программированием?", "Синхронное программирование ожидает завершения задачи, прежде чем двигаться дальше, в то время как асинхронное программирование позволяет выполнять задачи одновременно, не блокируя поток программы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль системы управления базами данных (СУБД) в программировании?", "СУБД — это программное обеспечение, позволяющее создавать базы данных, управлять ими и запрашивать их, обеспечивая интерфейс между приложениями и хранилищем данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова концепция инкапсуляции в объектно-ориентированном программировании?", "Инкапсуляция — это практика объединения данных и методов внутри класса, скрытия внутренних деталей и предоставления контролируемого доступа к состоянию объекта.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Каково назначение структур данных в программировании?", "Структуры данных используются для эффективной организации и хранения данных, что позволяет выполнять такие операции, как вставка, удаление и извлечение.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между серверным и клиентским языками программирования?", "Серверные языки работают на сервере и генерируют динамический контент для клиента, в то время как клиентские языки работают в браузере и взаимодействуют с Пользователь.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль рекурсии в программировании?", "Рекурсия позволяет функции вызывать саму себя, обеспечивая элегантное решение проблем, которые можно разбить на более мелкие, похожие подзадачи.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция абстракции в программировании?", "Абстракция включает в себя упрощение сложных систем за счет предоставления упрощенного интерфейса или представления, сокрытия ненужных деталей и сосредоточения внимания на основных функциях.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между битом и байтом в компьютерном программировании?", "Бит — это наименьшая единица данных, которая может представлять либо 0, либо 1, а байт состоит из 8 бит и может представлять больший диапазон ценностей.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель параллелизма в программировании?", "Параллелизм позволяет выполнять несколько задач или процессов одновременно, повышая производительность и скорость отклика программных приложений.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль сервера приложений в веб-разработке?", "Сервер приложений обеспечивает среду для развертывания и запуска веб-приложений, обработки запросов и управления ресурсами.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция композиции в объектно-ориентированном программировании?", "Композиция позволяет составлять объекты из других объектов как компонентов или частей, создавая сложные структуры и позволяя повторно использовать код.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между процедурным и функциональным программированием?", "Процедурное программирование фокусируется на пошаговых процедурах, в то время как функциональное программирование делает упор на вычисление математических функций и избегает изменяемых данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель проверки данных в программировании?", "Проверка данных гарантирует, что входные данные соответствуют определенным критериям или ограничениям, предотвращая ошибки и обеспечивая надежность программы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль кэширования в веб-разработке?", "Кэширование включает в себя хранение часто используемых данных или результатов для повышения производительности и уменьшения потребности в дорогостоящих вычислениях или запросах к базе данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция абстракции в объектно-ориентированном программировании?", "Абстракция включает в себя упрощение сложных систем за счет предоставления упрощенного интерфейса или представления, сокрытия ненужных деталей и сосредоточения внимания на основных функциях.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между битом и байтом в компьютерном программировании?", "Бит — это наименьшая единица данных, которая может представлять либо 0, либо 1, а байт состоит из 8 бит и может представлять больший диапазон ценностей.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль сервера приложений в веб-разработке?", "Сервер приложений обеспечивает среду для развертывания и запуска веб-приложений, обработки запросов и управления ресурсами.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель параллелизма в программировании?", "Параллелизм позволяет выполнять несколько задач или процессов одновременно, повышая производительность и скорость отклика программных приложений.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция композиции в объектно-ориентированном программировании?", "Композиция позволяет составлять объекты из других объектов как компонентов или частей, создавая сложные структуры и позволяя повторно использовать код.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между процедурным и функциональным программированием?", "Процедурное программирование фокусируется на пошаговых процедурах, в то время как функциональное программирование делает упор на вычисление математических функций и избегает изменяемых данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель проверки данных в программировании?", "Проверка данных гарантирует, что входные данные соответствуют определенным критериям или ограничениям, предотвращая ошибки и обеспечивая надежность программы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль кэширования в веб-разработке?", "Кэширование включает в себя хранение часто используемых данных или результатов для повышения производительности и уменьшения потребности в дорогостоящих вычислениях или запросах к базе данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция рекурсии в программировании?", "Рекурсия — это метод программирования, при котором функция вызывает себя для решения проблемы, разбивая ее на более мелкие, похожие подзадачи.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль IDE в программировании?", "IDE (интегрированная среда разработки) предоставляет полный набор инструментов и функций для облегчения разработки программного обеспечения, включая редактирование кода, отладку и управление проектами.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между процедурным и декларативным программированием?", "Процедурное программирование фокусируется на указании шагов и процедур для решения проблемы, в то время как декларативное программирование фокусируется на указании того, чего должно достичь решение, без указания точных шагов для его достижения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель документации по программному обеспечению?", "Документация по программному обеспечению предоставляет информацию о дизайне, функциональности и использовании программного обеспечения, помогая разработчикам понимать, поддерживать и совместно работать над кодом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем заключается концепция динамического программирования?", "Динамическое программирование — это метод, используемый для решения сложных задач путем их разбиения на перекрывающиеся подзадачи и сохранения решений подзадач для повторного использования.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль сериализации данных в программировании?", "Сериализация данных — это процесс преобразования сложных структур данных или объектов в формат, который можно хранить или передавать, облегчая хранение данных, совместное использование и связь.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между статическим и динамическим веб-сайтом?", "Статический веб-сайт состоит из фиксированного контента, который не меняется, если его не обновлять вручную, в то время как динамический веб-сайт создает контент динамически на основе взаимодействия с пользователем или ввода данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова концепция программирования, управляемого событиями?", "Программирование, управляемое событиями, вращается вокруг событий или действий пользователя, которые запускают определенные функции или действия в программе, что позволяет создавать интерактивное и отзывчивое программное обеспечение.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль интерпретатора в программировании?", "Интерпретатор — это программа, которая читает и выполняет исходный код напрямую, переводя его в машинный код или байт-код на лету без предварительной компиляции.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между процедурной и объектно-ориентированной базой данных?", "Процедурная база данных использует процедурные языки для запроса и обработки данных, в то время как объектно-ориентированная база данных хранит данные в объектах и позволяет выполнять сложное моделирование данных и отношения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель шифрования данных в программировании?", "Шифрование данных обеспечивает конфиденциальность и целостность конфиденциальных данных путем преобразования их в нечитаемый формат, который можно расшифровать только с помощью соответствующего ключа дешифрования.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова концепция функционального программирования?", "Функциональное программирование — это парадигма программирования, которая рассматривает вычисления как оценку математических функций и подчеркивает неизменность и отсутствие побочных эффектов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль менеджера пакетов в программировании?", "Менеджер пакетов — это инструмент, упрощающий установку, управление и обновление внешних библиотек, фреймворков и зависимостей в программном проекте.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между веб-приложением и настольным приложением?", "Веб-приложение запускается в веб-браузере и доступно через Интернет, тогда как настольное приложение устанавливается и запускается непосредственно на компьютере пользователя.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель тестирования программного обеспечения?", "Тестирование программного обеспечения направлено на выявление дефектов, ошибок или несоответствий в программном обеспечении, чтобы гарантировать его функциональность, надежность и качество, прежде чем оно будет развернуто для пользователей.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова концепция функционального программирования?", "Функциональное программирование — это парадигма программирования, которая рассматривает вычисления как оценку математических функций и подчеркивает неизменность и отсутствие побочных эффектов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль менеджера пакетов в программировании?", "Менеджер пакетов — это инструмент, упрощающий установку, управление и обновление внешних библиотек, фреймворков и зависимостей в программном проекте.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между веб-приложением и настольным приложением?", "Веб-приложение запускается в веб-браузере и доступно через Интернет, тогда как настольное приложение устанавливается и запускается непосредственно на компьютере пользователя.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель тестирования программного обеспечения?", "Тестирование программного обеспечения направлено на выявление дефектов, ошибок или несоответствий в программном обеспечении, чтобы гарантировать его функциональность, надежность и качество, прежде чем оно будет развернуто для пользователей.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем заключается концепция модульного тестирования?", "Модульное тестирование включает изолированное тестирование отдельных модулей или компонентов программного обеспечения, чтобы убедиться, что они ведут себя должным образом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между ошибкой и функцией в разработке программного обеспечения?", "Ошибка — это непреднамеренная ошибка или недостаток в программном обеспечении, а функция — это запланированная и преднамеренная функциональность, предназначенная для удовлетворения конкретных требований.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль API в веб-разработке?", "API (интерфейс прикладного программирования) позволяет различным программным приложениям или службам взаимодействовать и взаимодействовать друг с другом, обеспечивая обмен данными и функциями.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель бенчмаркинга в программировании?", "Бенчмаркинг включает в себя измерение и сравнение производительности программных или аппаратных компонентов для выявления узких мест, оптимизации кода и повышения общей эффективности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем заключается концепция контроля версий в программировании?", "Системы контроля версий отслеживают и управляют изменениями в файлах и коде с течением времени, позволяя нескольким разработчикам сотрудничать, отменять изменения и вести историю модификаций.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль языка запросов к базе данных, такого как SQL?", "SQL (язык структурированных запросов) позволяет пользователям запрашивать, манипулировать и управлять данными, хранящимися в реляционных базах данных, обеспечивая стандартизированный способ взаимодействия с базами данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между процедурными и декларативными языками запросов?", "Процедурные языки запросов определяют, как извлекать данные шаг за шагом, в то время как декларативные языки запросов указывают, какие данные нужно получить, не указывая точных шагов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель оптимизации кода в программировании?", "Оптимизация кода направлена на повышение эффективности и производительности программного обеспечения за счет сокращения времени выполнения, использования памяти или других ресурсов при сохранении правильной функциональности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция многопоточности в программировании?", "Многопоточность позволяет одновременно запускать несколько потоков или путей выполнения в одной программе, обеспечивая параллелизм и повышая производительность.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль нормализации данных в структуре базы данных?", "Нормализация данных — это процесс организации данных в базе данных для устранения избыточности, улучшения целостности данных и оптимизации хранения и поиска данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между компилируемым и интерпретируемым языком программирования?", "Компилируемый язык транслируется в машинный код перед выполнением, тогда как интерпретируемый язык выполняется напрямую без предварительной компиляции.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель программной среды в программировании?", "Программная среда обеспечивает повторно используемую основу и набор инструментов и библиотек, которые упрощают разработку программных приложений, обеспечивая структуру и предоставляя общие функции.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль структур данных в разработке алгоритмов?", "Структуры данных обеспечивают эффективные способы хранения и организации данных, позволяя разрабатывать и реализовывать алгоритмы, которые могут эффективно обрабатывать данные и манипулировать ими.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция сборки мусора в программировании?", "Сборка мусора — это метод автоматического управления памятью, который освобождает память, занятую объектами, на которые больше нет ссылок или которые больше не используются, предотвращая утечки памяти и ручное управление памятью.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова концепция архитектуры, управляемой событиями?", "Архитектура, управляемая событиями, фокусируется на производстве, обнаружении и потреблении событий, позволяя различным компонентам или службам асинхронно взаимодействовать и реагировать на события.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель обработки ошибок в программировании?", "Обработка ошибок включает в себя предвидение и реагирование на ошибки, исключения или непредвиденные ситуации в программном обеспечении для предотвращения сбоев, корректной обработки сбоев и обеспечения надежности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между интерпретируемой и JIT-компиляцией?", "Интерпретируемые языки выполняют исходный код напрямую, а JIT-компиляция включает динамическую трансляцию кода в машинный код во время выполнения для повышения производительности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль виртуальной машины в программировании?", "Виртуальная машина обеспечивает изолированную среду выполнения, которая эмулирует компьютерную систему, позволяя программам работать на разных платформах или операционных системах.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между монолитной архитектурой и архитектурой микросервисов?", "Монолитная архитектура состоит из одного большого приложения, а архитектура микросервисов разбивает приложение на более мелкие, слабо связанные службы, которые взаимодействуют друг с другом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель профилирования производительности в программировании?", "Профилирование производительности включает измерение и анализ поведения во время выполнения и характеристик производительности программного обеспечения для выявления узких мест, оптимизации кода и повышения общей эффективности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова концепция внедрения зависимостей в разработке программного обеспечения?", "Внедрение зависимостей — это шаблон проектирования, который позволяет объектам передавать свои зависимости или требуемые службы из внешних источников, способствуя слабой связи и тестируемости.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль балансировщика нагрузки в веб-разработке?", "Балансировщик нагрузки распределяет входящий сетевой трафик между несколькими серверами или ресурсами, обеспечивая высокую доступность, масштабируемость и улучшенную производительность веб-приложений.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между языком сценариев и компилируемым языком?", "Языки сценариев интерпретируются или выполняются напрямую без предварительной компиляции, тогда как компилируемые языки перед выполнением транслируются в машинный код.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель документации по коду в программировании?", "Документация по коду содержит пояснения, инструкции по использованию и примеры, помогающие разработчикам понимать и эффективно использовать код, способствуя удобочитаемости, ремонтопригодности и совместной работе.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова концепция аспектно-ориентированного программирования?", "Аспектно-ориентированное программирование фокусируется на модуляции сквозных задач или функций, что позволяет разработчикам разделять различные аспекты кода и управлять ими независимо.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль средства запуска задач или сборки в программировании?", "Средство запуска задач или средство сборки автоматизирует повторяющиеся задачи, такие как компиляция, тестирование и развертывание, обеспечивая оптимизированный рабочий процесс и гарантируя согласованность сборок.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между первичным ключом и внешним ключом в структуре базы данных?", "Первичный ключ — это уникальный идентификатор записи в таблице, а внешний ключ устанавливает связь между двумя таблицами, ссылаясь на первичный ключ другого стола.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция непрерывной интеграции в разработке программного обеспечения?", "Непрерывная интеграция включает в себя частую интеграцию изменений кода от нескольких разработчиков в общий репозиторий, что обеспечивает раннее обнаружение ошибок, совместную работу и автоматизированное тестирование.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель шаблона проектирования программного обеспечения?", "Шаблоны проектирования программного обеспечения предоставляют проверенные решения часто возникающих проблем проектирования при разработке программного обеспечения, способствуя повторному использованию кода, гибкости и ремонтопригодности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль транзакции в управлении базой данных?", "Транзакция обеспечивает атомарность, согласованность, изоляцию и устойчивость (ACID) операций базы данных, позволяя группировать несколько операций вместе как единую единицу работы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между поверхностной и глубокой копией в программировании?", "Поверхностная копия создает новый объект, который ссылается на те же данные, что и оригинал, а глубокая копия создает новый объект с собственной независимой копией данные.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция непрерывного развертывания в разработке программного обеспечения?", "Непрерывное развертывание предполагает автоматический выпуск изменений программного обеспечения в производственные среды после прохождения серии автоматических тестов, что обеспечивает быстрые и частые выпуски.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель инфраструктуры объектно-реляционного отображения (ORM)?", "Структура ORM позволяет разработчикам взаимодействовать с базами данных, используя концепции объектно-ориентированного программирования, отображая объекты в таблицы базы данных и упрощая операции с базой данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль проверки кода в разработке программного обеспечения?", "Проверки кода включают систематическую проверку кода на наличие ошибок, ошибок, соблюдение стандартов кодирования и общее качество для улучшения качества кода, удобства обслуживания и совместной работы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между интерпретатором и компилятором?", "Интерпретатор выполняет код напрямую, транслируя и выполняя его построчно, в то время как компилятор переводит код в машинный код или байт-код перед выполнением.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель рефакторинга программного обеспечения?", "Рефакторинг программного обеспечения включает в себя реструктуризацию существующего кода без изменения его внешнего поведения для улучшения его удобочитаемости, ремонтопригодности и производительности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль системы отслеживания ошибок в разработке программного обеспечения?", "Система отслеживания ошибок помогает отслеживать, управлять и расставлять приоритеты по сообщениям о проблемах или ошибках программного обеспечения, облегчая их решение и обеспечивая эффективное управление ошибками.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова концепция принципа единой ответственности в разработке программного обеспечения?", "Принцип единой ответственности гласит, что у класса или модуля должна быть только одна причина для изменения, что способствует связности и модульности в разработке программного обеспечения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль архитектора программного обеспечения в процессе разработки?", "Архитектор программного обеспечения отвечает за проектирование и надзор за общей структурой, компонентами и взаимодействием программной системы, обеспечивая соответствие бизнес-требованиям и техническим ограничениям.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между абстрактным классом и интерфейсом в объектно-ориентированном программировании?", "Абстрактный класс может предоставлять частичную реализацию и может быть подклассом, в то время как интерфейс определяет контракт методов, которые должен реализовать класс.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель репозитория кода в разработке программного обеспечения?", "Репозиторий кода хранит версии исходного кода и управляет ими, обеспечивая совместную работу, контроль версий и совместное использование кода разработчиками.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль стандарта кодирования в разработке программного обеспечения?", "Стандарт кодирования определяет набор рекомендаций, соглашений и передовых методов написания кода, обеспечивающих согласованность, удобочитаемость и удобство сопровождения кодовой базы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель нагрузочного тестирования при разработке программного обеспечения?", "Нагрузочное тестирование включает в себя воздействие на систему больших объемов смоделированного пользовательского трафика для измерения ее производительности, стабильности и поведения при ожидаемых или пиковых нагрузках.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль среды пользовательского интерфейса (UI) в разработке программного обеспечения?", "Платформа пользовательского интерфейса предоставляет набор повторно используемых компонентов, инструментов и функций для проектирования и разработки пользовательских интерфейсов в программных приложениях.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель профилирования кода в программировании?", "Профилирование кода включает анализ поведения программного обеспечения во время выполнения и характеристик производительности для выявления узких мест в производительности, оптимизации кода и повышения эффективности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция модульности кода в программировании?", "Модульность кода включает в себя организацию кода в отдельные автономные модули или единицы, обеспечивающие возможность повторного использования, ремонтопригодность и простоту понимания.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между статической типизацией и динамической типизацией в языках программирования?", "В статической типизации переменные явно объявляются с их типами, а проверка типов выполняется во время компиляции, в то время как в динамической типизации переменные явно не объявляются и проверки типов выполняются во время выполнения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль кеша в разработке программного обеспечения?", "Кэш хранит часто используемые или дорогостоящие для вычислений данные в быстродоступном хранилище, повышая производительность и уменьшая потребность в повторных вычислениях.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель регрессионного тестирования в разработке программного обеспечения?", "Регрессионное тестирование проверяет, что изменения или исправления в программном обеспечении не приводят к новым ошибкам или регрессиям, гарантируя, что ранее работавшая функциональность остается неизменной.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция инверсии управления в разработке программного обеспечения?", "Инверсия управления позволяет инвертировать управление потоком приложения, при этом компоненты или платформы управляют потоком выполнения, зависимостями и жизненным циклом объектов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова концепция разделения задач в программной инженерии?", "Разделение задач включает в себя разделение программной системы на отдельные разделы или модули, каждый из которых решает конкретную задачу или функциональность, обеспечивая удобство сопровождения и возможность повторного использования.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель жизненного цикла разработки программного обеспечения (SDLC)?", "Жизненный цикл разработки программного обеспечения определяет набор фаз, процессов и действий по созданию программного обеспечения, обеспечивая структурированный подход к разработке и обслуживанию программного обеспечения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между фреймворком и библиотекой в разработке программного обеспечения?", "Фреймворк предоставляет полную структуру, набор инструментов и рекомендации по созданию приложений, в то время как библиотека предоставляет многократно используемые функции или компоненты для использования в приложении. .");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова концепция проектирования, ориентированного на предметную область (DDD) в разработке программного обеспечения?", "Проектирование, ориентированное на предметную область, делает упор на сотрудничество и согласованность между экспертами в предметной области и разработчиками программного обеспечения, уделяя особое внимание пониманию и моделированию основной бизнес-области в программных системах.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель проверки данных при разработке программного обеспечения?", "Проверка данных гарантирует, что вводимые или обработанные данные соответствуют определенным критериям, таким как тип данных, диапазон или формат, предотвращая появление ошибок или проблем в работе неверных или противоречивых данных. система.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль системы управления конфигурацией в разработке программного обеспечения?", "Система управления конфигурацией помогает управлять и отслеживать изменения в конфигурациях программного обеспечения, средах и зависимостях, обеспечивая согласованность и воспроизводимость.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между тестированием белого ящика и тестированием черного ящика?", "Тестирование белого ящика включает тестирование внутренних структур или деталей реализации программного обеспечения, в то время как тестирование черного ящика фокусируется на внешнем поведении и функциональности без учета внутренней реализации .");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция повторного использования программного обеспечения в программировании?", "Повторное использование программного обеспечения означает возможность повторного использования существующего кода или компонентов при разработке нового программного обеспечения, повышая производительность, согласованность и сокращая время разработки.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль балансировщика нагрузки в распределенных системах?", "Балансировщик нагрузки распределяет входящий сетевой трафик между несколькими серверами или ресурсами в распределенной системе, обеспечивая масштабируемость, доступность и оптимальное использование ресурсов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль статического анализатора в программировании?", "Статический анализатор анализирует исходный код, не выполняя его, обнаруживая потенциальные проблемы, запахи кода или нарушения стандартов кодирования и предлагая предложения по улучшению.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между веб-приложением и настольным приложением?", "Веб-приложение запускается на веб-сервере и доступно через веб-браузер, в то время как настольное приложение запускается локально на компьютере пользователя и обычно устанавливается и запускается в операционной системе.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция шаблона проектирования в разработке программного обеспечения?", "Шаблон проектирования предоставляет повторно используемое решение часто встречающейся проблемы в разработке программного обеспечения, инкапсулируя передовой опыт и способствуя ремонтопригодности, гибкости и масштабируемости.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между платформой программного обеспечения и библиотекой программного обеспечения?", "Инфраструктура программного обеспечения обеспечивает повторно используемую основу, включая предопределенные структуры, функциональные возможности и инструменты, для создания приложений, в то время как библиотека программного обеспечения предоставляет определенные функции или компоненты, которые необходимо использовать. используется внутри приложения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель разработки прототипа программного обеспечения?", "Прототип программного обеспечения представляет собой раннюю упрощенную версию системы или функции для сбора отзывов, проверки требований и уточнения дизайна перед полномасштабной разработкой.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель анализа журналов при разработке программного обеспечения?", "Анализ журналов включает в себя изучение и извлечение ценной информации из файлов журналов, созданных программными приложениями или системами, что помогает в устранении неполадок, анализе производительности и обнаружении ошибок.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль менеджера по программному продукту в процессе разработки?", "Менеджер по программному продукту отвечает за определение и контроль стратегического видения, планирование и разработку программного продукта, согласование его с бизнес-целями и потребностями клиентов. .");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между конструктором и методом в объектно-ориентированном программировании?", "Конструктор — это специальный метод, используемый для инициализации объектов при их создании, а метод — это функция, связанная с объектом или классом, которая выполняет конкретное действие или расчет.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем заключается концепция непрерывного мониторинга при разработке программного обеспечения?", "Непрерывный мониторинг включает в себя сбор и анализ данных в режиме реального времени о программных системах для выявления проблем с производительностью, уязвимостей в системе безопасности и обеспечения соответствия эксплуатационным требованиям.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция повторного использования кода в программировании?", "Повторное использование кода включает разработку и реализацию кода, который можно легко повторно использовать в нескольких контекстах или проектах, уменьшая дублирование усилий и повышая эффективность.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель архитектурного шаблона в разработке программного обеспечения?", "Архитектурный шаблон обеспечивает высокоуровневую структуру и набор руководящих принципов для организации и проектирования программных систем, решения общих архитектурных проблем и обеспечения масштабируемости и удобства обслуживания.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль соглашения о кодировании в программировании?", "Соглашение о кодировании устанавливает набор рекомендаций и правил для форматирования, именования и организации кода, обеспечивая согласованность, удобочитаемость и удобство обслуживания в кодовой базе.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между интеграционным тестированием и модульным тестированием?", "Интеграционное тестирование проверяет взаимодействие и связь между несколькими компонентами или модулями, в то время как модульное тестирование фокусируется на изолированном тестировании отдельных модулей или компонентов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между монорепозиторием и мультирепозиторием в управлении версиями?", "Монорепозиторий хранит весь код и проекты в одном репозитории, а мультирепозиторий использует отдельные репозитории для разных кодовых баз или проектов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль тестировщика программного обеспечения в процессе разработки?", "Тестировщик программного обеспечения отвечает за планирование и выполнение тестов, проверку соответствия программного обеспечения требованиям, выявление дефектов и обеспечение общего качества и надежности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель контроля версий в разработке программного обеспечения?", "Контроль версий позволяет отслеживать и управлять изменениями в файлах и коде с течением времени, облегчая совместную работу, исправление ошибок и возможность вернуться к предыдущим версиям.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между стеком и очередью в структурах данных?", "Стек следует принципу «последним пришел — первым обслужен» (LIFO), а очередь — принципу «первым пришел — первым обслужен» (FIFO). Принцип добавления и удаления элементов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель анализа проекта при разработке программного обеспечения?", "Анализ проекта оценивает проектные решения, архитектуру и спецификации программной системы, выявляя потенциальные проблемы, риски и области для улучшения перед внедрением.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между индексом базы данных и представлением базы данных?", "Индекс базы данных повышает производительность извлечения и поиска данных в таблице базы данных, в то время как представление базы данных представляет собой виртуальную таблицу, которая представляет данные из одной или нескольких таблиц. настроенным или предопределенным образом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова концепция управления версиями кода в разработке программного обеспечения?", "Управление версиями кода включает в себя присвоение уникальных идентификаторов или тегов различным версиям исходного кода, облегчение отслеживания, сравнения и управления изменениями кода, а также возможность отката к предыдущим версиям.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между функциональным требованием и нефункциональным требованием при разработке программного обеспечения?", "Функциональное требование указывает, что должна делать программная система или какие функции она должна иметь, в то время как нефункциональное требование определяет качество, производительность или ограничения системы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между горизонтальным и вертикальным масштабированием в архитектуре системы?", "Горизонтальное масштабирование добавляет больше машин или узлов для распределения рабочей нагрузки между несколькими экземплярами, тогда как вертикальное масштабирование включает увеличение ресурсов (таких как ЦП или ОЗУ) одна машина для обработки более тяжелой нагрузки.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль проектной документации в разработке программного обеспечения?", "Проектная документация содержит подробное описание архитектуры, проектных решений и спецификаций программной системы, служащее справочным материалом для реализации и дальнейшего обслуживания.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль обработчика ошибок или обработчика исключений в программировании?", "Обработчик ошибок или обработчик исключений отвечает за захват и обработку ошибок, исключений или непредвиденных событий, возникающих во время выполнения программы, обеспечивая корректную обработку ошибок обращение и восстановление.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль инженера по обеспечению качества программного обеспечения (QA)?", "Инженер по обеспечению качества программного обеспечения отвечает за планирование и реализацию процессов, обеспечивающих соответствие программных продуктов стандартам качества, проведение тестов и выявление областей для улучшения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между первичным ключом и уникальным ключом в структуре базы данных?", "Первичный ключ — это уникальный идентификатор записи в таблице, который обычно используется для ссылки и установления отношений, в то время как уникальный ключ обеспечивает уникальность но не обязательно служит основным идентификатором.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция сборки мусора в языках программирования?", "Сборка мусора автоматически освобождает память, которая больше не используется программой, освобождая ресурсы и предотвращая утечки памяти и ошибки ручного управления памятью.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель функционального тестирования в разработке программного обеспечения?", "Функциональное тестирование проверяет, соответствует ли программная система заданным функциональным требованиям, гарантируя, что она ведет себя так, как ожидается, и обеспечивает предполагаемую функциональность.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель приемочного тестирования пользователей (UAT) при разработке программного обеспечения?", "Приемочное тестирование пользователей включает тестирование программной системы с реальными пользователями или представителями, чтобы убедиться, что она соответствует их требованиям, ожиданиям и потребностям в удобстве использования.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель распределенной системы контроля версий (DVCS)?", "Распределенная система контроля версий позволяет нескольким разработчикам одновременно работать над проектом, поддерживая полную копию репозитория на компьютере каждого разработчика и обеспечивая автономную работу и децентрализованное сотрудничество.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между синхронным и асинхронным программированием?", "В синхронном программировании операции выполняются последовательно и блокируются до завершения, в то время как в асинхронном программировании операции могут инициироваться и продолжаться в фоновом режиме, позволяя другим задачам выполняться одновременно.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль исправления ошибки в разработке программного обеспечения?", "Исправление ошибки устраняет проблемы или дефекты, обнаруженные в программном обеспечении, улучшая его функциональность, стабильность и общее качество.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между рекурсивной функцией и итеративным циклом?", "Рекурсивная функция вызывает сама себя, потенциально создавая цепочку рекурсивных вызовов, в то время как итеративный цикл повторяет блок кода до тех пор, пока не будет выполнено условие.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция покрытия кода при тестировании программного обеспечения?", "Покрытие кода измеряет степень, в которой исходный код программы выполняется тестами, помогая выявить непроверенные или непокрытые части кода.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель балансировки нагрузки в распределенных системах?", "Балансировка нагрузки равномерно распределяет рабочие нагрузки или запросы между несколькими ресурсами или серверами, оптимизируя использование ресурсов, обеспечивая высокую доступность и повышая производительность.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль менеджера по выпуску программного обеспечения?", "Менеджер по выпуску программного обеспечения отвечает за планирование, координацию и управление выпуском программных продуктов, обеспечивая плавное развертывание, контроль версий и соблюдение графиков выпуска.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль руководителя группы разработки программного обеспечения?", "Руководитель группы разработки программного обеспечения обеспечивает руководство и руководство командой разработчиков, наблюдая за выполнением проекта, координируя задачи и способствуя общению и совместной работе.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между переполнением стека и утечкой памяти в программировании?", "Переполнение стека происходит, когда стек вызовов исчерпывает свое доступное пространство, обычно из-за чрезмерной рекурсии, в то время как утечка памяти происходит, когда память выделена неправильно высвобождается, что приводит к необратимой потере памяти.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова концепция непрерывного совершенствования в разработке программного обеспечения?", "Непрерывное совершенствование направлено на регулярную оценку и совершенствование процессов, инструментов и методов разработки программного обеспечения для достижения более высокой эффективности, качества и удовлетворенности клиентов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова концепция разработки через тестирование (TDD) в разработке программного обеспечения?", "Разработка через тестирование включает в себя написание тестов перед написанием фактического кода, гарантируя, что код соответствует желаемому поведению и управляет процессом разработки.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель службы хостинга репозитория кода, такой как GitHub или GitLab?", "Служба хостинга репозитория кода предоставляет платформу для размещения и управления репозиториями контроля версий, облегчения совместной работы, совместного использования кода и поддержки рабочего процесса разработки программного обеспечения. .");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова концепция рефакторинга кода в разработке программного обеспечения?", "Рефакторинг кода включает в себя реструктуризацию и улучшение существующего кода без изменения его внешнего поведения, повышение удобочитаемости, ремонтопригодности и производительности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция состояния гонки в параллельном программировании?", "Состояние гонки возникает, когда результат программы зависит от относительного времени или порядка нескольких параллельных операций, что приводит к непредсказуемым или неправильным результатам.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между первичным ключом и внешним ключом в структуре базы данных?", "Первичный ключ — это уникальный идентификатор записи в таблице, а внешний ключ — это поле, которое устанавливает связь или отношение между двумя таблицы, ссылаясь на первичный ключ другой таблицы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль методологии разработки программного обеспечения, такой как Agile или Waterfall?", "Методология разработки программного обеспечения обеспечивает структуру, принципы и рекомендации для управления процессом разработки, организации задач и облегчения коммуникации, обеспечивая эффективность и предсказуемость поставка программного обеспечения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель шифрования данных в программных приложениях?", "Шифрование данных преобразует данные в закодированную форму для защиты от несанкционированного доступа или перехвата, обеспечивая конфиденциальность и безопасность данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция документации кода в разработке программного обеспечения?", "Документация кода включает описание и объяснение функциональности, назначения и использования кода с помощью комментариев, аннотаций и файлов документации, что способствует пониманию, обслуживанию и совместной работе.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель проверки кода при разработке программного обеспечения?", "Проверка кода включает в себя проверку и оценку качества кода, правильности, соответствия стандартам и потенциальных проблем, способствуя обмену знаниями, сотрудничеству и улучшению кодовой базы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое концепция непрерывной интеграции в разработке программного обеспечения?", "Непрерывная интеграция включает в себя частое слияние изменений кода от нескольких разработчиков в общий репозиторий, автоматическое создание и тестирование интегрированного кода для раннего обнаружения проблем интеграции.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль инженера по развертыванию программного обеспечения?", "Инженер по развертыванию программного обеспечения отвечает за планирование и выполнение процесса развертывания, обеспечивая бесперебойную установку, настройку и выпуск программных приложений в различных средах.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между веб-сервером и сервером приложений?", "Веб-сервер обрабатывает HTTP-запросы и обслуживает статический веб-контент, в то время как сервер приложений выполняет логику приложения и поддерживает генерацию динамического контента, часто в сочетании с веб-сервером .");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель документа спецификации требований к программному обеспечению?", "Документ спецификации требований к программному обеспечению фиксирует и описывает функциональные и нефункциональные требования программной системы, служащие в качестве справочного материала для проектирования, разработки и тестирования.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова концепция непрерывного развертывания в разработке программного обеспечения?", "Непрерывное развертывание автоматизирует процесс выпуска и развертывания, позволяя часто и быстро доставлять изменения программного обеспечения в производственные среды, как правило, после прохождения автоматизированных тестов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель дизайна пользовательского интерфейса (UI) в разработке программного обеспечения?", "Дизайн пользовательского интерфейса направлен на создание интуитивно понятных, визуально привлекательных и удобных интерфейсов для программных приложений, обеспечивающих положительный пользовательский опыт и эффективное взаимодействие.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Каковы преимущества использования облачных вычислений?", "Некоторые преимущества облачных вычислений включают масштабируемость, экономичность, доступность из любого места, где есть подключение к Интернету, автоматические обновления программного обеспечения и возможность использовать передовые технологии, такие как искусственный интеллект и большие аналитика данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между архитектором программного обеспечения и инженером-программистом?", "Архитектор программного обеспечения отвечает за проектирование общей структуры и компонентов программной системы, принятие решений высокого уровня, а инженер-программист занимается реализацией, кодирование и тестирование программного обеспечения на основе предоставленного дизайна и спецификаций.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль дизайнера пользовательского опыта (UX) в разработке программного обеспечения?", "Дизайнер пользовательского опыта фокусируется на создании положительного и значимого опыта для пользователей путем разработки интуитивно понятных интерфейсов, проведения исследований пользователей и оптимизации удобства использования и удовлетворенности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль инструмента управления конфигурацией программного обеспечения?", "Инструмент управления конфигурацией программного обеспечения помогает управлять и контролировать изменения в конфигурациях, версиях и зависимостях программного обеспечения, обеспечивая согласованность, прослеживаемость и воспроизводимость.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое ИТ?", "ИТ означает информационные технологии, которые охватывают использование, разработку и управление компьютерными системами, программным обеспечением и сетями для хранения, обработки и передачи информации.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль ИТ-отдела в организации?", "ИТ-отдел отвечает за управление и обслуживание технологической инфраструктуры организации, включая оборудование, программное обеспечение, сети и базы данных. Они обеспечивают техническую поддержку, обеспечивают безопасность данных и внедряют ИТ-решения для поддержки бизнес-операций.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль аналитика требований к программному обеспечению?", "Аналитик требований к программному обеспечению отвечает за сбор, анализ и документирование функциональных и нефункциональных требований к программной системе, обеспечивая четкое понимание и взаимодействие между заинтересованными сторонами и разработчиками. команда.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем заключается концепция интерфейса прикладного программирования (API)?", "Интерфейс прикладного программирования определяет набор правил и протоколов, позволяющих различным программным системам связываться и взаимодействовать друг с другом, обеспечивая интеграцию и взаимодействие.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое сервер в ИТ?", "В ИТ под сервером понимается компьютер или система, которая предоставляет услуги или ресурсы другим компьютерам или устройствам в сети. Серверы можно использовать для различных целей, таких как размещение веб-сайтов, хранение файлов или запуск приложений.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между программной структурой и программной платформой?", "Программная среда обеспечивает многократно используемую основу и структуру для создания приложений, в то время как программная платформа предоставляет среду, инструменты и службы для разработки и запуска приложений.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое кибербезопасность?", "Кибербезопасность относится к практике защиты компьютерных систем, сетей и данных от несанкционированного доступа, кражи, повреждения или разрушения. Это включает в себя реализацию мер безопасности, таких как брандмауэры, шифрование и аутентификация пользователей, для защиты цифровых активов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем заключается концепция объектно-ориентированного программирования (ООП)?", "Объектно-ориентированное программирование — это парадигма программирования, которая организует программное обеспечение вокруг объектов, представляющих объекты реального мира, инкапсулирующих данные и поведение и обеспечивающих модульность, повторное использование, и поддерживаемый код.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое база данных?", "База данных представляет собой организованный набор данных, хранящихся и доступных в электронном виде. Он обеспечивает структурированный способ хранения, извлечения и управления данными, что позволяет эффективно обрабатывать данные, выполнять поиск и анализ.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель регрессионного тестирования при разработке программного обеспечения?", "Регрессионное тестирование проверяет правильность функционирования ранее разработанного и протестированного программного обеспечения после внесения изменений или дополнений, гарантируя, что новые обновления не приведут к непреднамеренным побочным эффектам или проблемам.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между статическим веб-сайтом и динамическим веб-сайтом?", "Статический веб-сайт отображает один и тот же контент для всех пользователей и не часто меняется, в то время как динамический веб-сайт генерирует контент «на лету», часто персонализированный или интерактивный. , на основе пользовательских запросов или данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль менеджера программного проекта?", "Менеджер проекта программного обеспечения наблюдает за планированием, выполнением и доставкой программного проекта, координируя ресурсы, управляя рисками и обеспечивая достижение целей проекта вовремя и в рамках бюджета. .");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем заключается концепция непрерывного тестирования при разработке программного обеспечения?", "Непрерывное тестирование включает в себя автоматизированное тестирование на протяжении всего жизненного цикла разработки программного обеспечения, обеспечивая быструю обратную связь о качестве, производительности и надежности программного обеспечения и гарантируя раннее обнаружение ошибок.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль менеджера ИТ-проекта?", "Менеджер ИТ-проекта отвечает за планирование, организацию и надзор за выполнением ИТ-проектов в организации. Они управляют ресурсами, сроками и бюджетами и обеспечивают успешную реализацию проектов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова цель плана обслуживания программного обеспечения при разработке программного обеспечения?", "План обслуживания программного обеспечения описывает стратегии, процедуры и действия по управлению и обслуживанию программного обеспечения после его первоначальной разработки, включая исправление ошибок, усовершенствования и поддержку.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое Интернет вещей (IoT)?", "Интернет вещей относится к сети взаимосвязанных устройств, датчиков и объектов, которые могут собирать и обмениваться данными через Интернет. Это позволяет устройствам общаться и взаимодействовать друг с другом, что приводит к повышению уровня автоматизации и принятию решений на основе данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое виртуальная реальность (VR)?", "Виртуальная реальность — это технология, использующая компьютерное моделирование для создания моделируемой среды, с которой пользователи могут взаимодействовать. Обычно это связано с ношением гарнитуры, обеспечивающей трехмерное погружение.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое аналитика данных?", "Аналитика данных включает в себя процесс изучения больших наборов данных для выявления закономерностей, тенденций и идей, которые могут помочь в принятии решений и способствовать улучшению бизнеса. Он часто включает использование методов статистического анализа, интеллектуального анализа данных и машинного обучения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между разработчиком программного обеспечения и инженером-программистом?", "Термины «разработчик программного обеспечения» и «инженер-программист» часто используются взаимозаменяемо, но некоторые рассматривают программную инженерию как более широкую дисциплину, охватывающую разработку программного обеспечения. Инженеры-программисты могут быть вовлечены в проектирование и архитектурные решения более высокого уровня, в то время как разработчики программного обеспечения больше внимания уделяют кодированию и реализации.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое сеть?", "Сеть — это набор взаимосвязанных устройств, таких как компьютеры, серверы, коммутаторы и маршрутизаторы, которые могут взаимодействовать и совместно использовать ресурсы. Сети могут быть локальными сетями (LAN) в пределах ограниченной области или глобальными сетями (WAN), которые охватывают большие географические расстояния.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое искусственный интеллект (ИИ)?", "Искусственный интеллект — это отрасль информатики, которая фокусируется на создании интеллектуальных машин, способных выполнять задачи, для которых обычно требуется человеческий интеллект. Это включает в себя такие действия, как обработка естественного языка, распознавание изображений и решение проблем.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое программное обеспечение с открытым исходным кодом?", "Программное обеспечение с открытым исходным кодом — это программное обеспечение, которое находится в свободном доступе и позволяет пользователям просматривать, изменять и распространять его исходный код. Он способствует сотрудничеству, прозрачности и развитию сообщества, при этом многие популярные программные приложения и платформы имеют открытый исходный код.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое гибкая методология в управлении ИТ-проектами?", "Гибкая методология — это итеративный и гибкий подход к управлению проектами, который делает упор на сотрудничество, адаптивность и обратную связь с клиентами. Он включает в себя разбиение проектов на более мелкие этапы, называемые спринтами, что позволяет часто проверять, адаптировать и доставлять работающее программное обеспечение.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие распространенные языки программирования используются в ИТ?", "Некоторые распространенные языки программирования, используемые в ИТ, включают Python, Java, C++, JavaScript и Ruby. Каждый язык имеет свои сильные стороны и используется для разных целей, таких как веб-разработка, анализ данных или разработка мобильных приложений.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между синхронной и асинхронной связью в программных системах?", "Синхронная связь требует немедленных ответов и взаимодействий между компонентами или системами, в то время как асинхронная связь допускает отложенные ответы и неблокирующие взаимодействия.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое компьютерный вирус?", "Компьютерный вирус — это тип вредоносного программного обеспечения, которое может воспроизводить себя и распространяться на другие компьютеры или системы. Вирусы могут повредить файлы, нарушить работу системы или похитить конфиденциальную информацию. Антивирусное программное обеспечение обычно используется для обнаружения и удаления вирусов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое конфиденциальность данных?", "Конфиденциальность данных относится к защите личной и конфиденциальной информации от несанкционированного доступа, использования или раскрытия. Это включает в себя реализацию мер безопасности и соблюдение правил и лучших практик для обеспечения конфиденциальности личных данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое центр обработки данных?", "Центр обработки данных — это объект, используемый для размещения компьютерных систем, серверов и сетевого оборудования. Он обеспечивает контролируемую среду с питанием, охлаждением и мерами безопасности для обеспечения надежной работы ИТ-инфраструктуры.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое интеллектуальный анализ данных?", "Интеллектуальный анализ данных — это процесс обнаружения закономерностей, взаимосвязей и выводов из больших наборов данных. Он включает в себя извлечение и анализ данных с использованием различных методов, таких как кластеризация, классификация и правила ассоциации, для раскрытия ценной информации и принятия решений на основе данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое шифрование данных?", "Шифрование данных — это процесс преобразования данных в закодированную или зашифрованную форму для защиты от несанкционированного доступа. Алгоритмы и ключи шифрования используются для шифрования данных, что делает их нечитаемыми без соответствующего ключа дешифрования.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое резервное копирование данных?", "Резервное копирование данных — это процесс создания копий важных файлов и данных для защиты от потери, повреждения или случайного удаления. Резервные копии обычно хранятся в отдельных местах или на внешних устройствах, что позволяет восстанавливать данные в случае системного сбоя или аварии.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое хранилище данных?", "Хранилище данных включает сбор, хранение и управление большими объемами структурированных, а иногда и неструктурированных данных из различных источников. Хранилища данных обеспечивают централизованный репозиторий для анализа данных и отчетности, облегчая бизнес-аналитику и принятие решений.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль системного аналитика?", "Системный аналитик отвечает за анализ бизнес-процессов, систем и требований организации для предложения технологических решений. Они тесно сотрудничают с заинтересованными сторонами для сбора требований, проектирования системных архитектур и обеспечения эффективного использования технологий для достижения бизнес-целей.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое система управления контентом (CMS)?", "Система управления контентом — это программное приложение, которое позволяет пользователям создавать, редактировать, упорядочивать и публиковать цифровой контент, такой как веб-страницы, сообщения в блогах или статьи. CMS обеспечивают удобный интерфейс и оптимизируют процессы управления контентом.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое облачные вычисления?", "Облачные вычисления — это доставка вычислительных ресурсов, таких как хранилище, серверы, базы данных, программное обеспечение и приложения, через Интернет. Это позволяет организациям получать доступ к этим ресурсам и использовать их по требованию, без необходимости в физической инфраструктуре или предварительных инвестициях.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое программный патч?", "Программный патч — это небольшое обновление или исправление, выпущенное поставщиками программного обеспечения для устранения ошибок, уязвимостей в системе безопасности или улучшения функциональности. Патчи обычно применяются к существующим установкам программного обеспечения, чтобы обеспечить последнюю версию и сохранить целостность системы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое контроль версий в разработке программного обеспечения?", "Контроль версий — это управление изменениями в исходном коде и других файлах при разработке программного обеспечения. Это позволяет нескольким разработчикам совместно работать над проектом, отслеживать изменения и при необходимости возвращаться к предыдущим версиям. Популярные системы контроля версий включают Git и SVN.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем разница между аппаратным и программным обеспечением в ИТ?", "Аппаратное обеспечение относится к физическим компонентам компьютерной системы, таким как процессор, память, жесткий диск и периферийные устройства. Программное обеспечение, с другой стороны, представляет собой набор программ, приложений и данных, которые указывают аппаратному обеспечению, что делать.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль специалиста службы поддержки?", "Специалист службы поддержки предоставляет техническую поддержку и помощь пользователям компьютеров в организации. Они отвечают на запросы, устраняют неполадки и решают проблемы, а также предоставляют рекомендации по использованию оборудования, программного обеспечения и систем.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое доменное имя?", "Доменное имя — это уникальный адрес или идентификатор, который представляет веб-сайт в Интернете. Он является частью более крупной иерархической системы, называемой системой доменных имен (DNS), которая преобразует доменные имена в IP-адреса, позволяя пользователям получать доступ к веб-сайтам, используя удобочитаемые имена.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое брандмауэр?", "Брандмауэр — это устройство сетевой безопасности, которое действует как барьер между внутренней сетью и внешними сетями, такими как Интернет. Он отслеживает и контролирует входящий и исходящий сетевой трафик на основе заданных правил безопасности, помогая предотвратить несанкционированный доступ и злонамеренные действия.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое сжатие данных?", "Сжатие данных — это процесс уменьшения размера файлов или потоков данных для оптимизации эффективности хранения или передачи. Методы сжатия удаляют избыточные или ненужные данные, в результате чего получаются сжатые файлы, которые можно распаковать для восстановления исходных данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое миграция данных?", "Миграция данных — это процесс переноса данных из одной системы или места хранения в другую. Это часто делается при обновлении систем, изменении платформ или консолидации данных из нескольких источников. Миграция данных требует тщательного планирования и проверки для обеспечения точности и целостности данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое пользовательский интерфейс (UI)?", "Пользовательский интерфейс — это точка взаимодействия между пользователем и компьютерной системой или приложением. Он включает в себя такие элементы, как меню, кнопки, формы и графические дисплеи, которые позволяют пользователям перемещаться и взаимодействовать с программным обеспечением.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое протокол компьютерной сети?", "Протокол компьютерной сети — это набор правил и соглашений, регулирующих связь между устройствами в сети. Протоколы определяют, как данные форматируются, передаются, принимаются и интерпретируются, позволяя устройствам эффективно обмениваться информацией.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое визуализация данных?", "Визуализация данных — это представление данных в визуальном формате, например в виде диаграмм, графиков или карт, для облегчения понимания, анализа и принятия решений. Это помогает выявлять закономерности, тенденции и идеи, которые могут быть незаметны в необработанных данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое операционная система?", "Операционная система — это программное обеспечение, которое управляет аппаратными и программными ресурсами компьютера и предоставляет общие услуги для компьютерных программ. Он позволяет пользователям взаимодействовать с компьютером и координирует такие задачи, как управление памятью, операции с файловой системой и драйверы устройств.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое утечка данных?", "Утечка данных происходит, когда неавторизованные лица или организации получают доступ к важным или конфиденциальным данным без разрешения. Нарушение данных может привести к потере, краже или раскрытию данных, что может привести к финансовым потерям, ущербу для репутации или юридическим последствиям.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое целостность данных?", "Целостность данных относится к точности, непротиворечивости и надежности данных в течение их жизненного цикла. Это гарантирует, что данные являются полными, достоверными и не содержат ошибок или искажений. Целостность данных поддерживается за счет проверки, проверки на наличие ошибок и мер безопасности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое мобильное приложение (приложение)?", "Мобильное приложение или приложение — это программное приложение, предназначенное для работы на мобильных устройствах, таких как смартфоны или планшеты. Мобильные приложения служат различным целям, включая развлечения, производительность, общение и доступ к онлайн-сервисам.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кто такой специалист по данным?", "Специалист по данным — это профессионал, который использует научные методы, алгоритмы и статистические методы для извлечения информации, анализа данных и решения сложных проблем. Они сочетают в себе знания предметной области, навыки программирования и опыт анализа данных, чтобы извлекать ценную информацию из больших наборов данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова роль сетевого администратора?", "Сетевой администратор отвечает за управление и обслуживание компьютерных сетей организации. Они настраивают и контролируют сетевые устройства, устраняют неполадки в сети, обеспечивают сетевую безопасность и применяют сетевые политики и стандарты.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое программное обеспечение для автоматизированного проектирования (САПР)?", "Программное обеспечение для автоматизированного проектирования используется инженерами, архитекторами и дизайнерами для создания, изменения и оптимизации проектов в цифровой среде. Программное обеспечение САПР позволяет точно и эффективно создавать, редактировать и документировать проекты.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое автоматизация бизнес-процессов (BPA)?", "Автоматизация бизнес-процессов предполагает использование технологий для рационализации и автоматизации повторяющихся задач и рабочих процессов в организации. BPA повышает эффективность, уменьшает количество ошибок и высвобождает ресурсы для более стратегических действий.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое машинное обучение?", "Машинное обучение — это подмножество искусственного интеллекта, которое фокусируется на разработке алгоритмов и моделей, которые позволяют компьютерам учиться на данных и делать прогнозы или выполнять действия без явного программирования. Он включает в себя обучение моделей на данных для распознавания закономерностей, принятия решений или выполнения задач.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое бизнес-аналитика (BI)?", "Бизнес-аналитика охватывает стратегии, технологии и процессы, используемые для сбора, анализа и представления данных для поддержки принятия бизнес-решений. Системы бизнес-аналитики собирают данные из различных источников, преобразовывают их в осмысленные идеи и представляют в виде информационных панелей, отчетов или визуализаций.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое система автоматизированного производства (CAM)?", "Системы автоматизированного производства используются в обрабатывающей промышленности для контроля и автоматизации производственного процесса. Программное обеспечение CAM преобразует цифровые проектные данные в инструкции для станков, таких как станки с числовым программным управлением (ЧПУ), для точного и эффективного производства продукции.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое управление данными?", "Управление данными — это общее управление доступностью, удобством использования, целостностью и безопасностью данных в организации. Он включает в себя установление политик, процессов и средств контроля для обеспечения качества данных, соблюдения правил и эффективных методов управления данными.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое программная ошибка?", "Программная ошибка — это недостаток или ошибка в программном приложении, из-за которой оно ведет себя непредвиденно или дает неправильные результаты. Ошибки могут быть вызваны ошибками программирования, логическими ошибками или проблемами совместимости и обычно выявляются и устраняются в процессе отладки и тестирования.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое требование к программному обеспечению?", "Требование к программному обеспечению — это утверждение, описывающее желаемую функциональность, функцию или ограничение программной системы. Требования охватывают потребности заинтересованных сторон и служат основой для проектирования, разработки и тестирования программного обеспечения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое виртуализация в ИТ?", "Виртуализация — это процесс создания виртуальной или смоделированной версии вычислительного ресурса, такого как сервер, операционная система, устройство хранения или сеть, с помощью программного обеспечения. Это позволяет эффективно использовать физические ресурсы и позволяет создавать несколько виртуальных сред в одной физической инфраструктуре.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое жизненный цикл разработки программного обеспечения (SDLC)?", "Жизненный цикл разработки программного обеспечения — это процесс, используемый для планирования, проектирования, разработки, тестирования, развертывания и обслуживания программных приложений. Он обеспечивает структурированный подход к разработке программного обеспечения, обеспечивая качество, эффективность и соблюдение требований.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое управление данными?", "Управление данными — это общее управление доступностью, удобством использования, целостностью и безопасностью данных в организации. Он включает в себя установление политик, процессов и средств контроля для обеспечения качества данных, соблюдения правил и эффективных методов управления данными.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое Agile scrum framework?", "Agile scrum framework — это итеративный и совместный подход к разработке программного обеспечения, который делает упор на гибкость, адаптивность и сотрудничество с клиентами. Скрам-команды работают короткими итерациями, называемыми спринтами, с частыми встречами по обмену информацией, планированию и обзору.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое киоск данных?", "Киоск данных — это подмножество хранилища данных, ориентированное на определенный отдел, функцию или направление деятельности в организации. Витрины данных содержат структурированные данные, предназначенные для удовлетворения конкретных аналитических потребностей конкретного бизнес-подразделения или группы пользователей.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое комплект для разработки программного обеспечения (SDK)?", "Комплект для разработки программного обеспечения — это набор инструментов, библиотек и документации, предоставляемых поставщиком программного обеспечения или платформой, чтобы помочь разработчикам создавать приложения для конкретной платформы или технологии. Пакеты SDK предоставляют ресурсы и API, которые упрощают и ускоряют процесс разработки.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кто такой full-stack разработчик?", "Full-stack разработчик — это профессионал, который имеет опыт как в разработке front-end, так и в back-end. Они владеют несколькими языками программирования, платформами и технологиями и могут работать над всеми аспектами программного приложения, от пользовательских интерфейсов до серверной логики.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое виртуализация серверов?", "Виртуализация серверов — это процесс разделения физического сервера на несколько виртуальных серверов, каждый из которых работает со своей операционной системой и приложениями. Виртуализация обеспечивает эффективное использование ресурсов, масштабируемость и изоляцию рабочих нагрузок на одном физическом сервере.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое интерфейс прикладного программирования (API)?", "Интерфейс прикладного программирования — это набор правил, протоколов и инструментов, которые позволяют различным программным приложениям общаться и взаимодействовать друг с другом. API определяют методы и форматы данных, используемые для обмена информацией между приложениями.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое озеро данных?", "Озеро данных — это централизованный репозиторий, в котором хранятся структурированные, полуструктурированные и неструктурированные данные в необработанном формате. В отличие от традиционных хранилищ данных, озера данных позволяют хранить большие объемы данных различных типов, обеспечивая гибкую и масштабируемую аналитику и обработку данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое DevOps?", "DevOps — это совместный подход, который сочетает в себе разработку программного обеспечения (Dev) и ИТ-операции (Ops) для оптимизации процесса доставки программного обеспечения. DevOps делает упор на автоматизацию, непрерывную интеграцию, непрерывную доставку и тесное сотрудничество между командами разработки и эксплуатации.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое модель «программное обеспечение как услуга» (SaaS)?", "«программное обеспечение как услуга» — это модель доставки программного обеспечения, в которой приложения предоставляются через Интернет как услуга. Вместо того, чтобы устанавливать и поддерживать программное обеспечение локально, пользователи получают доступ к приложениям, размещенным у провайдера, и используют их, платя по подписке.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое большие данные?", "Большие данные относятся к большим и сложным наборам данных, которыми трудно управлять, обрабатывать и анализировать с использованием традиционных методов обработки данных. Большие данные часто связаны с большими объемами, скоростью и разнообразием данных и требуют специальных инструментов и методов для извлечения, хранения и анализа.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кто такой специалист по данным?", "Специалист по данным — это профессионал, который использует научные методы, алгоритмы и статистические методы для извлечения информации, анализа данных и решения сложных проблем. Они сочетают в себе знания предметной области, навыки программирования и опыт анализа данных, чтобы извлекать ценную информацию из больших наборов данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое соглашение об уровне обслуживания (SLA)?", "Соглашение об уровне обслуживания — это контракт или соглашение между поставщиком услуг и клиентом, определяющее ожидаемый уровень обслуживания. SLA обычно включают показатели производительности, целевые показатели доступности, время отклика и другие параметры, обеспечивающие качество и надежность услуги.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое контейнеризация?", "Контейнеризация — это облегченная технология виртуализации, позволяющая развертывать и изолировать приложения и их зависимости. Контейнеры обеспечивают согласованную и переносимую среду выполнения, упрощая упаковку, распространение и запуск программного обеспечения в различных вычислительных средах.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое облачные вычисления?", "Облачные вычисления — это доставка вычислительных ресурсов, таких как серверы, хранилища, базы данных, программное обеспечение и аналитика, через Интернет с оплатой по факту использования. Облачные вычисления обеспечивают масштабируемость, гибкость и экономическую эффективность за счет устранения необходимости в локальной инфраструктуре и предоставления ресурсов по запросу.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое Интернет вещей (IoT)?", "Интернет вещей относится к сети физических устройств, транспортных средств, бытовой техники и других объектов, оснащенных датчиками, программным обеспечением и средствами связи, что позволяет им собирать данные и обмениваться ими. . Устройства IoT могут взаимодействовать друг с другом и с людьми, что приводит к созданию интеллектуальных и взаимосвязанных систем.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое хранилище данных?", "Хранилище данных — это централизованное хранилище, в котором хранятся структурированные и исторические данные из различных источников внутри организации. Хранилища данных поддерживают анализ данных, составление отчетов и принятие решений, предоставляя консолидированное представление данных из нескольких систем.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое лицензия на программное обеспечение?", "Лицензия на программное обеспечение — это юридическое соглашение, определяющее условия использования, распространения и модификации программного обеспечения. Лицензии могут быть проприетарными или открытыми, и они определяют права и ограничения, связанные с программным обеспечением.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое глубокое обучение?", "Глубокое обучение — это подмножество машинного обучения, которое фокусируется на обучении искусственных нейронных сетей с несколькими слоями для изучения и извлечения закономерностей из больших объемов данных. Алгоритмы глубокого обучения успешно применяются в таких областях, как распознавание изображений, обработка естественного языка и распознавание речи.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кто такой аналитик данных?", "Аналитик данных — это профессионал, который собирает, анализирует и интерпретирует данные для извлечения информации и поддержки принятия решений. Аналитики данных используют статистические методы, инструменты визуализации данных и знания предметной области для выявления тенденций, закономерностей и корреляций в данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое тестирование программного обеспечения?", "Тестирование программного обеспечения — это процесс оценки программного приложения или системы для выявления дефектов, ошибок или расхождений между ожидаемыми и фактическими результатами. Тестирование обеспечивает качество, надежность и удобство использования программного обеспечения с помощью различных методов, таких как функциональное тестирование, тестирование производительности и тестирование безопасности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кто такой разработчик интерфейса?", "Разработчик интерфейса — это профессионал, который специализируется на разработке пользовательских интерфейсов и пользовательского опыта для программных приложений или веб-сайтов. Они используют такие языки, как HTML, CSS и JavaScript, для создания интерактивных и визуально привлекательных интерфейсов, с которыми пользователи взаимодействуют напрямую.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое интеллектуальный анализ данных?", "Интеллектуальный анализ данных — это процесс обнаружения закономерностей, взаимосвязей и выводов из больших наборов данных с использованием статистических методов, алгоритмов машинного обучения и методов визуализации данных. Интеллектуальный анализ данных помогает извлекать ценную информацию и знания, которые можно использовать для принятия решений и прогнозного анализа.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое виртуальная частная сеть (VPN)?", "Виртуальная частная сеть — это безопасное и зашифрованное соединение, которое позволяет пользователям получать доступ к частной сети через общедоступную сеть, например через Интернет. VPN обеспечивают конфиденциальность, конфиденциальность и безопасность данных, создавая безопасный туннель для передачи данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое система управления базами данных (СУБД)?", "Система управления базами данных — это программное обеспечение, которое управляет хранением, поиском, организацией и манипулированием данными в базе данных. СУБД обеспечивают структурированный и эффективный способ хранения данных и доступа к ним, гарантируя целостность данных, безопасность и параллелизм.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое программный патч?", "Программный патч — это фрагмент кода или обновление программного обеспечения, которое применяется к существующему программному приложению для исправления ошибок, устранения уязвимостей в системе безопасности или добавления новых функций. Патчи обычно выпускаются поставщиками программного обеспечения для обеспечения стабильности и безопасности своих продуктов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое брандмауэр?", "Брандмауэр — это устройство или программное обеспечение сетевой безопасности, которое отслеживает и контролирует входящий и исходящий сетевой трафик на основе заранее определенных правил безопасности. Брандмауэры помогают защитить сети и системы от несанкционированного доступа, вредоносных программ и других угроз безопасности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое предварительная обработка данных?", "Предварительная обработка данных относится к подготовке и преобразованию необработанных данных перед их использованием для анализа или моделирования. Этот процесс включает в себя такие задачи, как очистка данных, нормализация, выбор функций и обработка пропущенных значений для повышения качества и надежности данных.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое центр обработки данных?", "Центр обработки данных — это объект, в котором размещены компьютерные системы, серверы, сетевое оборудование и системы хранения для хранения, обработки, управления и распространения больших объемов данных. Центры обработки данных предоставляют инфраструктуру и ресурсы, необходимые для различных ИТ-услуг и приложений.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое адаптивный веб-дизайн?", "Адаптивный веб-дизайн — это подход к веб-дизайну и разработке, направленный на создание веб-сайтов, которые автоматически настраивают и адаптируют свой макет и содержимое в зависимости от устройства пользователя или размера экрана. Адаптивный дизайн обеспечивает оптимальный просмотр и удобство использования на разных устройствах, таких как настольные компьютеры, планшеты и смартфоны.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое индекс базы данных?", "Индекс базы данных — это структура данных, которая повышает скорость и эффективность операций поиска данных в базе данных. Индексы создаются для определенных столбцов таблицы и обеспечивают быстрый поиск, сортировку и фильтрацию данных, уменьшая потребность в сканировании всей таблицы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое система обнаружения вторжений (IDS)?", "Система обнаружения вторжений — это технология безопасности, которая отслеживает сетевые или системные действия на предмет подозрительного или злонамеренного поведения. IDS анализируют сетевой трафик, файлы журналов и системные события, чтобы обнаруживать и предупреждать администраторов о потенциальных нарушениях безопасности или атаках.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое анонимизация данных?", "Анонимизация данных — это процесс удаления или изменения информации, позволяющей установить личность, из наборов данных для защиты частной жизни и конфиденциальности. Анонимные данные не могут быть напрямую связаны с человеком, что гарантирует безопасность конфиденциальной информации и позволяет проводить анализ и исследования.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое предотвращение потери данных (DLP)?", "Предотвращение потери данных — это набор стратегий, политик и технологий, предназначенных для предотвращения несанкционированной потери, утечки или раскрытия конфиденциальных данных. Решения DLP отслеживают, классифицируют и защищают данные в сетях, конечных точках и хранилищах, чтобы обеспечить безопасность данных и соответствие требованиям.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое распределенная система?", "Распределенная система — это сеть взаимосвязанных компьютеров или серверов, которые работают вместе для достижения общей цели. Распределенные системы позволяют совместно использовать ресурсы, данные и обработку на нескольких узлах, улучшая масштабируемость, отказоустойчивость и производительность.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое контроль версий?", "Контроль версий — это система или процесс, который позволяет разработчикам отслеживать и управлять изменениями в исходном коде или файлах с течением времени. Системы контроля версий ведут учет каждой модификации, обеспечивают совместную работу разработчиков и предоставляют механизмы для ветвления, слияния и отмены изменений.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое парадигма программирования?", "Парадигма программирования — это способ классификации и организации языков программирования на основе их фундаментальных принципов и особенностей. Общие парадигмы включают процедурное, объектно-ориентированное, функциональное и декларативное программирование, каждое из которых подчеркивает разные подходы к решению проблем и организации кода.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое облачное хранилище?", "Облачное хранилище — это услуга, которая позволяет пользователям хранить, получать доступ и управлять своими данными и файлами удаленно на серверах, предоставляемых поставщиком облачного хранилища. Облачное хранилище предлагает масштабируемость, избыточность данных и доступность из любого места, где есть подключение к Интернету.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое сеть доставки контента (CDN)?", "Сеть доставки контента — это распределенная сеть серверов, расположенных в разных географических точках, которые доставляют конечным пользователям веб-контент, такой как изображения, видео и статические файлы. эффективно. CDN помогают повысить производительность веб-сайта, сократить задержки и справиться с высокой нагрузкой трафика.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое шифрование данных?", "Шифрование данных — это процесс преобразования данных открытого текста в зашифрованный текст для защиты от несанкционированного доступа или перехвата. Алгоритмы шифрования используют математические функции и ключи для шифрования данных, что делает их нечитаемыми без соответствующего ключа дешифрования.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое чат-бот?", "Чат-бот — это компьютерная программа или программное обеспечение на основе искусственного интеллекта, которое имитирует человеческий разговор с помощью текстовых или голосовых взаимодействий. Чат-боты предназначены для понимания пользовательских запросов и предоставления соответствующих ответов или выполнения задач, часто используемых для поддержки клиентов, поиска информации или автоматизации процессов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое программная среда?", "Программная среда — это предварительно разработанная программная инфраструктура многократного использования, которая обеспечивает основу для разработки приложений. Фреймворки предлагают библиотеки, инструменты и абстракции, которые упрощают и ускоряют процесс разработки, предоставляя предопределенные структуры и функции.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое резервная копия данных?", "Резервная копия данных — это копия цифровых данных или файлов, которые создаются и хранятся отдельно от исходных данных для защиты от потери или повреждения данных. Резервные копии можно использовать для восстановления данных в случае случайного удаления, сбоя оборудования или других катастрофических событий.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое виртуальная машина (ВМ)?", "Виртуальная машина — это программная эмуляция физического компьютера, которая позволяет нескольким операционным системам работать на одной физической машине. Виртуальные машины обеспечивают изоляцию, гибкость и аппаратную абстракцию, что позволяет эффективно использовать вычислительные ресурсы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое облачная архитектура?", "Облачная архитектура относится к проектированию и разработке приложений специально для облачных сред с использованием преимуществ облачных сервисов и таких принципов, как масштабируемость, эластичность, отказоустойчивость и автоматизация. Облачные приложения обычно контейнеризированы, слабо связаны и предназначены для распределенных сред.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое обработка данных?", "Обработка данных, также известная как очистка данных или обработка данных, представляет собой процесс преобразования и подготовки необработанных данных для анализа. Он включает в себя такие задачи, как очистка данных, интеграция данных, преобразование данных и обработка отсутствующих или несогласованных данных для обеспечения качества данных и удобства использования.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое интеллектуальный анализ данных?", "Интеллектуальный анализ данных — это процесс обнаружения закономерностей, взаимосвязей и выводов из больших наборов данных с использованием статистических методов, алгоритмов машинного обучения и методов визуализации данных. Интеллектуальный анализ данных помогает извлекать ценную информацию и знания, которые можно использовать для принятия решений и прогнозного анализа.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как поступить в Astana IT University (AITU / аиту)?", "Чтобы поступить в Astana IT University (AITU), вам следует следовать процессу приема, который включает несколько этапов. Вот общая информация о поступлении в AITU:  1. Ознакомьтесь с требованиямиПосетите официальный веб-сайт AITU (https://admission.astanait.edu.kz/) или свяжитесь с приемной комиссией, чтобы получить подробную информацию о требованиях к поступлению. Это включает актуальные даты и сроки подачи заявлений, необходимые документы и минимальные академические требования.  2. Пройдите вступительный экзамен АЕТВам могут потребоваться пройти отборочные испытания, которые могут включать в себя тестирование ваших знаний английского языка и компьютерной грамотности, собеседование или другие виды оценки. Точные требования могут различаться в зависимости от выбранной программы.  3. Зарегистрируйтесь на портале приема абитуриентовabitur.astanait.edu.kz  4. Заполните заявкуПодайте заявку на грант, предоставив запрашиваемую информацию о себе и выбранных программе обучения. Обычно это делается онлайн на официальном веб-сайте EGOV.KZ.  5. Предоставьте необходимые документыПри подаче заявки вы должны предоставить официальные копии свидетельства о среднем образовании (аттестата) или эквивалентного документа, а также другие требуемые документы, такие как результаты экзаменов, рекомендательные письма и т. д. (https://admission.astanait.edu.kz/#rec309542619)  6. Получите результаты поступленияПосле проведения отборочных испытаний и рассмотрения вашей заявки, вы будете уведомлены о результатах поступления. Если ваша заявка будет принята, вы получите приглашение на учебу в AITU.  Если у вас возникнут дополнительные вопросы или вам потребуется детальная информация о процессе поступления, рекомендуется связаться с приемной комиссией Astana IT University напрямую для получения актуальной информации и руководства по поступлению.  ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем преимущества Astana IT University (AITU / аиту)?", "Astana IT University – лидер инновационного непрерывного IT-образования и науки с устойчивыми академическими традициями и высокой социальной ответственностью. Стратегические ориентиры данного университета включают несколько пунктов: Академическое превосходство в непрерывном ІТ-образовании и высокая востребованность выпускников; Лидерство в исследованиях и инновациях; Национальное и международное признание; Реализация модели Smart и Digital university; Финансовая устойчивость и социальная ответственность; ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В чем цель Astana IT University (AITU / аиту)?", "Глобальная цель Astana IT University - обеспечение качества подготовки кадров в сфере ИКТ на междисциплинарной основе.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое Astana IT University (AITU / аиту)?", "Astana IT University (AITU) — это высшее учебное заведение, расположенное в Нур-Султане (ранее известном как Астана), столице Казахстана. Университет был основан в 2019 году в рамках стратегии по развитию информационных технологий в Казахстане.  Astana IT University предлагает программы бакалавриата и магистратуры в области информационных технологий, включая компьютерные науки, программирование, анализ данных, кибербезопасность и другие связанные области. Университет стремится подготовить высококвалифицированных специалистов, которые смогут успешно работать в индустрии информационных технологий и вносить вклад в технологическое развитие Казахстана.  Astana IT University сотрудничает с ведущими компаниями и организациями в области информационных технологий, чтобы обеспечить студентам доступ к актуальным знаниям, профессиональным практикам и возможностям стажировки. Университет также активно участвует в научных исследованиях и инновационных проектах, сотрудничая с национальными и международными научными организациями.  Astana IT University стремится стать ведущим учебным заведением в области информационных технологий в Казахстане и регионе Центральной Азии, предлагая качественное образование и способствуя развитию IT-сектора в стране.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у вас общежитие?", "Да. В 15 минутах от университета расположены три дома студентов для мальчиков и для девочек, в ЖК EXPO Residence.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какова стоимость обучения у вас в университете?", "Стоимость обучения в нашем университете составляет 2,500,000 (два миллиона пятьсот тысяч) тенге в год, без учета скидок.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как поступить на платное отделение Astana IT University (AITU / аиту)?", "Чтобы поступить на платное отделение в Astana IT University (AITU), вам следует следовать процессу приема, который включает несколько этапов: 1) Ознакомьтесь с требованиямиПосетите официальный веб-сайт AITU (https://admission.astanait.edu.kz/international-students-remove) или свяжитесь с приемной комиссией, чтобы получить подробную информацию о требованиях к поступлению. Это включает актуальные даты и сроки подачи заявлений, необходимые документы и минимальные академические требования. 2) Пройдите собеседование с деканом Astana IT University 3) Предоставте полный пакет документов, зарегистрировавшись на платформе приема абитуриентов (https://admission.astanait.edu.kz/#rec309542619) ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Сколько лет учатся в AITU (аиту) на бакалавриате?", "Срок обучения в AITU — 3 года. Наши студенты оканчивают университет на 1 год раньше, за счет интенсивности обучения. Студенты AITU учатся до июля месяца, тем самым закрывают больше кредитов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("На каком языке проводится обучение?", "Обучение в Astana IT University – ведется полностью на английском языке.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Можно ли поступить в университет с баллами MESC?", "Вы можете поступить в университет с баллами MESC, при условии их конвертации в баллы ЕНТ. Данную процедуру вы можете провести на базе Национального Центра Тестирования.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли в университете военная кафедра?", "В Astana IT University имеется военная кафедра. Наши студенты, могут получить звание офицер запаса, по 6 специализациям: кибербезопасность, геоинформационные системы, применение подразделений и частей топогеодезического обеспечения, организация воспитательной и идеологической работы в Сухопутных войсках, организация психологической работы, информационное обеспечение функционирования автоматизированных систем управления. Так же могут получить сержант запаса, по 3 специализациямТопогеодезия, штабные, электронно-вычислительные машины. Вы можете поступить на военную кафедру, как на грант, так и на платное отделение. ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Можно ли поступить в университет с баллами SAT?", "Вы можете поступить в университет с баллами SAT, при условии их конвертации в баллы ЕНТ. Данную процедуру вы можете провести на базе Национального Центра Тестирования.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой пороговый балл для поступления на грант?", "Порогового балла для поступления на грант не бывает. Для подачи заявления на грант в наш университет вам нужно набрать не менее 70 или 85 баллов (в зависимости от образовательной программы), а также не менее 5 баллов по каждому предмету на ЕНТ и сдать вступительные экзамены AET. Далее гранты будут распределяться по баллам. В первую очередь гранты получают абитуриенты с наивысшими результатами.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Где находится Astana IT University AITU (аиту)?", "Astana IT University находится в г. Астана по адресу: Мангилик Ел 55/11, Блок С1, Expo");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("График работы?", "С понедельника по пятницу - 9:00 — 18:00 Обеденное время - 13:00 — 14:00");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Контакты AITU (аиту)?", "Контактный номер: +7 (7172) 645-710 Emailinfo@astanait.edu.kz");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Когда можно сдать творческий экзамен?", "Период проведения творческих экзаменов с 8 июля по 15 июля (на момент 2023 года) Экзамены пройдут в очном формате в Astana IT University");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как поступить на Digital Journalism?", "Для поступления на Digital Journalism нужно сдать творческий экзамен в нашем университете. Творческие экзамены направлены на выявление способностей у абитуриентов и личностных предпосылок к овладению профессией творческой направленности. Подробнее ознакомиться с требованиями можно на сайте: https://admission.astanait.edu.kz/digitaljournalism");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("На каком языке будет проходить творческий экзамен?", "Абитуриенты имеют право сдать экзамен на трёх языках, это: русский язык, казахский язык и английский язык 20. На каком языке будет проходить обучение Digital Journalism (DJ) Цифровая журналистика Обучение для поступивших на образовательную программу «Цифровая журналистика» проводится на английском языке");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В каком жанре, стиле нужно писать сочинение?", "Сочинение на заданную тему – письменное изложение в произвольной прозаической форме и композиции индивидуальных мнений, впечатлений, размышлений. Стиль изложения – литературный или публицистический, язык – в рамках лексических, морфологических и синтаксических норм школьной программы. Основным требованием к эссе является раскрытие заявленной темы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как проходит творческий экзамен?", "Творческий экзамен состоит из 2-х этапов. Первый этап проводится письменно в форме сочинения на одну из заданных тем. Второй этап проводится устно в форме собеседования (защита творческого проекта и ответы на вопросы комиссии).");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие темы сочинения?", "А вот и список тем для первого этапа творческого экзамена (на момент 2023 года): Роль цифровизации в моей жизни или как я представляю цифровое будущее. Профессия «журналист»мы там, где кипит жизнь! Журналистика в 21 веке и ее социальная значимость. Роль журналистики в «Новом Казахстане». Челленджи глобализации (современные вызовы человечества). Мои идеи - моей стране.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как оценивается творческий экзамен (критерии оценивания)?", "Критерии оценивания первого этапа творческого экзамена: способность раскрыть тему, содержательность, аналитика – 15 баллов; структура, композиционная завершенность – 15 баллов; язык и стиль изложения, образность, грамотность – 15 баллов.  Общий объем эссе – 800-900 слов.  Время написания сочинения – 3 часа.  Результаты творческого экзамена объявляются в день проведения экзамена. Работы проходят проверку на плагиат.  Критерии оценивания второго этапа творческого экзаменапредставление опубликованных работ (статьи, сценарии) или презентация проекта – 20 баллов; общий кругозор абитуриента (знание абитуриентом общественно-политической жизни страны, государственного устройства, приоритетных направлений внутренней и внешней политики Казахстана и т. д.), уровень представления о деятельности СМИ (общая характеристика медиапорталов, средств массовой информации) – 10 баллов; умение излагать свои мысли, богатство речи, общая культура, ораторское мастерство, дикция и т.д. – 15 баллов.  Время защиты презентации составляет 10 минут, 5 минут на вопросы от членов комиссии. ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Молодежь - это будущее или настоящее Казахстана?", "Национальная культура и ментальность в турбулетном потоке времени. Книга (как альтернатива - фильм), повлиявшая на мои взгляды: параллель между мной и героем. Новая реальность. Влияние диджитал-пространства на социальные навыки.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как проходит второй этап творческого экзамена?", "2-ой творческий экзамен проводится устно в форме собеседования (защита творческого проекта и ответы на вопросы комиссии). Творческий проект – публичное представление абитуриентом собственного креативного решения для достижения поставленной цели в задании. Тематика заданий составляется, исходя из специфики будущей специальности.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как зарегистрироваться на творческий экзамен?", "Для регистрации на творческий экзамен можете перейти по ссылке: https://admission.astanait.edu.kz/digitaljournalism ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("На какие темы нужно делать творческий проект?", "Темы творческих проектов (абитуриент выбирает одну тему): Тема №1. Разработайте флешмоб или лайфхак, популяризующие Образовательную программу по журналистике Университета в социальных сетях. Разработайте стартап проект в области интерактивных медиа технологий. Тема №2. Предложите контент-план и контент-стратегию для продвижения страницы в социальных сетях1) страница Университета; 2) индивидуальная страница; 3) группа или проект в социальных сетях. Тема №3. Разработайте стартап проект в области цифровых медиа технологий, рекламы, PR, связей с общественностью, информационно-коммуникативных технологий. Тема № 4. Продемонстрируйте собственный видеофильм, рекламный ролик, короткометражный фильм, публикацию в СМИ и другие формы авторского медиаконтента.  Презентация проекта по одной из вышеуказанных тем обязательна, допускаются схематические и графические изображения в электронном и бумажном варианте. ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Где можно проконсультироваться со студентами?", "Для консультации со студентами приемной комиссии можно перейти по ссылке: https://t.me/+KrO4Eh0R2ZgxYmI6 Не забудьте ознакомиться с правилами чата На казахском языкеhttps://t.me/c/1796272843/45509 На русском языкеhttps://t.me/c/1796272843/45510 На английском языкеhttps://t.me/c/1796272843/45511 ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Можно ли посмотреть как выглядит университет?", "Можно пройти 3D-тур по университету по ссылке: https://kuula.co/share/collection/7PDWF?fs=1&vr=1&sd=1&initload=0&thumbs=1&info=0&logo=0 ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("На какие образовательные программы можно поступить в Astana IT University (AITU / аиту)?", "В Astana IT University можно поступить на следующие образовательные программы: Информационные технологии. Профильные предметыматематика + информатика. Пороговый балл85. Информационная безопасность. Профильные предметыматематика + информатика. Пороговый балл85. Коммуникации и коммуникационные технологии. Профильные предметыматематика + физика. Пороговый балл70. Электротехника и автоматика. Профильные предметыматематика + физика. Пороговый балл70. Менеджмент и управление. Профильные предметыматематика + география. Пороговый балл70. Журналистика и репортерское дело. Творческие экзамены.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие специальности входят в Информационные технологии?", "6B06102 Software Engineering (Программная инженерия) 6B06101 Computer Science (Компьютерные науки) 6B06103 Big Data Analysis (Анализ больших данных) 6B06106 Mathematical & Computational Science (Математические и вычислительные науки) 6B06105 Media Technologies (Медиа технологии)");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кем можно стать окончив Software Engineering (Программная инженерия)?", "Карьерные возможности после окончания бакалавриата на специальность Software Engineering (Программная инженерия): — Инженер-программист;  — Разработчик программного обеспечения;  — Back-end и front-end разработчик;  — Веб-разработчик;  — Тестировщик программного обеспечения;  — Менеджер и архитектор ИТ систем и проектов;  — Мобильный разработчик. ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кем можно стать\работать окончив Computer Science (Компьютерные науки)?", "Карьерные возможности после окончания бакалавриата на специальность Computer Science (Компьютерные науки): — Специалист по разработке программного обеспечения;  — Специалист по сопровождению компьютерных систем;  — Специалист по разработке и продвижению мобильных приложений;  — ИТ инженер или Специалист по серверному оборудованию;  — Менеджер ИТ-систем;  — Архитекторы информационных систем. ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кем можно стать\работать окончив Big Data Analysis (Анализ больших данных)?", "Карьерные возможности после окончания бакалавриата на специальность Big Data Analysis (Анализ больших данных): — Аналитик в крупных научных, исследовательских и консалтинговых проектах;  — Специалист по машинному обучению;  — Специалист по анализу данных;  — Аналитик в крупных областях промышленности, ритейле, в логистических компаниях аналитик бизнеса;  — Аналитик в банковской сфере, в телекоммуникациях, государственном секторе. ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кем можно стать\работать окончив Media Technologies (Медиа технологии)?", "Карьерные возможности после окончания бакалавриата на специальность Media Technologies (Медиа технологии): — Арт-директор;  — Медиа-аналитик;  — Менеджер по цифровым коммуникациям;  — Продюсер проектов в секторе информационных технологий и средств массовой информации;  — Режиссер цифрового контента и средств массовой информации.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кем можно стать\работать окончив Mathematical & Computational Science (Математические и вычислительные науки)?", "Карьерные возможности после окончания бакалавриата на специальность Mathematical & Computational Science (Математические и вычислительные науки): По завершении программы обучающиеся получат квалификации IT разработчик Data Scientist Научный сотрудник Инженер-вычислитель Инфлюенсер Аналитик социальных сетей ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кем можно стать\работать окончив Cybersecurity (Кибербезопасность)?", "По завершении программы обучающиеся получат квалификации: -Специалист по кибербезопасности; -Специалист по информационной безопасности; -Инженер по защите информации; -Специалист программно-аппаратной защиты информации; -Специалист SAP; -Анти-фрод аналитик; -Специалист по реверс-инжинирингу или аналитик кода; -Разработчик системы защиты информации;IT Entrepreneurship (IT предпринимательство) -Специалист по форензике или расследованию кибер-преступлений; -Пентестер. ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кем можно стать\работать окончив Industrial Internet of Things (Промышленный интернет вещей)?", "Карьерные пути, связанные с Industrial Internet of Things (IIoT), включают:  Инженер по разработке систем IIoT. Специалист по внедрению IIoT. Аналитик данных IIoT. Специалист по кибербезопасности IIoT. Менеджер проектов IIoT. Консультант IIoT. ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кем можно стать\работать окончив IT Management (IT менеджмент)?", "Карьерные пути, связанные с IT Management (IT менеджмент), включают: — Менеджер IT проектов.  — IT Директор.  — Директор Офиса системы управления знаниями CKO (Chief Knowledge Officer).  — Директор информационного офиса.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кем можно стать\работать окончив Smart Technologies?", "По завершении программы обучающиеся получат квалификации: — Менеджер IT проектов.  — IT Директор.  — Директор Офиса системы управления знаниями CKO (Chief Knowledge Officer).  — Директор информационного офиса.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кем можно стать\работать окончив IT Entrepreneurship (IT предпринимательство)?", "Карьерные пути, связанные с IT Entrepreneurship (IT предпринимательство), включают: — Предприниматель — Проектный менеджер — Бизнесмен — Менеджер продуктов — Маркетолог — Бизнес-аналитик — Менеджер по управлению онлайн-продажами — Персональный бренд-менеджер — Форсайтер — Front-End разработчик — Системный администратор/аналитик ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кем можно стать\работать окончив Digital Journalism (Цифровая журналистика)?", "Карьерные пути, связанные с Digital Journalism (Цифровая журналистика)?, включают: -Корреспондент, специальный корреспондент средств массовой информации;  -Редактор отдела, редактор СМИ;  -Модератор сайта / администратор SMM;  -Продюсер / копирайтер контента;  -Журналист-аналитик;  -Редактор, главный редактор электронных, цифровых медиа;  -Ведущий журналист новостных каналов в интернет пространстве. ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что умеет обучающийся после обучения на Software Engineering (Программная инженерия)?", "Обучающийся, после освоения всей образовательной программы, должен уметь выполнять следующие пункты: Формулировать и решать задачи, возникающие в ходе производственной деятельности, требующие углубленных профессиональных знаний. Для формулирования задачи могут быть использованы как математический аппарат, так и компьютерные средства; Выбирать необходимые подходы и методы анализа проблем, а также модифицировать существующие и разрабатывать новые, в зависимости из задач конкретного случая; Применять в процессе обучения психологические методы и средства повышения эффективности и качества обучения; Владеть иностранным (английским) языком на профессиональном уровне, позволяющем обучающимся проводить научные исследования на качественно высоком уровне и осуществлять преподавание специальных дисциплин в вузах; Моделировать и проектировать сложные системы используя математические и компьютерные модели и методы; Применять количественные и качественные методы и приёмы для сбора первичной информации для исследования, а также выработки эффективных решений проблем; Анализировать и проектировать программные инструменты анализа данных, а также алгоритмы, модели и методы, требуемые для разработки программных систем, эффективного анализа данных и извлечения знаний из данных; Управлять командой ИТ специалистов в процессе внедрения и развертывания программных систем, а также моделей и методов анализа данных; Выбирать стандарты, методы, технологии, инструменты и технические средства для проведения работ по дальнейшему сопровождению программных систем; Применять методы проектирования и разработки программных систем для решения широкого класса прикладных задач в различных областях, включая междисциплинарные отрасли; Программировать и тестировать различные решения (модели, методы), принимать участие в создании и управлении систем на всех этапах жизненного цикла разработки систем. Создавать реляционные и нереляционные базы данных для эффективного хранения и управления данными в различных крупных организациях, государственных учреждений и других компаниях. Создавать модели анализа структурированных, полуструктурированных и частично неструктурированных данных. Анализировать сложность вычислений и возможность распараллеливания (оптимизации) разрабатываемых алгоритмов и программ. Оценивать основные параметры получаемых параллельных программ, таких как численные показатели требуемых вычислительных ресурсов, ускорение, эффективность и масштабируемость. ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что учат на Software Engineering (Программная инженерия)?", "Образовательная программа «Software Engineering» (Программная инженерия) включает в себя дисциплины по основам разработки программных средств и систем, которые позволяют студентам позиционировать себя как профессионалов в области разработки и сопровождения программного обеспечения различных масштабов. По результатам обучения студенты свободно овладеют несколькими языками программирования, смогут разрабатывать программное обеспечение для крупных компании, такие как банки, страховые компании, государственные и национальные организации и другие. Программа также позволит развить навыки в разработке приложений для мобильных устройств, баз данных и веб-приложении.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой план обучения на Software Engineering (Программная инженерия)? Что проходят на Software Engineering (Программная инженерия)?", "Course Curriculum (План обучения) делится на пять циклов: ЦИКЛ БАЗОВЫХ ДИСЦИПЛИН (Вузовский компонент), ЦИКЛ ОБЩЕОБРАЗОВАТЕЛЬНЫХ ДИСЦИПЛИН (Обязательный компонент), Software Engineering (Программная инженерия) - это область знаний и практических навыков, связанных с разработкой, тестированием и поддержкой программного обеспечения. В учебных курсах по программной инженерии обычно рассматриваются следующие темы 1. Основы программированияизучение языков программирования, структур данных, алгоритмов и базовых концепций разработки программного обеспечения.  2. Проектирование программного обеспеченияпонимание принципов проектирования, архитектурных шаблонов, моделирования систем и управления требованиями.  3. Управление проектамиметодологии разработки программного обеспечения (например, Agile, Waterfall), планирование, управление рисками и оценка проектов.  4. Качество программного обеспечениятестирование, отладка, обеспечение качества, юнит-тестирование, интеграционное тестирование и автоматизация тестирования.  5. Разработка пользовательского интерфейсапроектирование интерфейсов, оптимизация пользовательского опыта и удобство использования.  6. Управление конфигурацией и версионированиеиспользование систем контроля версий, организация рабочего процесса и управление изменениями в коде.  7. Системная архитектура и инфраструктураразвертывание программного обеспечения, конфигурирование серверов, облачные вычисления и обработка больших данных.  8. Безопасность программного обеспеченияосновы безопасности, защита от взломов, шифрование данных и обеспечение конфиденциальности.  9. Управление жизненным циклом программного обеспеченияпонимание этапов жизненного цикла программного обеспечения, обслуживание программного обеспечения и управление его эволюцией.  Конкретный курс по программной инженерии может включать некоторые или все эти темы, а также предоставлять студентам возможность разрабатывать и работать над реальными программными проектами, чтобы применить полученные знания на практике. Со списком дисицплин подробнее можно ознакомиться на сайтеhttps://astanait.edu.kz/software-engineering/");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой план обучения на Computer Science (Компьютерные науки)? Что проходят на Computer Science (Компьютерные науки)?", "Computer Science (Компьютерные науки) - это область знаний, изучающая основы компьютерной техники, алгоритмов, программирования, структур данных и их применение. В учебных курсах по компьютерным наукам обычно рассматриваются следующие темы:  Основы программированияизучение языков программирования, основных концепций и практик разработки программного обеспечения.  Структуры данных и алгоритмыизучение различных структур данных (например, массивы, списки, деревья, графы) и алгоритмов (например, сортировка, поиск, графовые алгоритмы) и их эффективного применения.  Теория вычислительных системизучение основных концепций и моделей вычислений, формальных языков, автоматов, теории сложности и формальной логики.  Архитектура компьютеровпонимание основных компонентов компьютерных систем, таких как процессоры, память, устройства ввода-вывода, и их взаимодействие.  Операционные системыизучение основных принципов операционных систем, управление ресурсами, планирование процессов, синхронизация и управление памятью.  Базы данныхпонимание основных концепций баз данных, моделей данных, языков запросов и проектирования баз данных.  Искусственный интеллектвведение в основы искусственного интеллекта, машинного обучения, нейронных сетей и обработки естественного языка.  Компьютерная графика и визуализацияизучение основных алгоритмов и техник создания и отображения графики, трехмерного моделирования и анимации.  Криптография и безопасностьосновы защиты информации, криптографические протоколы, алгоритмы шифрования и методы обеспечения безопасности.  Сети и распределенные системыпонимание принципов работы компьютерных сетей, протоколов передачи данных, распределенных вычислений и облачных вычислений.  Это лишь некоторые из основных тем, которые могут включаться в программы по компьютерным наукам. Со списком дисицплин подробнее можно ознакомиться на сайтеhttps://astanait.edu.kz/computer-science/ ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой план обучения на Big Data Analysis (Анализ больших данных)? Что проходят на Big Data Analysis (Анализ больших данных)?", "Big Data Analysis (Анализ больших данных) - это область знаний, связанная с обработкой, анализом и извлечением ценной информации из больших объемов данных. В курсах по анализу больших данных обычно рассматриваются следующие темы:  1. Основы больших данныхвведение в концепцию больших данных, характеристики объемных данных, проблемы масштабирования и обработки данных.  2. Архитектура больших данныхизучение основных компонентов архитектуры, таких как распределенные файловые системы, кластеры данных и инфраструктуры для обработки больших данных.  3. Системы управления и обработки данныхзнакомство с различными системами управления базами данных и инструментами для обработки больших данных, такими как Hadoop, Apache Spark, NoSQL и другими.  4. Обработка и хранение данныхизучение методов сбора, очистки, преобразования и хранения больших объемов данных.  5. Алгоритмы анализа данныхизучение алгоритмов и методов анализа данных, включая машинное обучение, статистический анализ, кластеризацию, классификацию и предсказательное моделирование.  6. Визуализация данныхосновы визуализации данных, визуальный анализ и представление результатов анализа больших данных.  7. Масштабирование и оптимизацияметоды оптимизации запросов, параллельная обработка данных, оптимизация производительности и эффективное использование ресурсов.  8. Работа с реальными даннымипрактические навыки работы с реальными наборами данных, применение методов анализа для решения реальных проблем и создание полезных инсайтов.  9. Этика и безопасность данныхосновы этических вопросов, связанных с анализом больших данных, и обеспечение безопасности данных во время их обработки и хранения.  Курсы по анализу больших данных могут включать в себя и другие темы, а также предоставлять возможности для практических проектов и работы с реальными наборами данных. Со списком дисицплин подробнее можно ознакомиться на сайтеhttps://astanait.edu.kz/big-data-analysis/ ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой план обучения на Media Technologies (Медиа технологии)? Что проходят на Media Technologies (Медиа технологии)?", "Media Technologies (Медиа технологии) - это область знаний, связанная с разработкой, производством и обработкой мультимедийного контента, такого как аудио, видео, графика, анимация и другие формы медиа. В курсах по медиа технологиям обычно рассматриваются следующие темы:  1. Основы мультимедиавведение в концепцию мультимедиа, его характеристики и основные принципы создания и обработки различных типов медиа контента.  2. Аудио технологииизучение основ звукозаписи, аудиообработки, цифровой звуковой синтез, сжатие аудио данных и воспроизведение аудио контента.  3. Видео технологииосновы видеозаписи, видеообработки, монтажа и сжатия видео данных, а также технологии видеопотоков и видеокодеки.  4. Графика и анимацияизучение основ графического дизайна, создание и редактирование графических элементов, 2D и 3D анимация, специальные эффекты и визуализация данных.  5. Виртуальная и дополненная реальностьпонимание основ виртуальной реальности (VR) и дополненной реальности (AR), разработка и взаимодействие с виртуальными средами.  6. Управление цифровыми медиа ресурсамиорганизация и управление цифровыми активами, медиа базами данных, метаданными и интеграция с медиа системами.  7. Медиа кодирование и сжатиеизучение различных методов кодирования и сжатия медиа контента для оптимизации его хранения, передачи и воспроизведения.  8. Медиа платформы и приложениязнакомство с различными медиа платформами, программным обеспечением и инструментами для разработки и взаимодействия с мультимедийным контентом.  9. Интерактивные медиаразработка интерактивных медиа приложений, мультимедийных интерфейсов, игр и других форм интерактивного контента.  10. Этика и законодательство в медиа сфереосновы этики медиа, авторских прав и законодательства Со списком дисицплин подробнее можно ознакомиться на сайтеhttps://astanait.edu.kz/media-technologies/ ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой план обучения на Mathematical & Computational Science (Математические и вычислительные науки)? Что проходят на Mathematical & Computational Science (Математические и вычислительные науки)?", "Mathematical & Computational Science (Математические и вычислительные науки) - это область знаний, которая объединяет математическую моделирование и алгоритмические подходы к решению сложных проблем. В курсах по математическим и вычислительным наукам обычно рассматриваются следующие темы:  1. Математический анализизучение основ математического анализа, включая дифференциальное и интегральное исчисления, функциональный анализ и теорию вероятностей.  2. Линейная алгебраосновы линейной алгебры, включая матрицы, векторные пространства, собственные значения и собственные векторы.  3. Дифференциальные уравненияизучение дифференциальных уравнений и их применение для моделирования естественных явлений и процессов.  4. Численные методыизучение численных методов решения математических задач, включая численное интегрирование, решение систем линейных и нелинейных уравнений, аппроксимацию функций и численное дифференцирование.  5. Оптимизациявведение в методы оптимизации, включая выпуклую оптимизацию, градиентные методы, методы безусловной и условной оптимизации.  6. Вероятностные модели и статистикавведение в основы статистики, вероятностных моделей и методов анализа данных.  7. Вычислительная математикаизучение численных алгоритмов, методов вычислений, анализа ошибок и стабильности вычислений.  8. Математическое моделированиеразработка математических моделей для описания сложных систем и процессов, их анализ и прогнозирование.  9. Машинное обучение и искусственный интеллектвведение в основы машинного обучения, алгоритмы классификации, регрессии, кластеризации и нейронные сети.  10. Программирование и вычислительная техникаосновы программирования, алгоритмическое мышление, языки программирования и применение вычислительной техники для решения математических задач.  Это лишь некоторые из основных тем, которые могут включаться в программы по математическим и вычислительным наукам. Со списком дисицплин подробнее можно ознакомиться на сайтеhttps://astanait.edu.kz/mathematical-computational-science-3/ ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой план обучения на Cybersecurity (Кибербезопасность)? Что проходят на Cybersecurity (Кибербезопасность)?", "На курсах по кибербезопасности (Cybersecurity) обычно рассматриваются следующие темы:  1. Основы кибербезопасностивведение в основные концепции, термины и принципы кибербезопасности, включая угрозы информационной безопасности и основные виды атак.  2. Криптографияизучение основ криптографии, включая симметричное и асимметричное шифрование, алгоритмы шифрования, цифровые подписи и протоколы защиты информации.  3. Сетевая безопасностьпонимание уязвимостей сетей, архитектуры безопасности, защиты беспроводных сетей, защиты от атак и мониторинга сетевого трафика.  4. Защита операционных системметоды обеспечения безопасности операционных систем, управление доступом, защита от вредоносного программного обеспечения и злоумышленников.  5. Управление информационной безопасностьюстратегии и методы управления информационной безопасностью, оценка рисков, разработка политик и процедур безопасности.  6. Анализ уязвимостей и тестирование на проникновениеметоды обнаружения и анализа уязвимостей в сетях и системах, тестирование на проникновение, идентификация и реагирование на инциденты безопасности.  7. Защита данных и конфиденциальностьметоды шифрования данных, защита персональной информации, политики конфиденциальности и соблюдение нормативных требований.  8. Безопасность веб-приложенийуязвимости веб-приложений, методы и инструменты для их обнаружения и защиты, в том числе аутентификация, авторизация и защита от атак на веб-приложения.  9. Управление инцидентами и реагирование на нарушенияпланирование, обнаружение и реагирование на инциденты безопасности, восстановление после нарушений и юридические аспекты реагирования на инциденты.  10. Этические и правовые аспекты кибербезопасностиэтический кодекс, законодательство в области кибербезопас  ности, приватность и этические аспекты проведения исследований в области кибербезопасности.  Курсы по кибербезопасности обычно включают в себя как теоретические аспекты, так и практические задания, чтобы студенты могли развивать навыки анализа, защиты и реагирования на угрозы информационной безопасности. Со списком дисицплин подробнее можно ознакомиться на сайтеhttps://astanait.edu.kz/cybersecurity/ ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой план обучения на Smart Technologies? Что проходят на Smart Technologies?", "Smart Technologies (Умные технологии) - это область знаний, связанная с разработкой и применением технологий, которые обладают интеллектуальными и автоматизированными возможностями для улучшения качества жизни и оптимизации различных сфер деятельности. В курсах по умным технологиям обычно рассматриваются следующие темы:  1. Интернет вещей (IoT)понимание концепции и архитектуры IoT, изучение различных устройств, сенсоров и протоколов связи, а также методов сбора, передачи и анализа данных.  2. Системы умного домаизучение интеллектуальных систем, применяемых в домашней среде, таких как автоматизация освещения, отопления и кондиционирования, системы безопасности и контроля доступа, умные бытовые приборы и системы управления энергопотреблением.  3. Умные городапонимание принципов создания интеллектуальных городских систем, включая системы управления транспортом, мониторинг окружающей среды, управление энергопотреблением, общественную безопасность и улучшение городской инфраструктуры.  4. Искусственный интеллект (ИИ)введение в основы и методы искусственного интеллекта, машинного обучения и глубокого обучения, их применение в умных технологиях, анализ данных и прогнозирование.  5. Распределенные системы и облачные вычисленияизучение архитектуры распределенных систем, облачных технологий, управления ресурсами и безопасности в облачных вычислениях, а также применение облачных решений в умных технологиях.  6. Большие данные и анализ данныхосновы сбора, хранения, обработки и анализа больших объемов данных, методы машинного обучения и анализа данных для извлечения ценной информации и принятия умных решений.  7. Виртуальная и дополненная реальностьпонимание принципов и применение виртуальной реальности (VR) и дополненной реальности (AR) в различных сфера  х, таких как образование, развлечения, медицина и проектирование.  8. Робототехника и автономные системыизучение разработки и программирования роботов, автономных систем и дронов, их взаимодействия с окружающей средой и применение в различных областях, включая промышленность, медицину и транспорт.  9. Безопасность и приватность в умных технологияхосновы обеспечения безопасности и защиты данных в системах умных технологий, включая защиту персональной информации, кибербезопасность и этические аспекты.  10. Инновации и предпринимательство в умных технологияхпонимание процесса инноваций, разработка и коммерциализация умных технологических решений, бизнес-моделирование и управление проектами в области умных технологий.  Это лишь некоторые из основных тем, которые могут включаться в программу по умным технологиям. Со списком дисицплин подробнее можно ознакомиться на сайтеhttps://astanait.edu.kz/smart-technologies/ ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой план обучения на Smart Technologies? Что проходят на Smart Technologies?", "В программе по умным технологиям (Smart Technologies) обычно рассматриваются следующие темы:  1. Основы умных технологийвведение в концепцию умных технологий, их роль и применение в различных сферах, а также обзор современных тенденций и развития.  2. Интернет вещей (IoT)понимание архитектуры IoT, протоколов связи, устройств и сенсоров, а также методов сбора, передачи и анализа данных.  3. Системы умного домаизучение интеллектуальных систем, применяемых в домашней среде, таких как автоматизация освещения, отопления и кондиционирования, системы безопасности и контроля доступа, умные бытовые приборы и системы управления энергопотреблением.  4. Умные городаосновы создания интеллектуальных городских систем, включая системы управления транспортом, мониторинг окружающей среды, управление энергопотреблением, общественную безопасность и улучшение городской инфраструктуры.  5. Безопасность умных технологийосновы обеспечения безопасности и защиты данных в системах умных технологий, включая защиту персональной информации, кибербезопасность и этические аспекты.  6. Искусственный интеллект (ИИ)введение в основы и методы искусственного интеллекта, машинного обучения и глубокого обучения, их применение в умных технологиях и анализе данных.  7. Большие данные и анализ данныхметоды сбора, хранения, обработки и анализа больших объемов данных, методы машинного обучения и анализа данных для извлечения ценной информации и принятия умных решений.  8. Распределенные системы и облачные вычисленияархитектура распределенных систем, облачные технологии, управление ресурсами и безопасность в облачных вычислениях, а также применение облачных решений в умных технологиях.  9. Робототехника и автономные системыосновы разработки и программирования роботов, автономных систем и дронов, их взаимодействие с окружающей средой и применение в различных областях, таких как промышленность, медицина и транспорт.  10. Этические и юридические аспекты умных технологийобсуждение этических и юридических вопросов, связанных с использованием умных технологий, включая приватность данных, ответственность и нормативные аспекты.  Это лишь общий обзор тем, которые могут быть включены в программу по умным технологиям. Со списком дисицплин подробнее можно ознакомиться на сайтеhttps://astanait.edu.kz/smart-technologies/ ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что проходят на Industrial Internet of Things?", "Программа по Industrial Internet of Things (IIoT) обычно включает следующие темы:  1. Основы Industrial Internet of Thingsвведение в концепцию IIoT, его роль и значимость для промышленности, а также обзор технологий и стандартов, используемых в IIoT.  2. Промышленные сети и протоколыизучение протоколов связи и сетевых архитектур, применяемых в промышленных средах, включая Ethernet, Modbus, Profibus, OPC-UA и другие.  3. Датчики и устройства IIoTобзор различных типов датчиков и устройств, используемых в IIoT, таких как датчики измерения температуры, давления, вибрации, устройства сбора данных и умные контроллеры.  4. Системы сбора и анализа данныхметоды сбора, хранения и обработки данных, сенсорные сети, базы данных, облачные вычисления и аналитика данных в промышленном контексте.  5. Киберфизические системыпонимание концепции киберфизических систем, которые объединяют физические устройства, сенсоры и сети с цифровыми системами и аналитикой для оптимизации процессов и принятия управленческих решений.  6. Безопасность IIoTосновы обеспечения безопасности и защиты данных в промышленных сетях и системах IIoT, включая аутентификацию, шифрование данных, механизмы контроля доступа и обнаружение угроз.  7. Интеграция систем и протоколовметоды интеграции различных систем и протоколов в IIoT, включая интеграцию существующих систем автоматизации и управления производством (SCADA, DCS) с современными технологиями IIoT.  8. Промышленная автоматика и управление производствомприменение IIoT в автоматизации и управлении производственными процессами, мониторинге и контроле оборудования, оптимизации энергопотребления и предиктивном обслуживании.  9. Применение IIoT в различных отрасляхизучение конкретных примеров и применений IIoT в различных промышленных отраслях, таких как производство, энергетика, транспорт, здравоохранение и другие.  10. Управление проектами и реализация IIoTосновы управления проектами в области IIoT, включая планирование, развертывание и масштабирование систем IIoT, а также анализ ROI и оценку бизнес-преимуществ. Со списком дисицплин подробнее можно ознакомиться на сайтеhttps://astanait.edu.kz/industrial-internet-of-things/ ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой план обучения на IT Entrepreneurship (IT предпринимательство)? Что проходят на IT Entrepreneurship (IT предпринимательство)?", "Программа по IT предпринимательству (IT Entrepreneurship) обычно включает следующие темы:  1. Основы предпринимательствавведение в предпринимательскую деятельность, основные концепции и принципы предпринимательства, планирование и создание нового бизнеса.  2. Инновации и идеи в ITстимулирование и развитие инновационных идей в области информационных технологий, обнаружение рыночных возможностей и создание ценности.  3. Развитие бизнес-моделипроектирование и анализ бизнес-модели, исследование рынка и целевой аудитории, определение уникального предложения и построение стратегии продукта.  4. Технический аспект предпринимательствапонимание технологических аспектов IT-проектов, разработка и управление техническими решениями, прототипирование и разработка MVP (Minimum Viable Product).  5. Финансирование стартаповисточники финансирования стартапов, бизнес-ангелы, венчурные капиталы, гранты, привлечение инвестиций и управление финансами стартапа.  6. Маркетинг и продвижениеосновы маркетинга для IT-предпринимателей, стратегии продвижения продукта и привлечение клиентов, цифровой маркетинг и социальные медиа.  7. Управление командой и рост предпринимательского проектапостроение и управление командой, развитие лидерских навыков, управление ростом и масштабирование бизнеса.  8. Правовые аспекты и интеллектуальная собственностьосновы юридических аспектов предпринимательства, регистрация бизнеса, защита интеллектуальной собственности и соблюдение законодательства.  9. Управление рисками и стратегическое планированиеоценка и управление рисками в предпринимательском проекте, стратегическое планирование и принятие решений.  10. Экосистема стартапов и связи с индустриейзнакомство с инкубаторами, акселераторами, технопарками и другими элементами экосистемы стартапов, создание связей с индустрией и налаживание партнерских отношений. Со списком дисицплин подробнее можно ознакомиться на сайтеhttps://astanait.edu.kz/it-entrepreneurship/ ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой план обучения на IT Management (IT менеджмент)? Что проходят на IT Management (IT менеджмент)?", "Программа по IT-менеджменту (IT Management) обычно включает следующие темы:  1. Управление информационными технологиямивведение в область IT-менеджмента, роль и значимость информационных технологий в организации, основные концепции и модели управления IT.  2. IT-стратегия и планированиеразработка стратегии информационных технологий, выработка целей и планов, выравнивание IT-стратегии с бизнес-стратегией организации.  3. Управление проектами ITметодологии управления проектами в области информационных технологий, планирование, выполнение и контроль проектов, управление рисками и ресурсами.  4. Управление IT-сервисамиосновы организации и предоставления IT-сервисов, управление каталогом сервисов, процессы поддержки и обеспечения качества IT-сервисов.  5. IT-инфраструктура и операциипланирование, развертывание и управление IT-инфраструктурой, включая сети, серверы, базы данных, хранилища данных, облачные решения и безопасность.  6. Управление IT-процессамиметоды и модели управления IT-процессами, такие как ITIL (Information Technology Infrastructure Library), COBIT (Control Objectives for Information and Related Technologies), и ISO/IEC 20000.  7. Бюджетирование и финансовый аспект ITпланирование и управление IT-бюджетом, оценка стоимости проектов и сервисов, расчет ROI (Return on Investment) и бизнес-ценности IT-инвестиций.  8. Управление изменениями и инновациямивнедрение изменений в IT-организации, управление инновациями, оценка и адаптация новых технологий и трендов.  9. Управление командой и коммуникациилидерство в IT-организации, формирование и развитие команды, эффективное общение с заинтересованными сторонами и управление конфликтами.  10. Этические и правовые аспекты ITобсуждение этических и правовых вопросов, связанных с использованием информационных технологий, включая конфиденциальность данных, защиту информации и соблюдение нормативных требований. Со списком дисицплин подробнее можно ознакомиться на сайтеhttps://astanait.edu.kz/it-management/ ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Сколько лет проходит магистратура в Astana IT University?", "Срок обучения на магистратуре в AITU - 2 года.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("На каком языке проходит обучение на магистратуре в Astana IT University?", "2-ух годичное обучение в смешанном формате (Казахский, русский, английский)");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какой план обучения на Digital Journalism (Цифровая журналистика)? Что проходят на Digital Journalism (Цифровая журналистика)?", "Программа по цифровой журналистике (Digital Journalism) обычно включает следующие темы:  1. Основы журналистикивведение в профессию журналиста, основные принципы и этические стандарты, роль и значимость журналистики в цифровой эпохе.  2. Цифровые навыки для журналистаосвоение инструментов и технологий, используемых в цифровой журналистике, включая работу с социальными медиа, создание и редактирование цифрового контента, базовые навыки веб-дизайна и разработки.  3. Репортаж и редактирование в цифровой средеметоды сбора и анализа информации, интервьюирование, написание статей и репортажей, использование мультимедийных элементов (фотографий, видео, аудио) в журналистской работе.  4. Исследовательская журналистика и данныхиспользование цифровых инструментов и методов для проведения исследовательской журналистики, работа с большими объемами данных, визуализация данных и представление результатов.  5. Мобильная журналистикаиспользование мобильных устройств и приложений для съемки, редактирования и публикации контента на месте событий, стриминг и живые трансляции.  6. Социальные медиа и цифровой маркетингстратегии использования социальных медиа в журналистике, распространение контента через социальные платформы, создание и управление персональными брендами в социальных медиа.  7. Аналитика и измерение успехаиспользование инструментов аналитики для измерения эффективности контента, анализ поведения аудитории, оптимизация стратегий публикации и распространения контента.  8. Юридические и этические аспекты цифровой журналистикиправовые ограничения и нормы, связанные с публикацией контента в сети, защита авторских прав, этические вопросы в использовании цифровых источников и технологий.  9. Интерактивная журналистика и взаимодействие с аудиториейсоздание интерактивного контента, использование инструментов для вза  имодействия с аудиторией, учет мнения и обратной связи читателей.  10. Тенденции и инновации в цифровой журналистикеизучение актуальных тенденций и новых технологий в цифровой журналистике, включая виртуальную и дополненную реальность, искусственный интеллект, автоматизацию процессов и т.д. Со списком дисицплин подробнее можно ознакомиться на сайтеhttps://astanait.edu.kz/digital-journalism/ ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие образовательные программы есть на магистратуре в Astana IT University?", "Наш университет предлагает следующие образовательные программы на магистратуру: 1. Прикладная аналитика данных 2. Вычислительные науки 3. Компьютерные науки и инженерия 4. Проектный менеджмент 5. Цифровые государственное управление и услуги 6. Медиа технологии ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие документы нужно\необходимо собрать для поступления на магистратуру?", "Прикрепить заявление на магистратуру (https://admission.astanait.edu.kz/magistratura#rec334587707) Диплом об окончании бакалавриата (оригинал) Приложение к диплому (оригинал) Сертификат КТ Удостоверение личности (копия) Фото 3х4 (6 шт.) При наличии, сертификат IELTS/TOEFL IBT/TOEFL ITP/APTIS/Interpress (г. Алматы, Астана и Караганда) (копия) Справка по форме 075 (оригинал) Документ, подтверждающий трудовую деятельность (для лиц, имеющих трудовой стаж) список научных и научно-методических работ (в случае их наличия) Чек об оплате за обучение (при поступлении на платное отделение)");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как можно получить скидку на обучение магистратуры?", "Магистрантам, зачисленным на 1 курс на платной основе, предоставляются следующие виды скидок:  Обладателям диплома с отличием предыдущего уровня образования - 30% Обладателям диплома предыдущего уровня образования с GPA от 3,6 и выше -20% Выпускникам Университета, поступившим в магистратуру -30% ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Зачем/почему стоит участвовать в AITU ICode?", "Вот несколько причин участия в олимпиаде AITU ICode: 1) Образовательный грант в Astana IT University 2) Льготы при поступлении в Astana IT University 3) Задания для улучшения своих навыков по программированию 4) Мастер-классы от профессионалов из индустрии IT 5) Обучение программированию во время олимпиады Подробнее с олимпиадой можно ознакомиться здесьhttps://www.icode.astanait.edu.kz/");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как поступить в колледж АИТУ (ASTANA IT COLLEGE)?", "Этапы поступления: 1. Сдать государственные экзамены и получить аттестат 2. Зарегистрироваться на этом сайте 3. Подать заявление на грант/обучение через nursultan.pem.kz 4. Принести пакет документов в университет.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие специальности есть в колледже ASTANA IT COLLEGE/ Какие образовательные программы ОП есть в колледже ASTANA IT COLLEGE?", "ASTANA IT COLLEGE предлагает следующие образовательные программы: 1. Программное обеспечение 06130100 - код образовательной программы Квалификация – 4S06130103 Разработчик программного обеспечения Срок обучения – на базе 9 кл – 2 года 10 месяцев Форма обучения – очная Язык обучения – казахский язык, русский язык  Разработчик программного обеспечения осуществляет разработку прикладных и мобильных приложений на языках программированияС#, Java, Python, Swift и т.д. Владеет основами разработки средств по управлению базами данных Access, MySQL, 1С. На основе языков программирования HTML, PHP, XML, JavaScript создает универсальные сетевые приложения и сайты, изучает компьютерную графику на основе Adobe InDesign, Photoshop и CorelDraw, осваивает сетевые технологии и методы криптографии.  2. Вычислительная техника и информационные сети 06120100 - код образовательной программы Квалификация – 4S06120103 - Техник по администрированию базы данных Срок обучения – на базе 9 кл – 2 года 10 месяцев, на базе 11 кл-1 год 10 месяцев Форма обучения – очная Язык обучения – казахский язык, русский язык  Техник по администрированию баз данных — лицо, отвечающее за выработку требований к базе данных, её проектирование, реализацию, эффективное использование и сопровождение, включая управление учётными записями пользователей базы данных и защиту от несанкционированного доступа. Не менее важной функцией техника по администрированию базы данных является поддержка целостности базы данных. ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что умеет разработик программного обеспечения?", "Специалист производит:  - анализ, проектирование и программирование прикладных задач;  - выбор правильной технологии на всех этапах разработки программ;  - выбор языка программирования;  - внедрение и сопровождение программ и программных средств;  - тестирование программ и программных продуктов.  После окончания колледжа выпускник данной специальности может работать - государственные и частные предприятия и организации, разрабатывающие, внедряющие и использующие программное обеспечение во всех сферах человеческой деятельности. ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое AITU ICode?", "AITU ICode - Республиканская олимпиада по программированию для учащихся 9-12 классов. Соревнования проводятся в 3 этапа: 1 этап (Уровень сложностилегкий. Форматонлайн. Площадкаejudge) - Решение задач на одном из языков программирования (Python, C++, Java, Pascal) с применением условных операторов, циклов, функций. 2 этап (Уровень сложностисредний. Форматонлайн. Площадкаejudge) - Решение задач на одном из языков программирования (Python, C++, Java, Pascal) с применением массивов, строк. Второй этап может включать в себя темы первого этапа. 3 этап (Уровень сложностисложный. Форматофлайн. Площадкаejudge) - Решение задач на одном из языков программирования (Python, C++, Java, Pascal) с применением рекурсии, базовых алгоритмов и структур данных. Третий этап может включать в себя темы второго этапа. Подробнее с олимпиадой можно ознакомиться здесьhttps://www.icode.astanait.edu.kz/");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что умеет Техник по администрированию баз данных?", "Специалист производит:  - Проектирование базы данных.  - Оптимизация производительности базы данных.  - Обеспечение безопасности в базе данных.  - Резервное копирование и восстановление базы данных.  - Обеспечение целостности баз данных.  - Обеспечение перехода на новую версию системами управления базами данных.  После окончания колледжа выпускник данной специальности может работать - в банках, разрабатывая и обслуживая банковские и биржевые компьютерные программы  - на отдельных предприятиях и в управлении целыми отраслями, создавая автоматизированные системы планирования и управления производством  - на предприятиях и в учреждениях, разрабатывая и обслуживая системы массового обслуживания и информационно-справочные системы  - в учреждениях образования  в сервисных центрах по ремонту и диагностики компьютеров и оргтехники ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие скидки можно получить на платной основе (платке) бакалавриата?", "1. Обладателям знака «Алтын белгi» - 30% (действуют на весь период обучения при условии сдачи обучающимися экзаменационных сессий с оценками А, А-, В+, В, В-, С+);  2. Обладателям аттестата особого образца или диплома о техническом и профессиональном образовании с отличием - 30% (действуют на весь период обучения при условии сдачи обучающимися экзаменационных сессий с оценками А, А-, В+, В, В-, С+);  3. Выпускникам Университета, успешно окончившим программу технического и профессионального или послесреднего образования - 20% (действуют на весь период обучения при условии сдачи обучающимися экзаменационных сессий с оценками А, А-, В+, В, В-, С+);  4. Победителям и призерам Республиканской олимпиады по профильным предметам образовательной программы и Республиканского конкурса научных проектов по профилю образовательной программы: 1 место - 30% 2 место - 20% 3 место - 20% (действуют на весь период обучения при условии сдачи обучающимися экзаменационных сессий с оценками А, А-, В+, В, В-, С+);  5. Победителям и призерам областных туров Республиканской олимпиады по профильным предметам образовательной программы и Республиканского конкурса научных проектов по профилю образовательной программы1 место - 30% 2 место - 20% 3 место - 20% (действуют на весь период обучения при условии сдачи обучающимися экзаменационных сессий с оценками А, А-, В+, В, В-, С+);  7. Лицам, имеющими один из международных сертификатов, подтверждающих владение иностранным языком в соответствии с общеевропейскими компетенциями (стандартами) владения иностранным языком - 20% International Englisli Language Tests System (IELTS), пороговый балл - не менее 6.0; Test of English as а Foreign Language Institutional Testiпg Program (TOEFL ITP), не менее 163 баллов; Test of English as а Foreign Language Institutional Testiпg Program Internet-based Test (TOEFL IBT), пороговый балл - не менее 46; Test of English as а Foreign Language Papeг-based testing (TOEFL РВТ), пороговый балл - не менее 453; Test of English as а Foreign Language Papeг-deliveгed testing (TOEFL PDT), пороговый балл - не менее 65. (действуют на весь период обучения при условии сдачи обучающимися экзаменационных сессий с оценками А, А-, В+, В, В-, С+);  8. Лицам, у которых один из родителей или оба родителя являются лицом с ограниченными физическими возможностями, или оба родителя являются пенсионерами - 30% (действуют на весь период обучения);  9. Лицам, у которых один родитель является пенсионером - 20% (действуют на весь период обучения);  10. Лицам с ограниченными физическими возможностями - 30% (действуют на весь период обучения); ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("До какого числа проходит прием заявлений на творческий экзамен?", "Прием заявлений на творческий экзамен ппроходит до 7 июля.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое аккредитация?", "Аккредитация - это процесс оценки и признания учебного заведения, программы обучения или организации, проводимый специализированным органом или агентством. Цель аккредитации состоит в установлении качественных стандартов и обеспечении соответствия образовательных учреждений или программ определенным критериям и нормам.  В образовательной сфере аккредитация выполняет несколько функций:  1. Оценка качестваАккредитационные органы проводят оценку качества образовательных учреждений или программ на основе установленных стандартов. Они анализируют такие аспекты, как уровень преподавания, квалификация преподавателей, содержание программы, учебные ресурсы, степень соответствия учебных целей и результатов обучения.  2. Обеспечение стандартовАккредитация помогает установить и поддерживать высокие стандарты в образовательной сфере. Она способствует улучшению качества образования и обеспечению соответствия учебных программ современным требованиям и потребностям студентов и рынка труда.  3. Признание и довериеАккредитация служит индикатором качества образовательных учреждений или программ. Она помогает студентам, работодателям и другим заинтересованным сторонам принять информированное решение о выборе учебного заведения или программы обучения, а также дает доверие в отношении качества и признания полученного образования.  4. Поддержка улучшенияПроцесс аккредитации предоставляет обратную связь и рекомендации образовательным учреждениям и программам для улучшения их работы. Он способствует развитию и совершенствованию учебных планов, методов преподавания и управления учебными заведениями.  Аккредитация может быть проведена на разных уровнях, включая национальный, региональный или международный уровни. Конкретные процедуры и критерии аккредитации могут различаться в зависимости от страны, образовательной системы и типа образовательного учреждения или программы. ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("До какого числа проходит прием заявлений для участия в конкурсе государственных грантов?", "Прием заявлений для участия в конкурсе государственных грантов проходит с 13 по 20 июля.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть ли у университета лицензии и аккредитация?", "Конечно! Наш университет имеет лицензии, выданные Комитетом по обеспечению качества в сфере образования и науки Министерства образования и науки Республики Казахстан, а также аккредитация на основе Независимого агентства аккредитации и рейтинга IAAR. Подробнее с предоставленной информацией можно ознакомиться здесь: https://astanait.edu.kz/licenses/ ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Когда начинает работать/открывается приемная комиссия Astana IT University?", "1 июня - Открытие приемной комиссии.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие документы нужны для поступления в Astana IT University?", "Документы необходимые для поступления: Аттестат или диплом (оригинал) Приложение к аттестату или диплому (оригинал) Сертификат ЕНТ Удостоверение личности (копия) Фото 3х4 (6 шт.) При наличии, сертификат IELTS/TOEFL IBT/TOEFL ITP/APTIS/Interpress (г. Алматы, Астана и Караганда) (копия) Справка по форме 075 (оригинал) + флюорография (снимки) 063 справка - прививочная карта (копия) Приписное свидетельство - для юношей (копия) Заявление (*Нужно заранее распечатать и заполнить перед подачей документов в университет*) для поступающих на программы бакалавриата Договор в 2-ух экземплярах (Необходим только после фактического получения гранта или же фактического зачисления в университет на платной основе) Свидетельство о рождении ребенка при подписании договора об оказаний образовательных услуг родителями за ребенка, не достигшего 18-ти лет (копия) Удостоверение личности одного из родителя при подаче документов за ребенка, не достигшего 18-ти лет (копия) ");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какая студенческая жизнь в АИТУ?", "В Astana IT University активно функционирует студенческое самоуправление, которое является неотъемлемым элементом студенческой жизни и работает с учетом мнения каждого учащегося. Система студенческого самоуправления - это инициативная, самостоятельная, общественная форма деятельности обучающихся. В Astana IT University работает более 40 студенческих клубов по интересам.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("До какого числа работает приемная комиссия? Когда закрывается приемная комиссия?", "25 августа - Закрытие приемной комиссии.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что нужно сдать чтобы поступить в АИТУ?", "Вам нужно сдать ЕНТ и АЕТ.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Где кабинет 1с?", "Кабинет 1с находится на втором этаже первого блока - С.1.1.239.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Сколько модулей в АЕТ?", "Экзамен состоит из 2 модулей и проводится в виде тестов на компьютерах. Первый модуль - экзамен, подтверждающий владение английским языком. Второй модуль - основы компьютерных наук и логика.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Как я буду сдавать, если у меня есть IELTS?", "Надо сдать лишь второй модуль АЕТ (основы компьютерных наук и логика), если имеется сертификат IELTS выше 5.0");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что будет если не успешно сдать АЕТ даже 2 попытки", "К сожалению, Вам могут отказать в поступлении в наш университет");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Сколько времени дают на АЕТ?", "Дают по часу на каждый модуль.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Сколько дней универ делает справку с места учебы?", "Обычно 2-3 рабочих дня.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Минимальные проходные баллы на грант за 2022 год?", "B057 IT (SE, Comp. Science, BDA, MT, MCS) - 104 балл (квота 69) B058 Cyber Security - 69 балл B059 Smart Technology - 58 балл (квота 50) B044 ITM, ITE, DPAS - 120 балл (квота 107) B042 Digital Journalism - 122 балл (квота 120) B044 содержит в себе менеджмент.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Прошлые минимальные баллы на грант?", "Прошлые минимальные баллы на грант ➡за 2020: B057 Информационные технологии (Computer Science, Software Engineering, Big Data Analysis, Industrial Automation, Media Technologies) — 92 B058 Информационная безопасность (Cyber Security) — 89 B059 Коммуникации и коммуникационные технологии (Tel.Com.Systems) — 85; B044 Менеджмент и управление (IT Management) — 110  B042 Журналистика и репортерское дело (Digital Journalism) — 98; ➡️за 2021B057 Информационные технологии (Computer Science, Software Engineering, Big Data Analysis, Industrial Automation, Media Technologies)  — 106 B058 Информационная безопасность (Cyber Security) — 81  B059 Коммуникации и коммуникационные технологии (Tel.Com.Systems) — 80   B044 Менеджмент и управление (IT Management) — 120  B042 Журналистика и репортерское дело (Digital Journalism) — 123 ; ➡️за 2022B057 Информационные технологии (Computer Science, Software Engineering, Big Data Analysis, Industrial Automation, Media Technologies — 104 B058 Информационная безопасность (Cyber Security)  — 69  B059 Коммуникации и коммуникационные технологии (Smart Technologies) — 58  B044 Менеджмент и управление (IT Management, IT Entrepreneurship) — 120  B042 Журналистика и репортерское дело (Digital Journalism) — 122");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что дает олимпиада AITU IQyn в случае выигрыша?/ что если победить олимпиаду Айкын?", "Пропускаете первый модуль АЕТ.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое АЕТ?", "АЕТ (AITU Excellence Test) - это вступительный экзамен Astana IT University для поступления в университет. Экзамен состоит из 2 модулей и проводится в виде тестов на компьютерах. Первый модуль - экзамен, подтверждающий владение английским языком. Второй модуль - основы компьютерных наук и логика. Подробнее на https://admission.astanait.edu.kz/aet");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("2 этап IQyn оффлайн будет?", "В 2023 году второй этап олимпиады IQyn был онлайн с веб-камерой.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("На IQyn знания по английскому будут проверяться?", "На первом туре проверяются лексика, грамматика и навыки чтения (текст объемом до 400 слов) в формате онлайн тестирования.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Если перевестись в другой вуз то учить те курсы которые проходил здесь, не придется проходить там снова?", "Это зависит от вуза, так как у вузов разные кредиты на определенные предметы. Вам стоит обратиться в приемную комиссию АИТУ для подробной информации (по номеру 8(7172)-64-57-10)");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Сколько месяцев длится курс от айту если ты набрал меньше 35 на первом модуле АЕТ?", "Это зависит от вашего уровня английского.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Можно ли пропустить АЕТ?", "Лица, имеющие следующие действующие международные сертификаты, подтверждающие владение английским языком в соответствии с общеевропейскими стандартами, освобождаются от сдачи 1-го модуля по английскому языку. • International English Language Testing System (IELTS), пороговый балл — не менее 5.0; • Test of English as a Foreign Language Institutional Testing Programm (TOEFL ITP) — не менее 460 баллов; • Test of English as a Foreign Language Internet-based Test (TOEFL IBT), пороговый балл – 46-59; • Сертификат Aptis, с результатом, соответствующим уровню B1; • Сертификаты уровня B1, выданные языковыми центрами InterPress городов Нур-Султан, Алматы и Караганда в 2021 году. • Сертификат олимпиады AITU iQyn Напоминаем, что это не освобождает вас от второго модуля АЕТ. Лица, имеющие сертификаты призёра AITU iCode, AITU Open, Жаутыковской или Евразийской олимпиалы, освобождаются от сдачи модуля по основам компьютерных наук и логики. Напоминаем, что это не освобождает вас от первого модуля АЕТ.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие аккредитации есть у АИТУ?", "У АИТУ всего 4 аккредитации. Вот они. Институциональная аккредитация – процесс оценивания качества организации образования аккредитационным органом на соответствие заявленному статусу и установленным стандартам аккредитационного органа. Специализированная аккредитация — оценка качества отдельных образовательных программ, реализуемых организацией образования. Национальная аккредитация — аккредитация организации образования или образовательной программы аккредитационным агентством в рамках страны. Международная аккредитация — аккредитация образовательной программы зарубежным аккредитационным агентством.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Cколько стоит проживание в общежитии?", "Проживание в общежитии стоит 30000 тенге в месяц.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В АИТУ 6 дневная учеба или 5 дневная?", "Как повезет с расписанием, у первашей может быть и 6 дней. У вторашей и третьяшей учеба обычно 5 дней, 1 день на военку.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Можно ли самим составлять раписание?", "Нет, нельзя (Не доросли вы еще до таких привилегий в жизни).");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Нужно ли носить ноуты постоянно?", "Почти в каждом кабинете АИТУ есть стационарные компьютеры, однако стоит носить свой ноутбук, т.к в зависимости от предмета вам предстоит работа со своими файлами, документами, учебными материалами, личными проектами, а также дополнительными програмными расширениями, виртуалбоксами и т.д.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("За что студент попадает на доску позора?", "Доску убрали сразу. Администрация университета приносит извинения перед всеми учащимися нашего университета за причиненные неудобства и уверяет, что подобной ошибки больше не повторится.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("В аудиториях розеток хватает?", "Да, их довольно много.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("До какого месяца надо сдать АЕТ чтобы подать на конкурс грантов?", "До начала приема заявлений на грант.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Software engineer это что?", "Образовательная программа «Software Engineering» (Программная инженерия) включает в себя дисциплины по основам разработки программных средств и систем, которые позволяют студентам позиционировать себя как профессионалов в области разработки и сопровождения программного обеспечения различных масштабов. Ты можешь быть разработчиком программного обеспечения. Это могут быть и компьютерные игры, и программы для поддержки и модернизации интернет-сайтов, и программы, позволяющие компаниям автоматизировать свои бизнес-процессы.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Расскажите про олимпиаду внутреннего гранта?", "Есть две олимпиады по которым можно выиграть ректорский грант: 1) AITU iCode 2) Республиканский конкурс научных IT проектов.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Общага дается на год или на один триместр?", "В случае прохождения курса на общежитие, место дается на весь первый год обучения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что если я второкурсник и мне нужно общежитие?", "К сожалению, количество мест в общежитии ограничено и распределяется преимущественно студентам первого курса. Однако, среди курсов выше также проводятся собрания и социальные опросы. Вы сможете подать заявку, как нуждающийся в жилье, и со временем, если место в общежитии освободится, на вашу заявку обязательно придет ответ. Я думаю, вы понимаете, что подача заявки не гарантирует вам место в общежитии, поэтому стоит задуматься о нахождении жилья для себя заранее. В этом телеграм-канале студенты АИТУ ищут себе сожителей: https://t.me/aitu_reklama");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("У вас только предметы с программированием?", "Нет. У нас есть предметы связанные с математикой, такие как: дискретная математика, линейная алгебра, Calculus 1, Calculus 2. Также есть предметы какПсихология, социология, культурология, политология, история. Есть языковыеEnglish, Қазақ тілі. Если есть IELTS 7.0, то вас освободят от кредитов по курсам английского и вы сможете выбрать курсы немецкого или китайского языка. В зависимости от специальности и ее учебного плана, количество предметов, связанных с программированием или другими направлениями, может меняться. Например на Media Technologies вы пройдете такие курсы, какКинематография и цифровой монтаж, Продюсирование, 3D анимация и т.д. Для подробного изучения данного вопроса вы можете перейти по ссылке https://astanait.edu.kz/programs/ и выбрать интересующую вас программу обучения.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Документы на общагу подаются вместе с заявкой на поступление или позже?", "Конкурс на общежитие проходит среди уже поступивших студентов, поэтому документы на общежитие подаются позже.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Если не участвовала в первом этапе AITU iCode, уже никак нельзя принять участие в соревновании?", "К сожалению, нельзя.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Летний триместр (летник) сколько длиться?", "По-разному, зависит от преподавателя и количество кредитов в предмете.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое летник?", "В случае получения оценки «неудовлетворительно» у вас появляется задолженность в виде летника - повторного курса предмета на летнем триместре.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Учеба когда заканчивается?", "Учеба заканчивается в июне.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Если учась на ректорском гранте, и если будешь активистом, учиться отлично и тд, то возможно будет получать стипендию?", "Стипендия выдаётся только на государственном гранте.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Если закрыть сессию по предмету на 60%, но при этом все задания за триместр были сделаны на 100, как расчет идет?", "Register midterm * 0.3 + register endterm * 0.3 + final * 0.4");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Aitu icode, aitu iqyn, aet что для чего и зачем?", "Aitu Icode - можете выиграть грант. Aitu iqyn - можете выиграть освобождение от первого модуля АЕТ. АЕТ - вступительный экзамен в универ, обязателен для поступления.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Когда выйдут результаты АЕТ?", "Результаты выходят сразу же. Список прошедших потоков выходит спустя день-два на сайте https://admission.astanait.edu.kz/aet");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Где делать документ что ты реально студент?", "Вы можете сделать документ, подтвреждающий что вы студент на сайте du.astanait.edu.kz. А также открыть студенческий билет на Kaspi в разделе Гос.услуги");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Есть пробники по АЕТ?", "К сожалению, пробников по АЕТ нет, но можно подготовиться по материалам на сайте университета.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие предметы нужно сдать чтоб поступить на Software Engineering Программная инженерия?", "Для того чтобы поступить на специальность Software Engineering нужно сдать ЕНТ по профильным предметам информатика и математика.");
