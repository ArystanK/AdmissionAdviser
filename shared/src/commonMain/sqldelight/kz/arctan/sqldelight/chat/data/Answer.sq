INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие были основные достижения СССР в науке и технике?", "СССР достиг значительных успехов в области космических исследований, атомной энергии, авиации, радиотехники и других областях науки и техники.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие были основные реформы в СССР в 1980-х годах?", "В 1980-х годах в СССР были проведены реформы, направленные на улучшение экономической и политической ситуации в стране, включая перестройку и гласность.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие были основные проблемы СССР?", "СССР сталкивался с проблемами, такими как экономический кризис, коррупция, нарушение прав человека и политическая репрессия.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие были последствия распада СССР?", "Распад СССР привел к экономическому кризису, политической нестабильности и социальным проблемам во многих странах, входивших в состав СССР.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие были основные принципы советской экономики?", "Основными принципами советской экономики были государственная собственность на средства производства, централизованное планирование и коллективизация.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие были основные праздники в СССР?", "Основными праздниками в СССР были День Победы, День Революции, Новый год и Международный женский день.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Что такое океаны?", "Океаны - это самые большие водные массы на Земле, которые покрывают более чем 70% поверхности Земли.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие были основные символы СССР?", "Основными символами СССР были герб, флаг и гимн.");
