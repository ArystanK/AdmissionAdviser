INSERT OR IGNORE INTO Answer(question, answer) VALUES("Какие организации и ресурсы доступны в Казахстане для получения информации о половом воспитании?", "В Казахстане доступны различные организации, такие как ЮНФПА, ЮНИСЕФ и местные НПО, которые предоставляют информацию, консультации и ресурсы по половому воспитанию.");
