INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кто написал картину «Звездная ночь»?", "«Звездную ночь» написал Винсент Ван Гог.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кто является нынешним премьер-министром Индии?", "На момент моего последнего обновления знаний в сентябре 2021 года премьер-министром Индии является Нарендра Моди.");
INSERT OR IGNORE INTO Answer(question, answer) VALUES("Кто является автором «Хоббита»?", "«Хоббит» был написан Дж. Р. Р. Толкином.");
